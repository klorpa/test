;=================================================================
;指定キャラのステータスを主に引き継ぎ用に初期化する
;;引き継いだキャラのレベルを人間なら5、悪魔なら1にして能力をＣＳＶから読み込んでからレベルに合わせて増減、習得スキルをＣＳＶから再読み込み。引継ぎフラグに応じて素質や能力変更
;ARG:0 キャラの登録番号
;RESULT 真:成功 偽:失敗
;=================================================================
@SHOP_NEWGAME_INITCHARA, ARG
	LOCAL = ARG

	;チェフェイ転生処理
	SIF NO:LOCAL >= 3701 && NO:LOCAL <= 3707
		CALL TYEFEI_TENSEI,ARG,810

	IF ABL:LOCAL:種族 == 0
		BASE:LOCAL:LV = 5
	ELSE
		BASE:LOCAL:LV = 1
	ENDIF

	;元が非戦闘員またはランダムキャラ(No.4901〜4920)ならベースステータスを保管
	IF CSVTALENT(NO:LOCAL,230,0) == 1 || (4900 < NO:LOCAL  && 4921 > NO:LOCAL)
		FOR LOCAL:1,1,7
			BASE:LOCAL:GET_BASESTATUS(LOCAL:1) = 3
			CFLAG:LOCAL:(GET_BASESTATUS(LOCAL:1) + "強化回数") = 0
		NEXT
	;元が非戦闘員またはランダムキャラ以外ならベースステータスをCSVから読み込み
	ELSE
		FOR LOCAL:1,1,7
			BASE:LOCAL:GET_BASESTATUS(LOCAL:1) = CSVBASE(NO:LOCAL,GETNUM(BASE,GET_BASESTATUS(LOCAL:1)),0)
			CFLAG:LOCAL:(GET_BASESTATUS(LOCAL:1) + "強化回数") = 0
		NEXT
	ENDIF



	;能力値の合計を計算
	LOCAL:2 = (18 + BASE:LOCAL:LV * 3 / 2)

	FOR LOCAL:1,1,7
		;レベルに応じて能力値増減
		BASE:LOCAL:(GET_BASESTATUS(LOCAL:1)) *= LOCAL:2
		BASE:LOCAL:(GET_BASESTATUS(LOCAL:1)) /= (18 + CSVBASE(NO:LOCAL,GETNUM(BASE,"LV"),0) * 3 / 2)
	NEXT

	;基本能力値合計がレベル分より低い場合差分を補填
	;IF BASE:LOCAL:力 + BASE:LOCAL:知恵 + BASE:LOCAL:魔力 + BASE:LOCAL:耐力 + BASE:LOCAL:速さ + BASE:LOCAL:運 < 18 + BASE:LOCAL:LV * 3 / 2
	;	LOCAL:1 = 18 + BASE:LOCAL:LV * 3 / 2 - (BASE:LOCAL:力 + BASE:LOCAL:知恵 + BASE:LOCAL:魔力 + BASE:LOCAL:耐力 + BASE:LOCAL:速さ + BASE:LOCAL:運)
	;	FOR LOCAL:2, 1, LOCAL:1 + 1
	;		BASE:LOCAL:(GET_BASESTATUS(LOCAL:2 % 6 + 1)) += 1
	;	NEXT
	;ENDIF
	;↑元々基本能力値合計がレベル分より低い（人修羅等）のも個性だと思うしそれを反映させたいので没

	;基本能力値合計がレベル分より低い場合2PTまで補填（計算が端数切捨てで低くなりがちなので
	IF BASE:LOCAL:力 + BASE:LOCAL:知恵 + BASE:LOCAL:魔力 + BASE:LOCAL:耐力 + BASE:LOCAL:速さ + BASE:LOCAL:運 < 18 + BASE:LOCAL:LV * 3 / 2
		BASE:LOCAL:運 += 1
		SIF (18 + BASE:LOCAL:LV * 3 / 2) - (BASE:LOCAL:力 + BASE:LOCAL:知恵 + BASE:LOCAL:魔力 + BASE:LOCAL:耐力 + BASE:LOCAL:速さ + BASE:LOCAL:運) > 1
			BASE:LOCAL:速さ += 1
	ENDIF

