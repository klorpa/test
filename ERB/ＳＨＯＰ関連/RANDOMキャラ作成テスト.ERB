@SHOPCOMABLE_699
[IF_DEBUG]
	RESULTS = Random Character Test
	RETURN 1
[ENDIF]
IF FLAG:DEBUG
	RESULTS = Random Character Test
	RETURN 1
ENDIF
RETURN -1

@SHOP_COM_699
#DIM CHARA, 1
LOCAL:1 = 0
FOR LOCAL, 4901, 4912
	IF LOCAL == 4911
		PRINT [-1] Random
	ELSE
		SIF !EXISTCSV(LOCAL, 0)
			CONTINUE
		PRINTFORM %@"[{LOCAL-4901,2}] %CSVCALLNAME(LOCAL, 0)%", 24, LEFT%
	ENDIF
	SIF LOCAL:1 % 3 == 2
		PRINTL 
	LOCAL:1++
NEXT
INPUT
SIF !RANGE(RESULT, -1, 99)
	RESTART
LOCAL:4 = RESULT == -1 ? -1 # 4901+RESULT
PRINTL 作成人数を入力してください
CALL INPUT_MANY(1, 100)
LOCAL:6 = RESULT
FOR LOCAL:5, 0, LOCAL:6
	CALL MAKE_RANDOM_CHARA, LOCAL:4, RAND(1, BASE:MASTER:LV)
	LOCAL:7 = RESULT
	CHARA = RESULT:1
	LOCAL:9 = RESULT:2
	IF RESULT
		CALL MESSAGE_B(1, @"%CALLNAME:CHARA%を作成しました")
		;確認する際は1、しないなら0
		IF 0
			PRINTL スキル
			FOR LOCAL, 1, 20
				LOCALS = \@CFLAG:CHARA:初期リンク悪魔 ? 初期変身悪魔# \@スキル{LOCAL}
				IF ABL:CHARA:LOCALS > 0
					CALLFORM SKILL_NAME_{ABL:CHARA:LOCALS}
					PRINTFORML [{LOCAL}] %RESULTS%
				ENDIF
			NEXT
			PRINTL LEARNI
			FOR LOCAL, 1, 20
				LOCALS = \@CFLAG:CHARA:初期リンク悪魔 ? 初期変身悪魔# \@習得スキル{LOCAL}
				LOCALS:1 = \@CFLAG:CHARA:初期リンク悪魔 ? 初期変身悪魔# \@習得LV{LOCAL}
				IF ABL:CHARA:LOCALS > 0
					CALLFORM SKILL_NAME_{ABL:CHARA:LOCALS}
					PRINTFORML [{LOCAL}] %RESULTS%(Lv{ABL:CHARA:(LOCALS:1)})
				ENDIF
			NEXT
		ENDIF
		;確認する際は1
		SIF 1
			CALL CHECK_RANDOM_CHARA,CHARA,LOCAL:9
	ENDIF
NEXT