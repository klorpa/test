@KUZI,ARG
#DIM ITEMID = 1010,1011,1012,1013,1014,1015,1008,1009,1007,1004,1031,1032,1033,1034,1035,1036,1040,1110,1111,1115,1116,1117,1118,5511,5513,5514,5503,5504,40,41,1402,3204,3608,5203,1412
#DIM ITEMNUM,35 

VARSET ITEMNUM,0
LOCAL = 0
FOR COUNT,0,ARG
	LOCAL = RAND:95
	SELECTCASE LOCAL
		CASE IS < 10
			;10/95 で装備箱
			ITEMNUM:30 += 1
		CASE IS < 60
			;50/95 で何かアイテム
			LOCAL = RAND:30
			SELECTCASE LOCAL
				CASE 29
					;ITEM:41 Aphrodisiac x10
					ITEMNUM:LOCAL += 10
				CASE IS >= 26
					;ITEM:5503 Holy Shell ITEM:5504 Curse Shell ITEM:40 Lube  x50
					ITEMNUM:LOCAL += 50
				CASE IS >= 23
					;ITEM:5511 Corona Shell ITEM:5513 Frost Shell ITEM:5514 Lantern Shell  x25
					ITEMNUM:LOCAL += 25
				CASEELSE
					;そのほか  x1
					ITEMNUM:LOCAL += 1
			ENDSELECT
		CASE IS < 65
			;5/95でレア装備
			LOCAL = RAND:3
			SELECTCASE LOCAL
				CASE 0
					;Portable Headphones
					ITEMNUM:31 += 1
				CASE 1
					;Frost Suit
					ITEMNUM:32 += 1
				CASEELSE
					;Snowflake
					ITEMNUM:33 += 1
			ENDSELECT
		;5/95で 1 MORE!
		CASE IS < 70
			PRINTW 1 MORE！
			COUNT -= 2
		CASEELSE
			;25/95ではずれくじ
			ITEMNUM:34 += 1
	ENDSELECT
NEXT

FOR COUNT,0,35
	IF ITEMNUM:COUNT > 0
		PRINTFORML {ITEMNUM:COUNT} ｘ %ITEMNAME_E(ITEMID:COUNT)% was the price!
		;が当たりました！
		CALL CHECK_ITEMMAX,ITEMID:COUNT
		SIF RESULT == 0
			CALL GET_ITEM,ITEMID:COUNT,ITEMNUM:COUNT
	ELSE
		CONTINUE
	ENDIF
NEXT
PRINTW
RETURN 0



;==========================================================
;@ITEM_GOUSEI_表示
; アイテム合成の表示用
;<引数>
;	ARGS:0 = 合成後のアイテム名
;	ARGS:1 = 一つ目の素材アイテム名
;	ARGS:2 = 二つ目の素材アイテム名
;	ARGS:3 = 三つ目の素材アイテム名
;	ARG:1  = 一つ目の素材アイテムの必要個数
;	ARG:2  = 二つ目の素材アイテムの必要個数
;	ARG:3  = 三つ目の素材アイテムの必要個数
;
;<戻り値>
;	なし
;==========================================================
@ITEM_GOUSEI_表示(ARGS:0, ARGS:1, ARG:1, ARGS:2 = "", ARG:2 = 0, ARGS:3 = "", ARG:3 = 0)
#DIM アイテム所持数, 4

;必要な素材が￥・魔貨だった場合は置き換える
FOR LOCAL, 1, 4
	SIF ARGS:LOCAL == ""
		BREAK
	IF ARGS:LOCAL == "Macca"
		アイテム所持数:LOCAL = MONEY:1
	ELSEIF ARGS:LOCAL == "￥"
		アイテム所持数:LOCAL = MONEY
	ELSE
		アイテム所持数:LOCAL = ITEM:(ARGS:LOCAL)
		;This is a work-around, since the skillcard-names are not hard-translated
		IF GETNUM(ITEM,ARGS:LOCAL) > 10000
			CALLFORM SKILL_NAME_{(GETNUM(ITEM,ARGS:LOCAL) - 10000)}
			ARGS:LOCAL = Skillcard【%RESULTS%】
		ENDIF
			
	ENDIF
NEXT

;合成後のアイテム名表示
SETCOLOR COLOR("灰色")
IF (ARG:2 == 0 && アイテム所持数:1 >= ARG:1) || (ARG:2 && ARG:3 == 0 && アイテム所持数:1 >= ARG:1 && アイテム所持数:2 >= ARG:2) || (ARG:3 && アイテム所持数:1 >= ARG:1 && アイテム所持数:2 >= ARG:2 && アイテム所持数:3 >= ARG:3)
	RESETCOLOR
ENDIF
PRINTFORM %ARGS:0,26,LEFT%
RESETCOLOR

;一つ目の素材アイテム名表示
IF アイテム所持数:1 >= ARG:1
	SETCOLOR COLOR("水色")
ELSEIF アイテム所持数:1 < ARG:1
	SETCOLOR COLOR("赤")
ENDIF
PRINTFORM (%ARGS:1,30,LEFT%({アイテム所持数:1}/{ARG:1})
;Materials:
IF ARGS:2 == ""
	PRINTL )
;	PRINTL 
ENDIF
RESETCOLOR

;二つ目の素材がない場合は戻る
SIF ARGS:2 == ""
	RETURN

;二つ目の素材アイテム名表示
IF アイテム所持数:2 >= ARG:2
	SETCOLOR COLOR("水色")
ELSEIF アイテム所持数:2 < ARG:2
	SETCOLOR COLOR("赤")
ENDIF
PRINTFORM 　%ARGS:2,20,LEFT%({アイテム所持数:2}/{ARG:2})
IF ARGS:3 == ""
	PRINTL )
;	PRINTL 
ENDIF
RESETCOLOR

;三つ目の素材がない場合は戻る
SIF ARGS:3 == ""
	RETURN

;三つ目の素材アイテム名表示
IF アイテム所持数:3 >= ARG:3
	SETCOLOR COLOR("水色")
ELSEIF アイテム所持数:3 < ARG:3
	SETCOLOR COLOR("赤")
ENDIF
PRINTFORML 　%ARGS:3,20,LEFT%({アイテム所持数:3}/{ARG:3}))
RESETCOLOR
