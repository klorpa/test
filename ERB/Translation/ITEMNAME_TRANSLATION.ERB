;----------------------------------------------------------
; Function that returns the english version of an item, based on the item number
;----------------------------------------------------------

@ITEMNAME_E(ARG)
#FUNCTIONS
SELECTCASE ARG
CASE 5 ;アナルバイブ
	RETURNF "Anal vibrator"
CASE 6 ;アナルビーズ
	RETURNF "Anal beads"
CASE 7 ;空気ポンプ
	RETURNF "Air pump"
CASE 8 ;拡張バルーン
	RETURNF "Balloon dilator"

CASE 10 ;二プルキャップ
	RETURNF "Nipple caps"

CASE 18 ;ビデオカメラ
	RETURNF "Video camera"
	
CASE 20 ;プレイマット
	RETURNF "Play Mat"
CASE 21 ;大鏡
	RETURNF "Large mirror"

CASE 24 ;電極プラグ
	RETURNF "Electrode plug"

CASE 29 ;ローター
	RETURNF "Rotor"
CASE 43 ;ビデオテープ
	RETURNF "Video tape"
CASE 46 ;低温蝋燭
	RETURNF "Cold candle"
CASE 47 ;強精神薬
	RETURNF "Potent mind medicine"

CASE 200 ;ドリー・カドモン
	RETURNF "Dolly Kadmon"
CASEELSE
	;Since we use ITEMNAME here, _TR should still work for stuff not directly translated above
	RETURNF ITEMNAME:ARG
ENDSELECT
