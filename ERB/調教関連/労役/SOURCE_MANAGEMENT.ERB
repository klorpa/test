SIF CUP:ARG:2 + PALAM:ARG:2 - CDOWN:ARG:2 >= PALAMLV:4
	CDOWN:ARG:2 = CUP:ARG:2 + PALAM:ARG:2 - PALAMLV:4 + 1

;絶頂Ｂ
IF CUP:ARG:3 + PALAM:ARG:3 >= PALAMLV:4 * 2
	LOCAL:3 = 2
	CDOWN:ARG:3 = (PALAMLV:4 * 2 - 1000)
ELSEIF CUP:ARG:3 + PALAM:ARG:3 >= PALAMLV:4
