	SOURCE:ARG:12 += 700 * LOCAL:2
	SOURCE:ARG:13 += 900 * LOCAL:2
	SOURCE:ARG:14 += 500 * LOCAL:2
	SOURCE:ARG:18 += 400 * LOCAL:2
