SIF CUP:ARG:3 + PALAM:ARG:3 - CDOWN:ARG:3 >= PALAMLV:4
	CDOWN:ARG:3 = CUP:ARG:3 + PALAM:ARG:3 - PALAMLV:4 + 1

IF LOCAL
	SOURCE:ARG:13 += 500 * LOCAL
	SOURCE:ARG:14 += 200 * LOCAL
	SOURCE:ARG:18 += 150 * LOCAL
ENDIF
