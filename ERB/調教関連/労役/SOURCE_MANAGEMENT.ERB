	SOURCE:ARG:12 += 500 * LOCAL:1
	SOURCE:ARG:13 += 700 * LOCAL:1
	SOURCE:ARG:14 += 400 * LOCAL:1
	SOURCE:ARG:18 += 300 * LOCAL:1
ENDIF
