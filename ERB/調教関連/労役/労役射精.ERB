
@労役中出し(ARG,ARG:1,ARG:2,ARGS)
IF ARG == MASTER
	LOCALS = 主人の中出氏による妊娠判定
ELSE
	;暫定父親の置換処理(かなり適当)
	SIF CFLAG:(ARG:1):労役中の中出氏による暫定父親 == -1 || !RAND:3
		CFLAG:(ARG:1):労役中の中出氏による暫定父親 = ARG
	LOCALS = 顧客の中出氏による妊娠判定
ENDIF
SELECTCASE ARGS
CASE "="
	CFLAG:(ARG:1):LOCALS = ARG:2
CASE "+"
	CFLAG:(ARG:1):LOCALS += ARG:2
CASE "-"
	CFLAG:(ARG:1):LOCALS -= ARG:2
CASE "*"
	CFLAG:(ARG:1):LOCALS *= ARG:2
CASE "/"
	CFLAG:(ARG:1):LOCALS /= ARG:2
ENDSELECT