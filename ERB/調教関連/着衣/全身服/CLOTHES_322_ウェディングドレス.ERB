
@CLOTHES_NAME_322
RESULTS = Wedding Dress

;露出部位
@CLOTHES_EXPOSE_322,ARG
;ARG = 露出する部位
;1 = 腕露出 2 = 足露出 3 = 脚露出 4 = 乳房露出 5 = 乳首露出 6 = ヘソ露出 7 = 陰唇露出 8 = 臀部露出
SELECTCASE ARG
	CASE 1,3
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;装備部位
@CLOTHES_PART_322
RETURN 3

;服の前が開くか
;1 開けられる、2 開きっぱなし
@CLOTHES_OPENFRONT_322
RETURN 1
;胸構造
; 0 開けられない 1 ボタン式 2 チャック式 4 上からこんにちは 8 たくしあげてこんにちは 16 和服など、合わせる衣装 32 前から差し込めそうな穴 64 口元-胸元間にペニス入るスペース（ビット演算）
@CLOTHES_BREAST_322
RETURN 4

;股間の構造
;1 = ズボン、2 = ビキニタイプ、3 = スカート、4 = 穴開き、5 前開き、6 開閉可能
@CLOTHES_CROTCH_322
RETURN 3
;スカートのタイプ
;1 = 具チラ 2 = ミニ 3 = 通常 4 = ロング 5 = タイト
@CLOTHES_SKIRT_322
RETURN 4

;装備時の調教効果
@CLOTHES_SOURCE_322,ARG = -1
RETURN 0

;装備時の経験追加
@CLOTHES_EXP_322,ARG = -1
		RETURN 0

@CLOTHES_PRICE_322
RETURN 200

;触覚
;1 = スルー 2 = 上から伝わる 4 = 潜り込ませられる
;ARG 0 = Ｃ 1 = Ｖ 2 = Ａ 3 = 乳首 4 = 乳房
@CLOTHES_触覚_322,ARG
SELECTCASE ARG
	CASE 3,4
		RETURN 2
ENDSELECT
