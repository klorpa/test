
@CLOTHES_NAME_319
RESULTS = Kantoui

;露出部位
@CLOTHES_EXPOSE_319,ARG
;ARG = 露出する部位
;1 = 腕露出 2 = 足露出 3 = 脚露出 4 = 乳房露出 5 = 乳首露出 6 = ヘソ露出 7 = 陰唇露出 8 = 臀部露出
SELECTCASE ARG
	CASE 3,4,5,6
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;装備部位
@CLOTHES_PART_319
RETURN 3

;服の前が開くか
;1 開けられる、2 開きっぱなし
@CLOTHES_OPENFRONT_319
RETURN 0

;股間の構造
;1 = ズボン、2 = ビキニタイプ、3 = スカート、4 = 穴開き、5 前開き、6 開閉可能
@CLOTHES_CROTCH_319
RETURN 3

;スカートのタイプ
;1 = 具チラ 2 = ミニ 3 = 通常 4 = ロング 5 = タイト
@CLOTHES_SKIRT_319
RETURN 1

