
@CLOTHES_NAME_331
RESULTS = Clown Outfit

;露出部位
@CLOTHES_EXPOSE_331,ARG
;ARG = 露出する部位
;1 = 腕露出 2 = 足露出 3 = 脚露出 4 = 乳房露出 5 = 乳首露出 6 = ヘソ露出 7 = 陰唇露出 8 = 臀部露出
SELECTCASE ARG
	CASE 1,2,3,4,5,6,7,8
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;装備部位
@CLOTHES_PART_331
RETURN 3

;服の前が開くか
;1 開けられる、2 開きっぱなし
@CLOTHES_OPENFRONT_331
RETURN 1
;胸構造
; 0 開けられない 1 ボタン式 2 チャック式 4 上からこんにちは 8 たくしあげてこんにちは 16 和服など、合わせる衣装 32 前から差し込めそうな穴 64 口元-胸元間にペニス入るスペース（ビット演算）
@CLOTHES_BREAST_331
RETURN 1

;股間の構造
;1 = ズボン、2 = ビキニタイプ、3 = スカート、4 = 穴開き、5 前開き、6 開閉可能
@CLOTHES_CROTCH_331
RETURN 1

;装備時の調教効果
@CLOTHES_SOURCE_331,ARG = -1
RETURN 0

;装備時の経験追加
@CLOTHES_EXP_331,ARG = -1
		RETURN 0

@CLOTHES_PRICE_331
RETURN 400
