
@CLOTHES_NAME_702
RESULTS = Tights w/ Holes

;露出部位
@CLOTHES_EXPOSE_702,ARG
;ARG = 露出する部位
;1 = 腕露出 2 = 足露出 3 = 脚露出 4 = 乳房露出 5 = 乳首露出 6 = ヘソ露出 7 = 陰唇露出 8 = 臀部露出


SELECTCASE ARG
	CASE 1,2,3,4,6
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;装備部位
@CLOTHES_PART_702
RETURN 7

;服の前が開くか
;1 開けられる、2 開きっぱなし
@CLOTHES_OPENFRONT_702
RETURN 0

;股間の構造
;1 = ズボン、2 = ビキニタイプ、3 = スカート、4 = 穴開き、5 前開き、6 開閉可能
@CLOTHES_CROTCH_702
RETURN 4

;装備時の調教効果
@CLOTHES_SOURCE_702,ARG = -1
RETURN 0

;装備時の経験追加
@CLOTHES_EXP_702,ARG = -1
RETURN 0

@CLOTHES_PRICE_702
RETURN 2000

;触覚
;1 = スルー 2 = 上から伝わる 4 = 潜り込ませられる
;ARG 0 = Ｃ 1 = Ｖ 2 = Ａ 3 = 乳首 4 = 乳房
@CLOTHES_触覚_702,ARG
SELECTCASE ARG
	CASE 4
		RETURN 2
ENDSELECT
