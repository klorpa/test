;前腕までを「手」の部位とするなら
@CLOTHES_NAME_411
RESULTS = Bracelets

;露出部位
@CLOTHES_EXPOSE_411,ARG
;ARG = 露出する部位
;1 = 腕露出 2 = 足露出 3 = 脚露出 4 = 乳房露出 5 = 乳首露出 6 = ヘソ露出 7 = 陰唇露出 8 = 臀部露出


SELECTCASE ARG
	CASE 0
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;装備部位
