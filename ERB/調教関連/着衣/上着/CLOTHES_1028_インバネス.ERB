
@CLOTHES_NAME_1028
RESULTS = inverness Cape

;露出部位
@CLOTHES_EXPOSE_1028,ARG
;ARG = 露出する部位
;1 = 腕露出 2 = 足露出 3 = 脚露出 4 = 乳房露出 5 = 乳首露出 6 = ヘソ露出 7 = 陰唇露出 8 = 臀部露出

SELECTCASE ARG
	CASE 1,3,4,5,6,7,8
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;装備部位
@CLOTHES_PART_1028
RETURN 10

;服の前が開くか
;1 開けられる、2 開きっぱなし
@CLOTHES_OPENFRONT_1028
RETURN 1

;股間の構造
;1 = ズボン、2 = ビキニタイプ、3 = スカート、4 = 穴開き、5 前開き、6 開閉可能
@CLOTHES_CROTCH_1028
RETURN 5

;装備時の調教効果
@CLOTHES_SOURCE_1028,ARG = -1
RETURN 0

;装備時の経験追加
@CLOTHES_EXP_1028,ARG = -1
		RETURN 0

;装備するための条件
@CLOTHES_PREREQUISITE_1028,ARG
SIF TEQUIP:ARG:服 == 1
	RETURN 0
RETURN 1



@CLOTHES_PRICE_1028
RETURN 500
