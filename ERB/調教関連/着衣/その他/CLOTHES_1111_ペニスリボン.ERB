
@CLOTHES_NAME_1111
RESULTS = Penis Ribbon

;露出部位
@CLOTHES_EXPOSE_1111,ARG
;ARG = 露出する部位
;1 = 腕露出 2 = 足露出 3 = 脚露出 4 = 乳房露出 5 = 乳首露出 6 = ヘソ露出 7 = 陰唇露出 8 = 臀部露出


SELECTCASE ARG
	CASE 0
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;装備部位
@CLOTHES_PART_1111
RETURN 6

;服の前が開くか
;1 開けられる、2 開きっぱなし
@CLOTHES_OPENFRONT_1111
RETURN 0

;装備時の調教効果
@CLOTHES_SOURCE_1111,ARG = -1
RETURN 0

;装備時の経験追加
@CLOTHES_EXP_1111,ARG = -1
		RETURN 0

;装備するための条件
@CLOTHES_PREREQUISITE_1111,ARG
SIF HAVE_PENIS(ARG)
	RETURN 1
RETURN 0

@CLOTHES_PRICE_1111
RETURN 200
