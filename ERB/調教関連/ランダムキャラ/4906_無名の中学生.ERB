;ARGは生成キャラの登録番号
@RANDOMCHARA_4906, ARGS, ARG, ARG:1
#LOCALSSIZE 1
SELECTCASE ARGS
	CASE "一人称"
		IF TALENT:ARG:オトコ == 1
			RESULTS = ぼく
		ELSE
			RESULTS = あたし
		ENDIF
	CASE "二人称"
		RESULTS = %CALLNAME:MASTER%
	CASE "体力"
		RETURN RAND(800, 1500)
	CASE "気力"
		RETURN RAND(800, 1500)
	CASE "属性"
		RETURN RAND(100, 200), RAND(100, 200)
	CASE "種族"
		RETURN 0
	CASE "戦闘素質"
		IF RAND:2 > 0
			RESULTS = 非戦闘員
			RETURN 1
		ELSEIF RAND:3 == 0
			RESULTS = サマナー
			RESULTS:1 = 異能者
			RETURN 1, 1
		ELSEIF RAND:3 == 0
			RESULTS = サマナー
			RESULTS:1 = ガンスリンガー
			RESULTS:2 = 道具知識
			RETURN 3, 1, 1
		ELSE
			RESULTS = 異能者
			RESULTS:1 = サクセサー
			RETURN 1, 1
		ENDIF
	CASE "リンク悪魔"
		RETURN 0
	CASE "初期ステータス比率"
		RETURN 7, 7, 8, 7, 8, 10
	CASE "スキル"
		SIF TALENT:ARG:異能者 == 0
			RETURN 0
		SELECTCASE RAND:5
			CASE 0
				RESULT = [[スキル:アギ]]
				RESULT:1 = [[スキル:アギラオ]]
				RESULT:2 = [[スキル:アギダイン]]
			CASE 1
				RESULT = [[スキル:マグナ]]
				RESULT:1 = [[スキル:マグナス]]
				RESULT:2 = [[スキル:マグダイン]]
			CASE 2
				RESULT = [[スキル:アクア]]
				RESULT:1 = [[スキル:アクエス]]
				RESULT:2 = [[スキル:アクアダイン]]
			CASE 3
				RESULT = [[スキル:ガル]]
				RESULT:1 = [[スキル:ガルーラ]]
				RESULT:2 = [[スキル:ガルダイン]]
			CASE 4
				RESULT = [[スキル:コウハ]]
				RESULT:1 = [[スキル:バイコウハ]]
				RESULT:2 = [[スキル:マハコウハ]]
		ENDSELECT
		SELECTCASE RAND:4
			CASE 0
				RESULT:3 = [[スキル:シバブー]]
				RESULT:4 = [[スキル:ポイズマ]]
				RESULT:5 = [[スキル:マカジャマ]]
				RESULT:6 = [[スキル:パニックボイス]]
			CASE 1
				RESULT:3 = [[スキル:ハピルマ]]
				RESULT:4 = [[スキル:ドルミナー]]
				RESULT:5 = [[スキル:テンタラフー]]
				RESULT:6 = [[スキル:マリンカリン]]
			CASE 2
				RESULT:3 = [[スキル:メ・ディア]]
				RESULT:4 = [[スキル:メ・ディアラマ]]
				RESULT:5 = [[スキル:パトラ]]
				RESULT:6 = [[スキル:一分の魔脈]]
			CASE 3
				RESULT:3 = [[スキル:マカ・カジャ]]
				RESULT:4 = [[スキル:ポズムディ]]
				RESULT:5 = [[スキル:エストマ]]
				RESULT:6 = [[スキル:一分の活泉]]
		ENDSELECT
		RESULT:7 = [[スキル:ディア]]
		RESULT:8 = [[スキル:ディアラマ]]
		RESULT:9 = [[スキル:ディアラハン]]
		RETURN RESULT
	CASE "相性"
		SELECTCASE GET_TYPE(ARG:1)
			CASE "破魔"
				RETURN 0
			CASEELSE
				RETURN 100
		ENDSELECT
	CASE "素質"
		RETURN GETNUM(TALENT, "臆病"), GETNUM(TALENT, "痛みに弱い"), GETNUM(TALENT, "生意気"), GETNUM(TALENT, "好奇心"), GETNUM(TALENT, "素直")
	CASE "除外素質"
		RETURN GETNUM(TALENT, "針さばき"), GETNUM(TALENT, "調合知識")
	;処女は特別 0はランダム(20%), 1なら処女, 2なら非処女
	CASE "処女"
		IF  RAND:5 == 0
			RETURN 2
		ELSE
			RETURN 1
		ENDIF
	;戻り値は0:ランダム 1:小人体型 2:小柄体型 4:大柄 5:巨体
	CASE "体型"
		SELECTCASE RAND:10
			CASE IS < 5
				RETURN 2
			CASE IS < 8
				RETURN 3
			CASE IS < 9
				RETURN 4
		ENDSELECT
	;戻り値は0:ランダム 1:絶壁　2:貧乳　3:並　4:巨乳　5:爆乳
	CASE "胸"	
		IF TALENT:ARG:オトコ == 0
			SELECTCASE RAND:10
				CASE IS < 6
					RETURN 2
				CASE IS < 8
					RETURN 3
				CASE IS < 9
					RETURN 1
				CASE IS < 10
					IF RAND:10 == 0
						RETURN 5
					ELSE
						RETURN 4
					ENDIF
			ENDSELECT
		ELSEIF TALENT:ARG:男の娘 == 1
			SELECTCASE RAND:4
				CASE IS < 2
					RETURN 2
				CASE IS < 4
					RETURN 1
			ENDSELECT
		ELSE
			RETURN 1
		ENDIF
	;通常の人間と肉体部位が違う場合はここで直接代入してください
	CASE "肉体部位"
		RETURN 0
	CASE "ABL"
		SELECTCASE ABLNAME:(ARG:1)
			CASE "従順", "欲望", "技巧"
				RETURN !RAND:3+!RAND:5
			CASE "Ｃ感覚", "Ｖ感覚", "Ｂ感覚"
				RETURN !RAND:5
			CASE "マゾっ気", "露出癖", "奉仕精神"
				RETURN !RAND:10
			CASE "サドっ気"
				RETURN !RAND:5 * !ABL:ARG:マゾっ気
			CASE "Ａ感覚", "自慰中毒", "精液中毒", "レズっ気", "ＢＬっ気", "セックス中毒"
				RETURN !RAND:10
		ENDSELECT
	;EXPはABLが1以上ある場合、ある程度調整される(ただし、ABL0の時は調整が動かないので適当に初期値をきめてください)
	CASE "EXP"
		;全く手付かずの場合
		SIF !GROUPMATCH(1, !TALENT:ARG:処女, ABL:ARG:Ｃ感覚, ABL:ARG:Ｖ感覚, ABL:ARG:Ａ感覚, ABL:ARG:Ｂ感覚, ABL:ARG:技巧)
			RETURN 0
		SELECTCASE EXPNAME:(ARG:1)
			CASE "Ｖ経験"
				RETURN RAND(0, 20) * RAND:2
			CASE "Ａ経験"
				RETURN RAND(0, 20) * !RAND:5
			CASE "絶頂経験"
				RETURN RAND(0, 20) * RAND:2
			CASE "射精経験"
				RETURN RAND(0, 20) * RAND:2
			CASE "性交経験"
				RETURN 0
			CASE "膣射経験"
				RETURN EXP:ARG:射精経験/2*EXP:ARG:性交経験 > 0
			CASE "自慰経験"
				RETURN RAND(0, 20)
			CASE "調教自慰経験"
				RETURN 0
			CASE "精液経験"
				RETURN RAND(0, 20) * RAND:2
			CASE "奉仕快楽経験"
				RETURN 0
			CASE "フェラ経験"
				RETURN RAND(0, 10) * RAND:2
			CASE "愛情経験"
				RETURN 0
			CASE "苦痛快楽経験"
				RETURN RAND(0, 20) * !RAND:4
			CASE "放尿経験"
				RETURN 0
			CASE "嗜虐快楽経験"
				RETURN RAND(0, 10) * !RAND:4
			CASE "野外露出経験"
				RETURN 0
			CASE "レズ経験"
				RETURN RAND(0, 20) * !RAND:4
			CASE "ＢＬ経験"
				RETURN RAND(0, 20) * !RAND:4
			CASE "異常経験"
				RETURN 0
			CASE "緊縛経験"
				RETURN RAND(0, 10) * EXP:ARG:苦痛快楽経験 > 0
		ENDSELECT
	CASE "帽子"
		IF TALENT:ARG:オトコ == 0
			SELECTCASE RAND:10
				CASE IS < 1
					RETURN [[衣装:フロスト頭巾]]
				CASE IS < 8
					RETURN [[衣装:Ribbon]]
				CASE IS < 10
					RETURN [[衣装:ヘアバンド]]
			ENDSELECT
		ENDIF
	CASE "服"
		IF IS_MALE(ARG)
				SELECTCASE RAND:10
					CASE IS < 5
						RETURN [[衣装:詰襟]]
					CASE IS < 10
						RETURN [[衣装:ワイシャツ]]
				ENDSELECT
		ELSE
			IF RAND:10 == 0
				RETURN [[衣装:体操服]]
			ELSE
				SELECTCASE RAND:10
					CASE IS < 5
						RETURN [[衣装:セーラー服]]
					CASE IS < 10
						RETURN [[衣装:ブラウス]]
				ENDSELECT
			ENDIF
		ENDIF
	CASE "下衣"
		IF IS_MALE(ARG)
			RETURN [[衣装:ズボン]]
		ELSE
			LOCALS = 服
			IF CFLAG:ARG:LOCALS == [[衣装:体操服]]
				RETURN [[衣装:ブルマ]]
			ELSE
				RETURN [[衣装:スカート]]
			ENDIF
		ENDIF
	CASE "全身服"
		RETURN 0
	CASE "手"
		RETURN 0
	CASE "下着（上）"
		IF IS_MALE(ARG)
			RETURN 0
		ELSE
			IF TALENT:ARG:巨乳 == 1 || TALENT:ARG:爆乳 == 1
				RETURN [[衣装:ブラジャー]]
			ELSEIF RAND:2 == 0
				RETURN [[衣装:スポーツブラ]]
			ELSE
				RETURN 0
			ENDIF
		ENDIF
	CASE "下着（下）"
		IF IS_MALE(ARG)
			IF RAND:5
				RETURN [[衣装:トランクス]]
			ELSE
				RETURN [[衣装:ブーメランパンツ]]
			ENDIF
		ELSE
			IF RAND:3 == 0
				RETURN [[衣装:縞パン]]
			ELSE
				RETURN [[衣装:パンティ]]
			ENDIF
		ENDIF
	CASE "全身下着"
		RETURN 0
	CASE "靴下"
		IF IS_MALE(ARG)
			RETURN [[衣装:靴下]]
		ELSEIF RAND:3 == 0
			RETURN [[衣装:ニーソックス]]
		ELSE
			RETURN [[衣装:靴下]]
		ENDIF
	CASE "靴"
		IF IS_MALE(ARG)
			RETURN [[衣装:スニーカー]]
		ELSE
			RETURN [[衣装:シューズ]]
		ENDIF
	CASE "上着"
			LOCALS = 服
			IF CFLAG:ARG:LOCALS == [[衣装:ブラウス]] || CFLAG:ARG:LOCALS == [[衣装:ワイシャツ]]
				RETURN [[衣装:ブレザー]]
			ENDIF
	CASE "その他"
		RETURN RAND:10 ? 0 # [[衣装:眼鏡]]
	CASE "会話タイプ"
	;悪魔汎用使用しています0:ランダム 1:女_子供　2:女_少女　3:女_若者　4:女_大人　5:女_淑女　6:女_古風　7:男_若者　8:男_大人　9:獣_ケダモノ
		IF TALENT:ARG:オトコ == 0
			SELECTCASE RAND:10
				CASE 0
					RETURN 6
				CASE IS < 3
					SELECTCASE RAND:2
						CASE 0
							RETURN 4
						CASE 1
							RETURN 5
					ENDSELECT
				CASEELSE
					SELECTCASE RAND:3
						CASE 0
							RETURN 1
						CASE 1
							RETURN 2
						CASE 2
							RETURN 3
					ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE RAND:4
				CASE 0
					RETURN 7
				CASE 1
					RETURN 8
				CASE 2
					RETURN 10
				CASE 3
					RETURN 11
			ENDSELECT
		ENDIF
	CASE "名前"
		IF TALENT:ARG:ガンスリンガー == 1 && TALENT:ARG:サマナー == 3
			RESULTS = 魔弾の
		ELSEIF TALENT:ARG:サマナー == 1
			RESULTS = 魔界の
		ELSEIF TALENT:ARG:異能者 == 1
			RESULTS = 不思議系
		ELSE
		    RESULTS = 無名の
		ENDIF
		IF TALENT:ARG:Ｖ敏感 == 1 && EXP:ARG:Ｖ経験 > 0
			RESULTS += "淫売中学生"
		ELSEIF EXP:ARG:Ａ経験 > 0 && TALENT:ARG:Ａ敏感 == 1
			RESULTS += "肛虐中学生"
		ELSEIF TALENT:ARG:爆乳 == 1
			RESULTS += "爆乳中学生"
		ELSEIF TALENT:ARG:巨乳 == 1
			RESULTS += "巨乳中学生"
		ELSEIF TALENT:ARG:オトコ == 0
			RESULTS += "女子中学生"
		ELSEIF TALENT:ARG:男の娘 == 1
			RESULTS += "美少年中学生"
		ELSE
			RESULTS += "男子中学生"
		ENDIF
	CASE "愛称"
		IF TALENT:ARG:ガンスリンガー == 1 && TALENT:ARG:サマナー == 3
			RESULTS = 魔弾の
		ELSEIF TALENT:ARG:サマナー == 1
			RESULTS = 魔界の
		ELSEIF TALENT:ARG:異能者 == 1
			RESULTS = 不思議系
		ELSE
		    RESULTS = 無名の
		ENDIF
		IF TALENT:ARG:Ｖ敏感 == 1 && EXP:ARG:Ｖ経験 > 0
			RESULTS += "淫売中学生"
		ELSEIF EXP:ARG:Ａ経験 > 0 && TALENT:ARG:Ａ敏感 == 1
			RESULTS += "肛虐中学生"
		ELSEIF TALENT:ARG:爆乳 == 1
			RESULTS += "爆乳中学生"
		ELSEIF TALENT:ARG:巨乳 == 1
			RESULTS += "巨乳中学生"
		ELSEIF TALENT:ARG:オトコ == 0
			RESULTS += "女子中学生"
		ELSEIF TALENT:ARG:男の娘 == 1
			RESULTS += "美少年中学生"
		ELSE
			RESULTS += "男子中学生"
		ENDIF
	CASE "解説"
		RESULTS = 　人身売買組織に拉致された中学生
		IF TALENT:ARG:ガンスリンガー == 1
			RESULTS:1 = 　Gun型COMPに偶然適合し、Gun使いとしての能力を目覚めさせつつある
		ELSEIF TALENT:ARG:サマナー == 1
			RESULTS:1 =　偶然拾ったゲーム型COMPを使い、悪魔召喚の勉強中らしい
		ELSEIF TALENT:ARG:異能者
			RESULTS:1 = 　生まれつき不思議な能力を持っており、ご近所の平和を守っていたらしい
		ELSE
			IF TALENT:ARG:オトコ == 0
				RESULTS:1 = 　通学途中に突然さらわれた無力な少女である
			ELSE
				RESULTS:1 = 　通学途中に突然さらわれた無力な少年である
			ENDIF
		ENDIF
		RESULTS:2 = 　
		RESULTS:3 = 　■出身地：
			SELECTCASE RAND:13
				CASE 0
					RESULTS:3 += "トーキョー"
				CASE 1
					RESULTS:3 += "カントー地方"
				CASE 2
					RESULTS:3 += "カンサイ地方"
				CASE 3
					RESULTS:3 += "トーホク地方"
				CASE 4
					RESULTS:3 += "ホッカイドー"
				CASE 5
					RESULTS:3 += "シンエツ地方"
				CASE 6
					RESULTS:3 += "ホクリク地方"
				CASE 7
					RESULTS:3 += "トーカイ地方"
				CASE 8
					RESULTS:3 += "キンキ地方"
				CASE 9
					RESULTS:3 += "チューゴク地方"
				CASE 10
					RESULTS:3 += "シコク地方"
				CASE 11
					RESULTS:3 += "キューシュー地方"
				CASE 12
					RESULTS:3 += "オキナワ"
			ENDSELECT
		RESULTS:4 = 　■血液型：
			SELECTCASE RAND:4
				CASE 0
					RESULTS:4 += "Ａ型" 
				CASE 1
					RESULTS:4 += "Ｂ型"
				CASE 2
					RESULTS:4 += "ＡＢ型"
				CASE 3
					RESULTS:4 += "Ｏ型"
			ENDSELECT
		RESULTS:5 = 　■好きな食べ物：
			SELECTCASE RAND:11
				CASE 0
					RESULTS:5 += "カレーライス"
				CASE 1
					RESULTS:5 += "ラーメン"
				CASE 2
					RESULTS:5 += "プリン"
				CASE 3
					RESULTS:5 += "お寿司"
				CASE 4
					RESULTS:5 += "ハンバーグ" 
				CASE 5
					RESULTS:5 += "スパゲティ"
				CASE 6
					RESULTS:5 += "ステーキ"
				CASE 7
					RESULTS:5 += "パインサラダ"
				CASE 8
					RESULTS:5 += "エビチャーハン"
				CASE 9
					RESULTS:5 += "きつねうどん"
				CASE 10
					RESULTS:5 += "オムライス（旗付き）"
			ENDSELECT
		RESULTS:6 = 　
		RESULTS:7 = 　
		RESULTS:8 = 　
		RESULTS:9 = 　
		IF TALENT:ARG:処女 == 0 && TALENT:ARG:オトコ == 0
		RESULTS:10 = 　%UNICODE(0x2661)%ロストバージンの思い出%UNICODE(0x2661)%
			SELECTCASE RAND:5
				CASE 0
					RESULTS:11 = 　誕生日に
				CASE 1
					RESULTS:11 = 　夏休みに
				CASE 2
					RESULTS:11 = 　クリスマスに
				CASE 3
					RESULTS:11 = 　バレンタインデーに
				CASE 4
					RESULTS:11 = 　両親の結婚記念日に
			ENDSELECT
			SELECTCASE RAND:5
				CASE 0
					RESULTS:11 += "ラブホテルで、"
				CASE 1
					RESULTS:11 += "自宅で、"
				CASE 2
					RESULTS:11 += "小学校で、"
				CASE 3
					RESULTS:11 += "市営プールで、"
				CASE 4
					RESULTS:11 += "近所の公園で、"
			ENDSELECT
			SELECTCASE RAND:10
				CASE 0
					RESULTS:11 += "お父さん"
				CASE 1
					RESULTS:11 += "知らないおじさん"
				CASE 2
					RESULTS:11 += "近所のおじさん"
				CASE 3
					RESULTS:11 += "お兄ちゃん"
				CASE 4
					RESULTS:11 += "担任の先生"
				CASE 5
					RESULTS:11 += "同級生の男の子"
				CASE 6
					RESULTS:11 += "上級生の男の子"
				CASE 7
					RESULTS:11 += "弟"
				CASE 8
					RESULTS:11 += "ネットで知り合った大学生たち"
				CASE 9
					RESULTS:11 += "老紳士とその愛犬"
			ENDSELECT
			SELECTCASE RAND:5
				CASE 0
					RESULTS:11 += "にレイプされた"
				CASE 1
					RESULTS:11 += "にバックから犯された"
				CASE 2
					RESULTS:11 += "とラブラブエッチした"
				CASE 3
					RESULTS:11 += "に自分から捧げた"
				CASE 4
					RESULTS:11 += "に一回１万円でヤらせてあげた"
			ENDSELECT
		ENDIF
	;最後に呼び出すので何らかの調整が必要な場合はここで
	CASE "最終調整"
		RETURN 0
ENDSELECT
