;-------------------------------------------------
;下着ずらし挿入アナル
;セックス系コマンド、レベル1
;-------------------------------------------------
@ACT_COM73
PRINTL Move Undie & Anal Insert

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = 下着ずらし挿入アナル
;CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
B = 1500

;ABL:調教者の技巧をみる
IF ABL:PLAYER:2 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:2 == 1
	TIMES B , 1.20
ELSEIF ABL:PLAYER:2 == 2
	TIMES B , 1.40
ELSEIF ABL:PLAYER:2 == 3
	TIMES B , 1.60
ELSEIF ABL:PLAYER:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:欲望をみる
IF ABL:1 == 0
	TIMES B , 1.00
ELSEIF ABL:1 == 1
	TIMES B , 1.10
ELSEIF ABL:1 == 2
	TIMES B , 1.20
ELSEIF ABL:1 == 3
	TIMES B , 1.30
ELSEIF ABL:1 == 4
	TIMES B , 1.40
ELSE
	TIMES B , 1.50
ENDIF

;PALAM:潤滑をみる
IF PALAM:4 < PALAMLV:1
	TIMES B , 0.60
ELSEIF PALAM:4 < PALAMLV:2
	TIMES B , 0.80
ELSEIF PALAM:4 < PALAMLV:3
	TIMES B , 1.00
ELSEIF PALAM:4 < PALAMLV:4
	TIMES B , 1.20
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES B , 1.40
ENDIF

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;再装填処理中は射精ゲージは20分の1に
SIF CFLAG:PLAYER:9 > 0
	B /= 20


SIF HAVE_PENIS(PLAYER)
	BASE:PLAYER:2 += B

;調教者←→調教対象
TFLAG:28 = 3
TEQUIP:TARGET:Ａ挿入者 = PLAYER
TFLAG:挿入続行 = 1
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、Willpower
DOWNBASE:TARGET:体力 += 100
DOWNBASE:TARGET:気力 += 120

SOURCE:TARGET:快Ａ = 350
SOURCE:TARGET:情愛 = 200
SOURCE:TARGET:痛み = 700
SOURCE:TARGET:中毒充足 = 200
SOURCE:TARGET:露出 = 20
SOURCE:TARGET:逸脱 = 200
SOURCE:TARGET:反感 = 500

;上のほうで計算した汚れデータ
SOURCE:TARGET:不潔 = Y*10 + 60

;調教者にFEELＣ
SOURCE:PLAYER:0 = 600
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:鬱屈 += 500

;Aへの苦痛は先に処理しておく
;EXP:Ａ経験をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:TARGET:痛み , 3.00
	TIMES SOURCE:TARGET:反感 , 3.00
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:TARGET:痛み , 1.20
	TIMES SOURCE:TARGET:反感 , 2.00
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:TARGET:痛み , 0.80
	TIMES SOURCE:TARGET:反感 , 1.50
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:TARGET:痛み , 0.60
	TIMES SOURCE:TARGET:反感 , 1.20
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:TARGET:痛み , 0.40
ELSEIF EXP:1 >= EXPLV:5
	TIMES SOURCE:TARGET:痛み , 0.20
ENDIF

;PALAM:潤滑をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:TARGET:痛み , 1.20
	TIMES SOURCE:TARGET:反感 , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:TARGET:痛み , 1.00
	TIMES SOURCE:TARGET:反感 , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:TARGET:痛み , 0.60
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:TARGET:痛み , 0.30
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:TARGET:痛み , 0.20
ENDIF

;中毒による中毒充足のソースは先に処理しておく
;セックス中毒を見る
IF ABL:18 == 0
	TIMES SOURCE:TARGET:中毒充足 , 0.00
ELSEIF ABL:18 == 1
	TIMES SOURCE:TARGET:中毒充足 , 0.30
ELSEIF ABL:18 == 2
	TIMES SOURCE:TARGET:中毒充足 , 0.60
ELSEIF ABL:18 == 3
	TIMES SOURCE:TARGET:中毒充足 , 0.90
ELSEIF ABL:18 == 4
	TIMES SOURCE:TARGET:中毒充足 , 1.20
ELSEIF ABL:18 == 5
	TIMES SOURCE:TARGET:中毒充足 , 1.50
ELSEIF ABL:18 == 6
	TIMES SOURCE:TARGET:中毒充足 , 1.80
ELSEIF ABL:18 == 7
	TIMES SOURCE:TARGET:中毒充足 , 2.10
ELSEIF ABL:18 == 8
	TIMES SOURCE:TARGET:中毒充足 , 2.40
ELSEIF ABL:18 == 9
	TIMES SOURCE:TARGET:中毒充足 , 3.00
ELSEIF ABL:18 >= 10
	TIMES SOURCE:TARGET:中毒充足 , 5.00
ENDIF

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
;射精先タイプの設定
TCVAR:PLAYER:ペニス使用対象 = TARGET
TCVAR:PLAYER:射精箇所 = 2
;射精先選択の可・不可
TFLAG:704 = 1
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
;奴隷のＡ⇔調教者のＰの汚れが移動
CALL MOVE_STAIN("アナル", TARGET, "ペニス", PLAYER)

;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ非被吸収 == 0
	LOCAL = (RAND:10+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ非被吸収 == 0
	LOCAL = (RAND:10+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;A経験
CALL ADDS_EXP("Ａ経験", 2, TARGET)
;PRINTL ＡEXP＋２

;SexEXP
CALL ADDS_EXP("性交経験", 1, TARGET)
;PRINTS EXPNAME:4;
;PRINTL +1
;BestialEXP
IF (TALENT:PLAYER:獣 || TALENT:PLAYER:鳥) && !(TALENT:TARGET:獣 || TALENT:TARGET:鳥)
	CALL ADDS_EXP("獣姦経験", 1, TARGET)
ENDIF

;LesbianEXP
A = 4
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("レズ経験", A, TARGET)
	CALL ADDS_EXP("レズ経験", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ経験", A, TARGET)
	CALL ADDS_EXP("ＢＬ経験", A, PLAYER)
ENDIF

;LoveEXP
IF TALENT:3 == 1 && ASSIPLAY == 0
;	PRINTS EXPNAME:23
;	PRINTL +1
	CALL ADDS_EXP("愛情経験", 1, TARGET)
;	PRINTFORML LoveEXP(主人)＋１
	CALL ADDS_EXP("愛情経験", 1, MASTER)
;	PRINTFORML ServicPlsEXP(調教者) + 1
	CALL ADDS_EXP("奉仕快楽経験", 1, PLAYER)
ELSEIF TALENT:6 == 1 && ASSIPLAY == 0
;	PRINTS EXPNAME:23
;	PRINTL +2
	CALL ADDS_EXP("愛情経験", 2, TARGET)
;	PRINTFORML LoveEXP(主人)＋１
	CALL ADDS_EXP("愛情経験", 1, MASTER)
;	PRINTFORML ServicPlsEXP(調教者) + 1
	CALL ADDS_EXP("奉仕快楽経験", 1, PLAYER)
ENDIF

;依存度ベクトル
TFLAG:33 = 1

;依存度補正値(服従で1、隷属で2）
IF TALENT:5 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:8 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1
;-------------------------------------------------
;実行判定処理
;正常位アナルと同じ
;-------------------------------------------------
@COM_ORDER_73, ARG, ARG:1
CALL COM_ORDER_62, ARG, ARG:1
;-------------------------------------------------
;実行値
;-------------------------------------------------
@COM_ORDER_NUM_73
RETURN 30
;===================================================
;調教タイプ
