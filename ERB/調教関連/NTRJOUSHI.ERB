;--------
;上司来襲
;--------
@JOUSHI_RAISYUU
#LOCALSIZE 7
#DIM リスト, 200
;CFLAG:上司来襲持ち用
#DIM 上司リスト, 200
VARSET リスト, -1
VARSET 上司リスト, -1
LOCAL:1 = 0
LOCAL:5 = 0
LOCAL:6 = 0
FOR LOCAL,1,CHARANUM
	LOCAL:2 = 0
	SIF CFLAG:LOCAL:この場に居ないフラグ == 1
		CONTINUE
	SIF TALENT:LOCAL:ＮＴＲ
		CONTINUE
	SIF !(TALENT:LOCAL:恋慕 || TALENT:LOCAL:親愛)
		CONTINUE
	SIF TALENT:LOCAL:妊娠 || TALENT:LOCAL:育児中
		CONTINUE
	SIF CFLAG:LOCAL:労役フラグ > 0
		CONTINUE
	SIF (CFLAG:LOCAL:その他 == 1116 || CFLAG:LOCAL:その他2 == 1116 || CFLAG:LOCAL:その他3 == 1116)
		CONTINUE
	SIF TALENT:LOCAL:オトコ
		CONTINUE
	SIF TALENT:LOCAL:魅力
		LOCAL:2 += 10
	SIF TALENT:LOCAL:巨乳 || TALENT:LOCAL:爆乳
		LOCAL:2 += 30
	SIF TALENT:LOCAL:妻
		LOCAL:2 += 30
	SIF (CFLAG:LOCAL:その他 == 1117 || CFLAG:LOCAL:その他2 == 1117 || CFLAG:LOCAL:その他3 == 1117)
		LOCAL:2 += 50
	SIF (危険日(LOCAL)) > 0
		LOCAL:2 += 40
		
	IF CFLAG:LOCAL:上司イベント進行度 > 0
		上司リスト:(LOCAL:6) = LOCAL
		LOCAL:5 = 1
		LOCAL:6 ++
	ELSEIF RAND:100 < 10 + LOCAL:2
		リスト:(LOCAL:1) = LOCAL
		LOCAL:1 ++
	ENDIF
NEXT
;LOCAL:1 == 0の場合、女要求率０
SIF LOCAL:1 == 0 && LOCAL:6 == 0
	GOTO YOUKYUUNASHI
;上司の餌食決定＞LOCAL
;CFLAG:上司イベント進行度がある場合は優先
$LOOP_JOUSHILIST
IF LOCAL:5 == 1
	LOCAL = 上司リスト:(RAND:(LOCAL:6))
ELSE
	LOCAL = リスト:(RAND:(LOCAL:1))
ENDIF
;ありえないと思うけど念のため。LOCALがキャラ登録番号の範囲外の場合なにもしない。
;CFLAG:上司イベント進行度持ちがいる場合はループする
IF LOCAL < 1 && LOCAL >= CHARANUM
	SIF LOCAL:5 == 1
		GOTO LOOP_JOUSHILIST
	RETURN 0
ENDIF
;女要求率
LOCAL:2 = 100
;要求金額倍率
LOCAL:3 = 0
;要求額
LOCAL:4 = 0

;金のみ（所持額の３分の一）か、金（所持額の半分）か彼女かの分岐
;どちらかの所持金が目標金額の半分を超えると、選択肢に恋慕持ちの人間の女が加わる率が+40％
SIF MONEY > (MONEY:2 / 2) || (MONEY:1 * 50) > (MONEY:2 / 2)
	LOCAL:2 += 40
;以下の素質が有ると+30％
SIF TALENT:LOCAL:魅力 || TALENT:LOCAL:巨乳 || TALENT:LOCAL:爆乳 || TALENT:LOCAL:妻
	LOCAL:2 += 30
;危険日は+40％
SIF (危険日(LOCAL)) > 0
	LOCAL:2 += 40
;要求金額倍率（選択肢）決定
IF RAND:100 < LOCAL:2
	LOCAL:3 = 2
ELSE
	LOCAL:3 = 5
ENDIF
;LOCAL:1 == 0の場合、女要求率０
$YOUKYUUNASHI
SIF LOCAL:1 == 0 && LOCAL:6 == 0
	LOCAL:3 = 5
;魔貨（換金した値）が￥の2倍より多い場合は魔貨要求
;最低要求額は￥１０万（2000魔貨）
;金も女も無い様な所には来ない
IF (MONEY:1 * 50) > MONEY * 2
	LOCAL:4 = MONEY:1 / LOCAL:3
	IF LOCAL:4 < 2000
		LOCAL:4 = 2000
		LOCAL:3 = 1
		SIF LOCAL:1 == 0
			RETURN 1
	ENDIF
ELSE
	LOCAL:4 = MONEY / LOCAL:3
	IF LOCAL:4 < 100000
		LOCAL:4 = 100000
		LOCAL:3 = 1
		SIF LOCAL:1 == 0
			RETURN 1
	ENDIF
ENDIF
;CFLAG:LOCAL:上司イベント進行度が6の場合は最終段階へ
IF LOCAL:5 > 0
	IF CFLAG:LOCAL:上司イベント進行度 == 10
		LOCAL:3 = 10
	ENDIF
ENDIF
IF LOCAL:3 == 10
	PRINTFORML 
	PRINTFORML 
	PRINTFORMW ファントムソサエティの幹部の男が訪ねて来た
	PRINTFORML 
	PRINTFORML 『先日、貴様が組織の情報を外部に流しているとの情報が入ってな』
	PRINTFORML 『貴様をよく思っていない連中が始末しろと言い出し始めたのだ』
	PRINTFORML 『私もこんな下らん事で優秀な駒を失いたくないのでな、火消しを引き受けてやる』
	LOCAL:4 = MONEY
	TIMES LOCAL:4,0.9
	LOCAL:4 = LIMIT(LOCAL:4 , 1000000, LOCAL:4)
	PRINTFORM 『なに、￥
	SETCOLOR COLOR("RED")
	PRINTFORM ￥{LOCAL:4}
	RESETCOLOR
	PRINTFORMW もあれば事足りるだろうが、まあ額が額だ』
	PRINTFORM 『なければ…
	SETCOLOR COLOR("Dark-pink")
	PRINTFORM %CALLNAME:LOCAL%
	RESETCOLOR
	PRINTFORMW を助手として借りようか』
ELSE
	PRINTFORML 
	PRINTFORML 
	PRINTFORMW ファントムソサエティの幹部の男が訪ねて来た
	PRINTFORML 
	PRINTFORML 『ふんっ、相変わらず癇に障る面をしているな』
	PRINTFORMW 『急遽、財界の人間を饗応する事になってな、貴様にも協力して貰う事になった』
	PRINTFORML 
	IF (MONEY:1 * 50) > MONEY * 2
		PRINTFORM 『
		SETCOLOR COLOR("RED")
		PRINTFORM {LOCAL:4}魔貨
		RESETCOLOR
		PRINTFORMW 程、都合してもらおうか』
	ELSE
		PRINTFORM 『
		SETCOLOR COLOR("RED")
		PRINTFORM ￥{LOCAL:4}
		RESETCOLOR
		PRINTFORMW 程、都合してもらおうか』
	ENDIF
	IF LOCAL:3 < 5
		PRINTFORML 『無理ならば、そうだな…』
		PRINTFORMW 男の視線が%CALLNAME:MASTER%の後ろへ向けられる
		PRINTFORML 
		;目をつけられた場合
		IF CFLAG:LOCAL:上司イベント進行度 > 0
			PRINTFORM 『代わりに
			SETCOLOR COLOR("Dark-pink")
			PRINTFORM %CALLNAME:LOCAL%
			RESETCOLOR
			PRINTFORML を預けて貰おうか』
			PRINTFORMW 『前回好評だったのでな、クックックッ』
		;通常
		ELSE
			PRINTFORML 『代わりにそのオンナを預けて貰おうか』
			PRINTFORM ニヤニヤと笑みを浮かべながら、
			SETCOLOR COLOR("Dark-pink")
			PRINTFORM %CALLNAME:LOCAL%
			RESETCOLOR
			PRINTFORML を舐めるように見ている
			PRINTFORML 
			PRINTFORML 『その器量ならば、さぞ役に立つだろうな、クククッ』
		ENDIF
	ENDIF
ENDIF
PRINTFORML 
IF LOCAL:3 == 2 || LOCAL:3 == 10
	PRINTFORM どうする？　　　所持金:￥{MONEY}　　{MONEY:1}魔貨　　　
	PRINTFORM %CALLNAME:LOCAL%　
	IF (危険日(LOCAL)) == 2
		SETCOLOR 0xCC33FF
			CALL HEARTB
		PRINTFORM 発情中
			CALL HEARTB
		RESETCOLOR
	ELSEIF (危険日(LOCAL)) == 1
		SETCOLOR 0xCC33FF
			CALL HEARTB
		PRINTFORM 危険日
			CALL HEARTB
		RESETCOLOR
	ELSE
		PRINTFORM 
	ENDIF
	PRINTFORMW 
ELSE
	PRINTFORMW どうする？　　　所持金:￥{MONEY}　　{MONEY:1}魔貨
ENDIF

IF LOCAL:3 == 10
	IF MONEY < 1000000
		CALL INPUT_SELECT(2, "女を預ける")
	ELSE
		CALL INPUT_SELECT(1, "支払う",2, "女を預ける")
	ENDIF
ELSEIF LOCAL:3 == 2
	CALL INPUT_SELECT(1, "支払う",2, "女を預ける", 3, "断る")
ELSEIF LOCAL:3 == 1
	CALL INPUT_SELECT(2, "女を預ける", 3, "断る")
ELSE
	CALL INPUT_SELECT(1, "支払う", 3, "断る")
ENDIF
	SELECTCASE RESULT
		CASE 1
			IF (MONEY:1 * 50) > MONEY * 2
				MONEY:1 -= LOCAL:4
				PRINTFORMW {LOCAL:4}魔貨を支払った
			ELSE
				MONEY -= LOCAL:4
				PRINTFORMW ￥{LOCAL:4}を支払った
			ENDIF
			IF LOCAL:3 == 10
				PRINTFORML 『おおっ、確かに受け取ったぞ』
				PRINTFORML 『後は私に任せておけ』
				PRINTFORMW 男は入金を確認すると、足早に去っていった
				FLAG:上司来襲率 = 0
				CFLAG:LOCAL:上司イベント進行度 = 0
			ELSE
				PRINTFORML 
				PRINTFORML 『なかなか物分りのいいヤツだな』
				PRINTFORML 『これからも貴様なりに組織の役に立つように』
				PRINTFORMW 男は見下しながら言い捨て、去っていった
				FLAG:上司来襲率 = FLAG:上司来襲率 / 3
				;10％の確率で上司のターゲットから外れる
				IF RAND:100 < 10 && LOCAL:3 == 2 && CFLAG:LOCAL:上司イベント進行度 > 0
					PRINTFORMW %CALLNAME:LOCAL%は%CALLNAME:MASTER%にそっと抱きつき、労っている
					CFLAG:LOCAL:上司イベント進行度 = 0
				ENDIF
				;CFLAG:上司イベント進行度がある場合は10％の確率で3段階目に移行する
				IF RAND:100 < 10 && LOCAL:3 == 2 && CFLAG:LOCAL:上司イベント進行度 < 7
					PRINTFORMW %CALLNAME:LOCAL%は%CALLNAME:MASTER%を心配そうに見つめている
					PRINTFORMW そして、幹部の男に密かに渡されたメモを握り締めた…
					CFLAG:LOCAL:上司イベント進行度 = 7
				ENDIF
			ENDIF
		CASE 2
			IF LOCAL:3 == 10
				PRINTFORML 
				PRINTFORML 『そうか、では預かるとしよう…助手としてな』
				PRINTFORML 『事態の収拾には一晩あれば十分だろう』
				PRINTFORML 『ではな…楽しみにしておけ』
				PRINTFORMW %CALLNAME:LOCAL%は悲しげな表情で男に連れて行かれた
			ELSEIF CFLAG:LOCAL:上司イベント進行度 > 3 && CFLAG:LOCAL:上司イベント進行度 <= 6
				PRINTFORML 
				PRINTFORMW %CALLNAME:MASTER%が%CALLNAME:LOCAL%を預けると述べると、男はニタリと頬を吊り上げた
				PRINTFORML 『ククッ、では、預からせてもらおう』
				PRINTFORML 『なに、明日の朝までには送り届けさせてもらうよ』
				PRINTFORMW 『それにしても…』
				IF TALENT:LOCAL:巨乳 || TALENT:LOCAL:爆乳
					PRINTFORML 男は%CALLNAME:LOCAL%の後ろに周ると
					PRINTFORML 豊満な胸を揉みしだきながら%CALLNAME:LOCAL%の股間に手をやり愛撫し始めた…
					PRINTFORML 男の激しい指使いに%CALLNAME:LOCAL%は腰砕けになり、足は今にも崩れそうなほど震えている
					PRINTFORMW 『ハハハハッ、相変わらずいやらしいカラダだ…これは今度の先方も満足するだろうな』
				ELSE
					PRINTFORML 男は%CALLNAME:LOCAL%を抱き寄せ、%CALLNAME:LOCAL%の唇にキスをしながら
					PRINTFORML %CALLNAME:LOCAL%の股間を弄り始めた…
					PRINTFORML %CALLNAME:LOCAL%は男の巧みな指使いに翻弄され、蕩けた顔をしながら男の唇にしゃぶりついている
					PRINTFORMW 『ククッ、相変わらず感度の良いカラダだ…これは今度の先方も満足するだろうな』
				ENDIF
				PRINTFORML 
				PRINTFORMW %CALLNAME:LOCAL%は快楽で腰砕けになったカラダを引きずられながら、男に連れられていった
			ELSEIF CFLAG:LOCAL:上司イベント進行度 > 6
				;セックス系
				IF RAND:2 == 0
					PRINTFORML 
					PRINTFORMW %CALLNAME:MASTER%が%CALLNAME:LOCAL%を預けると述べると、男はニタリと頬を吊り上げた
					PRINTFORML 『ククッ、では、預からせてもらおう』
					PRINTFORML 『なに、明日の朝までには送り届けさせてもらうよ』
					PRINTFORMW 『それにしても…』
					IF TALENT:LOCAL:巨乳 || TALENT:LOCAL:爆乳
						PRINTFORML 男は%CALLNAME:LOCAL%の後ろに周りジッパーを下しペニスを取り出すと
						PRINTFORML %CALLNAME:LOCAL%の豊満な胸を揉みしだきながら%CALLNAME:LOCAL%の下着をずらし膣内に挿入した
						PRINTFORML %CALLNAME:LOCAL%はいきなりの衝撃に目を白黒させながら喘いでいる
						PRINTFORMW 『ハハハッ、相変わらず良い具合の穴だ、先方がご執心になるのもわかるな』
						PRINTFORMW 
						PRINTFORMW %CALLNAME:LOCAL%は男にバックから突かれ、豊満な胸を大きく揺らしながら蕩けた表情を浮かべている
						PRINTFORMW 男が一際強く突き、%CALLNAME:LOCAL%が歓喜の悲鳴を上げると、男と%CALLNAME:LOCAL%の結合部から大量の精液が溢れ出した…
						PRINTFORMW 『おっと、あまりに気持ちが良すぎて思わず中出ししてしまったよ』
						PRINTFORMW 『まぁ、別に孕ませてしまってもかまわんだろう…？』
						PRINTFORML 
						PRINTFORMW 
						PRINTFORMW 『クククッ、もう少し味わいたいが、そろそろ行かんとな』
						
;						PRINTFORMW 『クククッ、どうやら%CALLNAME:LOCAL%クンはあまりに気持ち良くて腰を抜かしてしまったようだな』
;						PRINTFORMW 『私がこのまま今日の会場まで連れて行ってやるとするよ』
					ELSE
						PRINTFORML 男は%CALLNAME:LOCAL%を抱き寄せ唇を奪うと、ジッパーを下してペニスを取り出し%CALLNAME:LOCAL%の秘部に押し当て
						PRINTFORML キスで惚けた顔をしている%CALLNAME:LOCAL%の秘部を下から一気に突きあげた
						PRINTFORMW %CALLNAME:LOCAL%は男に抱えられながら犯され、声にならない声をあげている
						PRINTFORMW 『やはり%CALLNAME:LOCAL%クンのカラダは魅力的だな、思わず貴様の前で襲ってしまったよ』
						PRINTFORMW 
						PRINTFORMW %CALLNAME:LOCAL%は振り落とされないように男の腰に足を絡ませながら、男と熱烈なキスを交わしている
						PRINTFORMW 男は数回激しく突きあげたあと、%CALLNAME:LOCAL%を強く抱きしめそのままの体勢で射精をはじめた
						PRINTFORMW %CALLNAME:LOCAL%もまた、手を男の首にまわし、腰に絡ませている足に力をこめた
						PRINTFORMW 余韻に浸る%CALLNAME:LOCAL%と男との結合部から大量の精液が流れおち、床に白い水たまりを作っている
						PRINTFORML 
						PRINTFORMW 
						PRINTFORMW 『クククッ、もう少し味わいたいが、そろそろ行かんとな』
						
;						PRINTFORMW 『クククッ、どうやら%CALLNAME:LOCAL%クンはあまりに気持ち良くて腰を抜かしてしまったみたいでな』
;						PRINTFORMW 『私がこのまま今日の会場まで連れて行ってやるとするよ』
					ENDIF
					PRINTFORML 
					PRINTFORMW 乱れた衣装を馴れた手つきで整えると、%CALLNAME:LOCAL%は%CALLNAME:MASTER%を一瞥することなく、男に続いて部屋を出て行った
					
;					PRINTFORMW %CALLNAME:LOCAL%は男と恋人のように交わったまま連れられていった
				;奉仕系
				ELSE
					PRINTFORML 
					PRINTFORMW %CALLNAME:MASTER%が%CALLNAME:LOCAL%を預けると述べると、男はニタリと頬を吊り上げた
					PRINTFORML 『ククッ、では、預からせてもらおう』
					PRINTFORMW 『なに、明日の朝までには送り届けさせてもらうよ』
					PRINTFORML 
					PRINTFORMW 『それにしても…』
					PRINTFORML 『%CALLNAME:LOCAL%クンのウデは落ちてないだろうな？』
					PRINTFORML 『ククッ、大事な接待だ…　粗相があってはな…』
					PRINTFORMW わざとらしい言葉とともに男が%CALLNAME:LOCAL%の体を舐めるように見つめる
					PRINTFORML 
					IF RAND:2 == 0 && (TALENT:LOCAL:巨乳 || TALENT:LOCAL:爆乳)
						PRINTFORMW それだけで意図を察した%CALLNAME:LOCAL%は男の前にひざまずくと慣れた手つきでペニスを取り出し咥え始めた
						PRINTFORML 『ハハッ、わるいな、%CALLNAME:LOCAL%クン』
						PRINTFORMW 『おおぅ…この舌使い、先方がご執心になるのも無理もない』
						PRINTFORML 
						PRINTFORMW %CALLNAME:LOCAL%は、口での奉仕を続けながら胸をはだけると当然のように男のモノを胸で挟み込んだ
						PRINTFORML 『おおぅ…胸まで使うとは…』
						PRINTFORML 男の声を聞きいた%CALLNAME:LOCAL%は、豊満な胸と口を使っての奉仕をより激しくしていく
						PRINTFORMW やがて、男が呻きながら射精を始めると%CALLNAME:LOCAL%は蕩けた表情で顔や胸に飛び散った精液を舐め取っていった
						PRINTFORML 
						PRINTFORMW 『ククッ、いい体だ…これなら問題はないだろう…ハハハッ』
						PRINTFORML 
						PRINTFORMW 男は笑いながらペニスをしまうと、体に付いた精液を指ですくって舐め取っている%CALLNAME:LOCAL%を連れ部屋を出て行った
					ELSE
						PRINTFORMW それだけで意図を察した%CALLNAME:LOCAL%は男の前にひざまずくと慣れた手つきでペニスを取り出し咥え始めた
						PRINTFORML 『ハハッ、わるいな、%CALLNAME:LOCAL%クン』
						PRINTFORMW 『おおぅ…この舌使い、先方がご執心になるのも無理もない』
						PRINTFORML 
						PRINTFORMW 男のモノを口に含み舐めているだけで、奉仕する%CALLNAME:LOCAL%の顔がとろんと蕩けた顔になっていく
						PRINTFORML やがて、男が呻きながら%CALLNAME:LOCAL%の口内へ精液を吐き出すと、
						PRINTFORMW %CALLNAME:LOCAL%は喉を鳴らして飲み込み、当然とばかりにペニスに吸い付き口を使って掃除を始めた
						PRINTFORML 
						PRINTFORMW 『ククッ、後始末までしようとするとは…躾が行き届いているな…』
						PRINTFORMW 『これなら問題はないだろう…ハハハッ』
						PRINTFORML 
						PRINTFORMW 男は笑いながらペニスをしまうと、名残惜しそうに唇についた精液を舐め取る%CALLNAME:LOCAL%を連れ部屋を出て行った
					ENDIF
				ENDIF
			ELSE
				PRINTFORML 
				PRINTFORMW %CALLNAME:MASTER%が%CALLNAME:LOCAL%を預けると述べると、男はニタリと頬を吊り上げた
				PRINTFORML 『ククッ、では、預からせてもらおう』
				PRINTFORML 『なに、明日の朝までには送り届けさせてもらうよ』
				PRINTFORMW 『それにしても…』
				IF TALENT:LOCAL:巨乳 || TALENT:LOCAL:爆乳
					PRINTFORML 男は%CALLNAME:LOCAL%の豊満な胸を鷲掴みにした
					PRINTFORMW 『このカラダなら先方も喜ぶだろうなぁ、ハハハハッ』
				ELSE
					PRINTFORML 男は%CALLNAME:LOCAL%を抱き寄せ、指で顎先を持ち上げると%CALLNAME:LOCAL%の顔をネットリと眺めた
					PRINTFORMW 『ククッ、これなら先方も満足するだろうなぁ』
				ENDIF
				PRINTFORML 
				PRINTFORMW %CALLNAME:LOCAL%は不安そうな視線をこちらに送りながら、男に連れられていった
			ENDIF
			FLAG:上司来襲率 = 0
			;労役とパーティーを外す
			SIF TARGET == LOCAL
				TARGET = -1
			SIF ASSI == LOCAL
				ASSI = -1
			CFLAG:LOCAL:この場に居ないフラグ = 1
			CFLAG:LOCAL:労役フラグ = 0
			IF CFLAG:LOCAL:劇場出演順番 != 0
				DE:(49+(CFLAG:LOCAL:劇場出演順番)):(CFLAG:LOCAL:労役店舗-1) = -1
				DE:(59+(CFLAG:LOCAL:劇場出演順番)):(CFLAG:LOCAL:労役店舗-1) = -1
				CFLAG:LOCAL:労役フラグ = 0
				CFLAG:LOCAL:劇場出演順番 = 0
				CFLAG:LOCAL:労役店舗 = 0
			ENDIF
			SIF CPOS(LOCAL) > 0
				CALL REMOVE_POSITION,CPOS(LOCAL)
			;これが付くとNTR夜這い後に陵辱描写、翌朝帰ってくる
			CFLAG:LOCAL:上司来襲 = 1
			;上司イベントの進行度
			SIF CFLAG:LOCAL:上司イベント進行度 == 0
				CFLAG:LOCAL:上司イベント進行度 = 1
		CASE 3
			PRINTFORML 
			PRINTFORMW 『貴様っ…』
			PRINTFORML 『この事は上に報告させてもらうからなっ！！』
			PRINTFORML 男はドアを乱暴に閉め、去っていった
			PRINTFORML 
			PRINTFORMW 組織貢献度が{(FLAG:組織貢献度 / 20)}下がりました
			;断ると組織貢献度が20％減
