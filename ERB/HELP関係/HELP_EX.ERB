@DICTIONARY_ITEM_MAIN
DRAWLINE
PRINTFORML A rough explanation of [Training items]
DRAWLINE
;各表示
FOR LOCAL, 0, 50
	STRLENS ITEMNAME:LOCAL
	SIF RESULT > 0
		PRINTFORMLC [{LOCAL, 2}] %ITEMNAME_E(LOCAL)%
NEXT
PRINTL 
DRAWLINE
PRINTLC [100]Return

$INPUT_LOOP
INPUT
IF RESULT == 100
	RETURN 0
ELSEIF (RESULT >= 0 && RESULT <= 99)
	Q = RESULT
	STRLENS ITEMNAME:Q
	IF RESULT > 0
		PRINTFORML 【%ITEMNAME_E(Q)%】
		TRYCALLFORM DICTIONARY_ITEM_{Q}
		RESTART
	ELSE
		GOTO INPUT_LOOP
	ENDIF
ELSE
	GOTO INPUT_LOOP
ENDIF
RESTART

