;===========================================================================
@GREET_PUB324,ARG
;最初の挨拶
IF FLAG:月齢 == 8
;５回に一回くらいの確率で発生。
	SELECTCASE RAND:10
		CASE 0
			PRINTFORMW 「On the day of the full moon, %CSTR:ARG:11% is different from usual.」
			;「満月の日は、いつもの%CSTR:ARG:11%と違うんだ。」
			PRINTFORMW 「Whatever I'll do, please forgive me.」
			;「なにをしても、許してくれるかい。」
			PRINTFORML 
			PRINTFORML [1]Forgive
			;[1]許す
			PRINTFORML [2]I can't promise that
			;約束はできない
			$INPUT_LOOPF1
			INPUT
			PRINTFORML
			IF RESULT == 1
				;1.許す
				PRINTFORMW 「さっすが。」
				IF TALENT:PLAYER:オトコ == 0
					PRINTFORMW 「じゃあ……
					SETCOLOR COLOR("pink")
					PRINTFORMW 　%CSTR:ARG:11%と、イイコトしようぜ%UNICODE(0x2661)%」
					RESETCOLOR
					PRINTFORMW %CALLNAME:ARG%はギンギンに勃起したペニスを%CALLNAME:PLAYER%の身体に擦りつけながら、ねっとりとした手つきで全身を撫で回し始めた…
					PRINTFORMW 振り払いますか？
					CALL INPUT_YN
					IF RESULT == 0
					;はい
						IF PALAM:PLAYER:欲情 > 5000 || 危険日(PLAYER) == 2
						;普通に会話しても交渉目的がヤらせてと犯してしか出ないような時は強制的に食われちゃいます
							IF RAND:2 ==0
								PRINTFORMW しかし、発情した%CALLNAME:PLAYER%の体は%CALLNAME:ARG%の愛撫に逆らえず、とうとう%CALLNAME:ARG%のペニスを受け入れてしまった…
							ELSE
								PRINTFORMW %CALLNAME:PLAYER%の敏感な部分をたっぷりと責めたてられ、%CALLNAME:PLAYER%ははしたない声を上げている…
								PRINTFORM やがて、
								;死に分岐じゃないよね？
								IF TALENT:PLAYER:男の娘
									PRINTFORM 物欲しげにヒクつく%CALLNAME:PLAYER%の尻穴に
								ELSE
									PRINTFORM 愛液でぐっしょりと濡れた%CALLNAME:PLAYER%の秘裂に
								ENDIF
								PRINTFORMW %CALLNAME:ARG%の肉竿が突き入れられた…
							ENDIF
							$CYNICAL_SEXTALK
							TFLAG:捧げたキャラ = PLAYER
							CALL SEXIAL_NEGOCIATE,ARG,PLAYER
							PRINTW
							TFLAG:TALK用成功判定値 = FLAG:悪魔満足度 + CFLAG:ARG:友好度
							CFLAG:ARG:友好度 += FLAG:悪魔満足度
							;敗北してる場合ここでゲームオーバー
							IF FLAG:敗北フラグ
								;口上呼び出し
								CALL KOJO_TALK,ARG,42
								CALL SEXTALK_LOSE,ARG
							ENDIF
							;話しかけたキャラが倒れてたら逃げられる
							CALL INPUTABLE_CHARA,PLAYER
							IF RESULT == 0
								CALL KOJO_TALK,ARG,43
								RETURN 5
							ENDIF
							IF TFLAG:TALK用成功判定値 < 30
							;払ったのに逃げられる
								;口上呼び出し
								CALL KOJO_TALK,ARG,44
								RETURN 5
							ELSEIF TFLAG:TALK用成功判定値 < 80
							;満足し、アイテムをくれる
								;口上呼び出し
								CALL KOJO_TALK,ARG,54
								RETURN 2
							ELSE
							;満足
								;口上呼び出し
								CALL KOJO_TALK,ARG,53
								RETURN 1
							ENDIF
						ELSE
							PRINTFORMW 「ひ、ひどい。」
							PRINTFORMW 「%CSTR:ARG:11%とは、始めから遊びだったのか。」
							LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:知恵+MAXBASE:PLAYER:運/2)
							;失敗
							IF LOCAL < 30
								PRINTFORMW 「こうなったら、
								PRINTFORMW 　%CSTR:ARG:12%を殺して、%CSTR:ARG:11%も死んでやる。」
								RETURN 3
							;成功
							ELSE
								PRINTFORMW 「じゃあ、ソノ気になるまで、一生付きまとってやる。」
								RETURN 1
							ENDIF
						ENDIF
					ELSE
					;いいえ
						PRINTFORMW %CALLNAME:PLAYER%は%CALLNAME:ARG%に身を委ねた…
