					PRINTFORMW 「実は%CSTR:ARG:11%も、ちょっと微妙だと思ってたんだ。」
					PRINTFORMW 「いやいや、まじで。」
					RETURN 0
				ENDIF
			ELSE
				GOTO INPUT_LOOP1
			ENDIF
		CASE 2
			PRINTFORMW 「Hmm, I'm quite bored.」
			;「それにしても、退屈だなあ。」
			PRINTFORMW 「暇過ぎて、死にたくなってくる。いやいや、まじで。」
			PRINTFORMW 「Hey, %CSTR:ARG:11% know it's troublesome, but will you entertain %CSTR:ARG:12%?」
			;「なあ、せっかくだから%CSTR:ARG:12%、%CSTR:ARG:11%を楽しませてくれないか。」
			PRINTFORMW 「Well, if you don't, you'll die rather quick.」
			;「ほら、早くしないと本当に暇で死んじまうぞ。」
			PRINTFORML
			PRINTFORML [1]Show some tricks [2]Try to make laugh
			;[1]芸を見せる　[2]笑わせてみる
			PRINTFORML [3]Draw weapons
			;[3]武器を抜く
			$INPUT_LOOP2
			INPUT
			PRINTFORML
			IF RESULT == 1
				;1.芸を見せる
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:魔力+MAXBASE:PLAYER:運/2)
				;失敗
				IF LOCAL < 30
					CFLAG:ARG:友好度 -= RAND:30
					IF RAND:2 == 0
						PRINTFORMW 「あ、これが噂に聞く前衛芸術って奴か。」
						PRINTFORMW 「%CSTR:ARG:11%には、何が良いのかサッパリだけどな。」
					ELSE
						PRINTFORMW 「なんだそりゃ。」
						PRINTFORMW 「これなら、三回回ってワンと鳴いてくれたほうが、ずっと良かったぞ。」
					ENDIF
					RETURN 6
				;成功
				ELSE
					CFLAG:ARG:友好度 += RAND:30
					PRINTFORMW 「へえ。」
					PRINTFORMW 「素人の割には、なかなかやるじゃないか。」
					PRINTFORMW 「まじで。」
					RETURN 0
				ENDIF
			ELSEIF RESULT == 2
				;2.笑わせてみる
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:知恵+MAXBASE:PLAYER:運/2)
				;失敗
				IF LOCAL < 30
					CFLAG:ARG:友好度 -= RAND:50
					PRINTFORMW ＞%CALLNAME:PLAYER%は気のきいたジョークで笑わせようとした
					PRINTFORMW ＞しかし、%CALLNAME:ARG%には受けなかったようだ……
					PRINTFORMW 「………つまんねえよ、ばーか。」
					RETURN 6
				;成功
				ELSE
					CFLAG:ARG:友好度 += RAND:30
					PRINTFORMW ＞%CALLNAME:PLAYER%は気のきいたジョークで笑わせようとした
					PRINTFORMW 「…っぷ、く、くくく…」
					PRINTFORMW 「く、くっそ、こっ、こんなので…っ、こんなので…ッ…」
					PRINTFORMW 「ははっ、あははははっっ！」
					PRINTFORMW ＞なんと、%CALLNAME:ARG%に受けてしまった！！
					RETURN 0
				ENDIF
			ELSEIF RESULT == 3
				;3.武器を抜く
				LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:知恵+MAXBASE:PLAYER:運/2)
				SIF TALENT:ARG:獰猛
					LOCAL -= RAND:50
				SIF TALENT:ARG:臆病
					LOCAL += RAND:20
				SIF TALENT:ARG:気丈
					LOCAL += RAND:30
				;失敗
				IF LOCAL < 30
					CFLAG:ARG:友好度 -= RAND:30
					PRINTFORMW 「そうか。%CSTR:ARG:12%、%CSTR:ARG:11%と一緒に死にたいのか。」
					PRINTFORMW 「嬉しいよ。じゃあ、%CSTR:ARG:12%から死んでいいよ。」
					RETURN 3
				;成功
				ELSE
					CFLAG:ARG:友好度 += RAND:20
					IF TALENT:ARG:臆病
						PRINTFORMW 「ば、ばか。」
						PRINTFORMW 「人が死ぬって言ったからって、まじで止めをさそうとする奴がいるか。」
						PRINTFORMW 「いいからその物騒なものをしまえ、危ねえだろ。」
					ELSE
						PRINTFORMW 「ばーか、今のは冗談だぞ。まさか、本気にしたのか？」
						PRINTFORMW 「いい奴なのか悪い奴なのか、わかんねえ奴だな、%CSTR:ARG:12%は。」
					ENDIF
					RETURN 0
				ENDIF
			ELSE
				GOTO INPUT_LOOP2
			ENDIF
	ENDSELECT
LOOP 1
