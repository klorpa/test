;パ−ティ内に発情中のキャラが居る場合に１０％の確率で発生
IF RAND:10 == 0
	FOR LOCAL, 1, 7
		;存在しないか死んでいるならスキップ
		SIF POS(LOCAL) < 0 || GET_STATE(CFLAG:(POS(LOCAL)):ステート) == "DYING"
		CONTINUE
		;発情以外は弾く
		SIF 危険日(POS(LOCAL)) <= 1
			CONTINUE
		発情キャラ番号:発情人数 = POS(LOCAL)
		発情人数 ++
	NEXT
	;一人も居ない場合はスキップ
	SIF 発情人数 == 0
		GOTO SKIP_発情キャラ要求
	;要求キャラの選定
	発情要求キャラ番号 = 発情キャラ番号:(RAND:発情人数)
	
