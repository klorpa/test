				FOR COUNT, 1, 7
					SIF POS(COUNT) < 0
						CONTINUE
					SIF TALENT:(POS(COUNT)):オトコ
						CONTINUE
					LOCAL:(LOCAL:7+1) = POS(COUNT)
					LOCAL:7 ++
				NEXT
				
				;ここで要求キャラを決定
				;男しかいない場合
				IF LOCAL:7 == 0
					LOCAL = 0
				;女がいる場合
				ELSE
					LOCAL = LOCAL:(RAND:(LOCAL:7)+1)
				ENDIF
			
				;男しか居ない
				IF LOCAL == 0
					PRINTFORMW 「チッ、ヤローしか居ねぇのかよ」
					RETURN 0
				;会話をしているキャラを要求
				ELSEIF LOCAL == PLAYER
					PRINTFORMW 「イイな身体してんのな、おまえ」
					PRINTFORMW 「なぁ、一発ヤラせろや」
					PRINTFORML 
					PRINTFORML [1]%CALLNAME:ARG%に抱かれる　　　　　[2]断る
				;他の女を要求
				ELSE
					PRINTFORMW 「なぁ、ソコのオンナ、%CSTR:ARG:一人称%にヤラせろよ」
					PRINTFORMW %CALLNAME:ARG%は%CALLNAME:LOCAL%を指差した
					PRINTFORML 
					PRINTFORML 「なっ、いいだろ？」
					PRINTFORML 
					PRINTFORML [1]%CALLNAME:LOCAL%を差し出す　　　　　[2]断る
				ENDIF
				$INPUT_LOOP_A4
				INPUT
				PRINTFORML

				IF RESULT == 1
						;女を差し出す
						TFLAG:捧げたキャラ = LOCAL
						;異性をあてがった反応の口上呼び出し
						CALL KOJO_TALK,ARG,41
						CALL SEXIAL_NEGOCIATE,ARG,LOCAL
						PRINTW
				
						TFLAG:TALK用成功判定値 = FLAG:悪魔満足度 + CFLAG:ARG:友好度
						CFLAG:ARG:友好度 += FLAG:悪魔満足度
						;敗北してる場合ここでゲームオーバー
						IF FLAG:敗北フラグ
							;口上呼び出し
							CALL KOJO_TALK,ARG,42
							CALL SEXTALK_LOSE,ARG
						ENDIF
				
						;話しかけたキャラが倒れてたら逃げられる
						CALL INPUTABLE_CHARA,PLAYER
						IF RESULT == 0
							CALL KOJO_TALK,ARG,57
							PRINTFORML 
							PRINTFORMW 「そんじゃヤルことヤッたし、%CSTR:ARG:一人称%は帰るぜ」
							PRINTFORML 
							RETURN 5
						ENDIF
					
						IF TFLAG:TALK用成功判定値 < 30
						;払ったのに逃げられる
							;口上呼び出し
							CALL KOJO_TALK,ARG,44
							RETURN 5
						ELSEIF TFLAG:TALK用成功判定値 < 80
						;満足し、アイテムをくれる
						;口上呼び出し
								CALL KOJO_TALK,ARG,54
							RETURN 2
						ELSE
						;満足
							;口上呼び出し
							CALL KOJO_TALK,ARG,46
							RETURN 1
						ENDIF

				ELSEIF RESULT == 2
					;2.断る
					;交渉キャラが相手悪魔よりレベルが高い場合は『70 + RAND:20』％の確立で交渉続行
					;低い場合は『50 - (MAXBASE:ARG:ＬＶ - MAXBASE:PLAYER:ＬＶ) * 2』％にまで交渉継続確率が下がる
					;最低値は10％
					IF BASE:PLAYER:LV > BASE:ARG:LV
						LOCAL = 70 + RAND:20
					ELSE
						LOCAL = RAND:50
					ENDIF
					SIF LOCAL < 10
						LOCAL = 10
						
					IF RAND:100 < LOCAL
						CFLAG:ARG:友好度 += RAND:10
						;成功
						PRINTFORML 「ハハハッ、冗談だって」
						PRINTFORMW 「マジにすんなよな」
						RETURN 0
					ELSE
						CFLAG:ARG:友好度 -= RAND:50
						;失敗
						;LOCALが40以下だと奇襲、そうでない場合は敵対
						IF LOCAL < 40
							PRINTFORML 
							PRINTFORML 「ハァッ？ナニ断ってんだよ」
							PRINTFORMW 「んじゃ、殺してから犯すわ」
							PRINTFORML 
							RETURN 3
						ELSE
							PRINTFORML 
							PRINTFORMW 「へー、そんじゃ話はこれで終わりだ」
							PRINTFORML 
							RETURN 4
						ENDIF
					ENDIF
				ELSE
					GOTO INPUT_LOOP_A4
				ENDIF
			ELSE
				PRINTFORML 「Hey, did you know?」
				;「なあおい、知ってるか？」
				PRINTFORML 「There's a recent rumor.」
				;「最近の噂なんだがな」
				PRINTFORMW 「Did you hear about the awesome Devil Summoner that is active in many places?」
				;「凄腕のデビルサマナーがあちこちで活躍しているって話、聞いたことあるか？」
				PRINTFORML
				PRINTFORML [1]I know　　[2]I don't know
				;[1]知っている　　[2]知らない
				PRINTFORML [3]I am that one
				;[3]自分がそうだ
				$INPUT_LOOP_A5
				INPUT
				PRINTFORML
				IF RESULT == 1
					;1.知ってる
					LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:知恵+MAXBASE:PLAYER:運/2)
					SIF TALENT:ARG:好奇心
						LOCAL += RAND:15
					SIF TALENT:ARG:目立ちたがり
						LOCAL -= RAND:15
					SIF TALENT:ARG:生意気
						LOCAL -= RAND:15
					IF (CFLAG:ARG:友好度 + LOCAL) < 50
					;失敗
						IF TALENT:ARG:目立ちたがり
							CFLAG:ARG:友好度 -= RAND:10
							PRINTFORMW 「おいおい、そこは知らねーって言う所だろう？」
							PRINTFORMW 「%CSTR:ARG:一人称%にもっとしゃべらせてくれてもいいじゃねえか」
							RETURN 6
						ELSEIF TALENT:ARG:生意気
							CFLAG:ARG:友好度 -= RAND:15
							PRINTFORMW 「人間のくせに%CSTR:ARG:一人称%の知らない話を知っているってのか」
							PRINTFORMW 「なんかムカツク　…死にやがれ！」
							RETURN 3
						ELSE
							CFLAG:ARG:友好度 -= RAND:10
							PRINTFORMW 「…%CSTR:ARG:12%、本当に分かって言ってんのか？」
							RETURN 6
						ENDIF
					;成功
					ELSE
					CFLAG:ARG:友好度 += RAND:10
						IF TALENT:ARG:好奇心
							PRINTFORMW 「ほほーっ？　お前知っていたのか」
							PRINTFORMW 「もっとあの話の詳細聞かせてくれよ」
							CFLAG:ARG:友好度 += RAND:10
						ELSEIF TALENT:ARG:恥じらい
							PRINTFORMW 「あー、知ってるんならいいんだ、うん」
						ELSE
							PRINTFORML 「そうそう、アレのことだよアレ。」
							PRINTFORMW 「アレはそんなことになっているんだってな！」
						ENDIF
					RETURN 0
					ENDIF
				ELSEIF RESULT == 2
					;2.知らない
					LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:運)
					SIF TALENT:ARG:目立ちたがり
						LOCAL += RAND:15
					SIF TALENT:ARG:生意気
						LOCAL -= RAND:15
					SIF TALENT:ARG:ツンデレ
						LOCAL += RAND:15
					IF (CFLAG:ARG:友好度 + LOCAL) < 50
					;失敗
						IF TALENT:ARG:プライド高い
							CFLAG:ARG:友好度 -= RAND:10
							PRINTFORMW 「おいおい、そんなことも知らねえのか？」
							PRINTFORMW 「気が変わった。%CSTR:ARG:一人称%の情報を人間ごときにやるわけにはいかねえ」
							RETURN 6
						ELSEIF TALENT:ARG:生意気
							CFLAG:ARG:友好度 -= RAND:15
							PRINTFORML 「知りたいかい？　知りたいだろう？」
							PRINTFORMW 「でも教えてやんねー！」
							RETURN 0
						ELSE
							CFLAG:ARG:友好度 -= RAND:10
							PRINTFORMW 「おいおい%CSTR:ARG:12%、そんなことも知らないで大丈夫なのか？」
							RETURN 0
						ENDIF
					;成功
					ELSE
					CFLAG:ARG:友好度 += RAND:15
					;かなりの力技。独立変数にしたい
					PRINTFORM 「なんでもそいつはよ、
						IF BASE:MASTER:LV >= 50 && RAND:1
							PRINTFORMW 悪魔にかかわる人間なら知らないやつはいねーって話だぜ」
						ELSEIF ダンジョンフラグ:64:0 >= 4
							PRINTFORMW 魔界の大将アゼルをぶっ倒して、魔界征服してしまったんだとかよ」
						ELSEIF ダンジョンフラグ:25:0
							PRINTFORML どこぞの古墳から、女神様をかっぱらったらしいぜ」
							PRINTFORMW 「男としてリスペクトするぜ、そういうのよ。」
						ELSEIF ダンジョンフラグ:10:8
							PRINTFORMW セベクだかチャベクだかいう人間の組織のケツを蹴り上げたんだとか」
						ELSEIF ダンジョンフラグ:3:1
							PRINTFORMW 病院ひとつ凍らせた悪魔を叩き潰したとかよ」
						ELSEIF ダンジョンフラグ:2:11 
							PRINTFORMW 魔界を一つ作れるデビルサマナーにケンカ売ったらしいぜ」
						ELSEIF ダンジョンフラグ:1:11
							PRINTFORMW エコービルを占領していた陰陽師をぶっ潰したというぜ」
						ELSE
							PRINTFORML まだあんまり知られてないがよ、きっとすげえサマナーになんぜ」
							PRINTFORMW 「そうじゃねぇと、%CSTR:ARG:一人称%の賭けた晩飯代が帰ってこねぇ」
						ENDIF
					RETURN 0
					ENDIF
				ELSEIF RESULT == 3
					;3.自分がそうだ
					LOCAL = RAND:100 + RAND:(MAXBASE:PLAYER:知恵+MAXBASE:PLAYER:運/2)
					SIF TALENT:ARG:愚鈍
						LOCAL += RAND:15
					;MASTERのレベルが低い
					SIF BASE:ARG:LV > BASE:PLAYER:LV
						LOCAL -= 40
					IF LOCAL < 60
						CFLAG:ARG:友好度 -= RAND:10
						;失敗
						IF BASE:ARG:LV > BASE:PLAYER:LV
							CFLAG:ARG:友好度 -= RAND:50
							PRINTFORMW 「はぁ？　何言ってんだ%CSTR:ARG:12%」
							PRINTFORMW 「%CSTR:ARG:一人称%より弱いくせに調子こいてんじゃねえぞ！」
							RETURN 6
						ELSEIF TALENT:ARG:獰猛 || TALENT:ARG:残虐
							PRINTFORML 「ああ！？　%CSTR:ARG:12%がそうだったのか！」
							PRINTFORMW 「%CSTR:ARG:12%はよォ、　悪魔の間じゃ手配度５で死刑が確定してるんだよ！」
							RETURN 3
						ELSEIF TALENT:ARG:プライド高い
							PRINTFORML 「%CSTR:ARG:12%がそうなのか！」
							PRINTFORMW 「大将首だ、逃がしはしねえ！」
							RETURN 3
						ELSE
							PRINTFORMW 「おいおい%CSTR:ARG:12%、頭大丈夫か？」
							RETURN 6
						ENDIF
					ELSE
						CFLAG:ARG:友好度 += RAND:30
						;成功
						IF BASE:ARG:LV > BASE:PLAYER:LV
							CFLAG:ARG:友好度 -= RAND:20
							PRINTFORMW 「%CSTR:ARG:12%がそうだって言うのか？」
							PRINTFORMW 「とてもそうは見えねえが…そうなのか」
							RETURN 0
						ELSEIF TALENT:ARG:獰猛
							PRINTFORML 「道理で強そうだと思ったぜ」
							PRINTFORMW 「いちど手合せしてほしいもんだな、おい」
							RETURN 0
						ELSEIF TALENT:ARG:狡猾
							PRINTFORML 「%CSTR:ARG:12%がそうだってえこたぁ…儲かっているんだろ？」
							PRINTFORMW 「話を聞いてやろうか？へっへっへ…」
							CFLAG:ARG:友好度 += RAND:10
							RETURN 0
						ELSEIF TALENT:ARG:臆病
							CFLAG:ARG:友好度 += RAND:20
							PRINTFORML 「%CSTR:ARG:12%がそうだってのか、」
							PRINTFORMW 「そうとは気づかないままの数々の無礼　許してくれ」
							RETURN 0
						ELSEIF TALENT:ARG:友愛
							CFLAG:ARG:友好度 += RAND:15
							PRINTFORML 「人間の身で悪魔と張り合うなんてすげえな」
							PRINTFORMW 「そういう無茶やっているとケガも多いだろう、大丈夫か？」
							RETURN 0
						ELSE
							PRINTFORML 「%CSTR:ARG:12%がそうだったのか」
							PRINTFORMW 「悪魔ってのはな、強いやつが大好きなのさ」
							RETURN 0
						ENDIF
					ENDIF
				ELSE
					GOTO INPUT_LOOP_A5
				ENDIF
			ENDIF
	ENDSELECT

LOOP 1

@FUSION_MESSAGE_PUB316,ARG
;合体時口上
PRINTFORMW 



;==========================================================
;@AUTO_PUB316
; 会話を仲魔に任せているときの口上
;<引数>
;	ARG    = 相手悪魔
;	PLAYER = 会話担当キャラ
;<戻り値>
;	0以上.会話の成功率　-1.口上がない場合に使う、例えば女相手の口上しかない場合は『RETURN -1』を置く事で簡易式に飛ばす事が出来る
;==========================================================
@AUTO_PUB316,ARG
#DIM 会話成功率

会話成功率 = 50
FOR LOCAL, 0, 2
	;会話担当がオトコ
	IF TALENT:PLAYER:オトコ
		SELECTCASE RAND:2
			CASE 0
	;			PRINTFORML 
				RETURN -1
				
			CASE 1
	;			PRINTFORML 
				RETURN -1
				
		ENDSELECT

	;会話担当が女
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「オンナに聞くことといえばバストのサイズだよな」
				PRINTFORMW 「どのくらいの大きさよ？」
				PRINTFORML 
				;きつめの性格だと答えない、それ以外は答える
				;恥じらいがあるとさらにアップ
				;プライド高い・生意気・反抗的・ツンデレ
				IF TALENT:PLAYER:プライド高い || TALENT:PLAYER:生意気 || TALENT:PLAYER:反抗的 || TALENT:PLAYER:ツンデレ
					PRINTFORML %CALLNAME:PLAYER%は激しく拒否した
					PRINTFORML 
					PRINTFORMW 「はぁ？ケチってんじゃねーよ、あばずれが」
					PRINTFORML 
					PRINTFORMW 印象が悪くなったようだ
					会話成功率 -= 10

				;恥じらい
				ELSEIF TALENT:PLAYER:恥じらい
					PRINTFORML %CALLNAME:PLAYER%は恥じらいながら答えた
					PRINTFORML 
					PRINTFORMW 「フヒッ！なに？恥ずかしいわけ？いいねいいね〜」
					PRINTFORML 
					PRINTFORMW 印象がかなり良くなったようだ
					会話成功率 += 30
				
				
				;それ以外
				ELSE
					PRINTFORML %CALLNAME:PLAYER%は答えた
					PRINTFORML 
					PRINTFORMW 「ふ〜ん、そうなのか。ヒヒッ」
					PRINTFORML 
					PRINTFORMW 印象が良くなったようだ
					会話成功率 += 10
				
				ENDIF
				
				
				
			CASE 1
				PRINTFORML 「あー、なんかムラムラするなー」
				PRINTFORMW 「なぁ、ちょっと咥えてくんないかな。そのかわいいお口でよ？」
				PRINTFORML 
				PRINTFORMW 「なぁに、ちょっと舐めるだけでいいからさ、な？」
				PRINTFORML 
				;淫ＮＴＲ・発情中の場合は無条件で咥える
				IF TALENT:PLAYER:ＮＴＲ == 2 || CFLAG:PLAYER:ダンジョン内発情用欲情値 > 5000 || 危険日(PLAYER) == 2 
					PRINTFORML %CALLNAME:PLAYER%は躊躇する事もなく、突き出された%CALLNAME:ARG%のペニスを口で咥えた……
					PRINTFORML 
					PRINTFORMW 「ひひッ、そんな嬉しそうなツラで咥えるとか、最初からソノつもりだったんじゃねーのか？」
					PRINTFORML 
					PRINTFORMW 印象がかなり良くなったようだ
					会話成功率 += 40

				;浮気癖の場合は３０％
				ELSEIF TALENT:PLAYER:浮気癖 && RAND:100 < 30
					PRINTFORML %CALLNAME:PLAYER%はやや躊躇する素振りをみせたが、
					PRINTFORML %CALLNAME:ARG%のペニスを咥えると蕩けた表情で奉仕を始めた……
					PRINTFORML 
					PRINTFORMW 「最初の嫌そうなのは演技だったのか？」
					PRINTFORMW 「随分とノリがいいじゃねーかよ」
					PRINTFORML 
					PRINTFORMW 印象がかなり良くなったようだ
					会話成功率 += 40
				
				;臆病だと低確率で無理矢理咥えさせられる
				ELSEIF TALENT:PLAYER:臆病 && RAND:100 < 1
					PRINTFORML %CALLNAME:PLAYER%が突き出されたイチモツに怯えていると、
					PRINTFORML %CALLNAME:ARG%が頭を掴んで無理矢理咥えさせた……
					PRINTFORML 
					PRINTFORMW 「グズグズしてんじゃねーよ！」
					PRINTFORMW 「さっさと咥えてご機嫌取れや」
					PRINTFORML 
					PRINTFORMW 印象がかなり良くなったようだ
					会話成功率 += 30
				
				
				;それ以外は断る
				ELSE
					;1/3の確率で印象が悪く、それ以外は変わらず
					IF RAND:3 ==0
						PRINTFORML %CALLNAME:PLAYER%は拒否した
						PRINTFORML 
						PRINTFORMW 「はぁ？そうかよ、気分悪いわー」
						PRINTFORML 
						PRINTFORMW 印象が悪くなったようだ
						会話成功率 -= 10
					ELSE
						PRINTFORML %CALLNAME:PLAYER%は拒否した
						PRINTFORML 
						PRINTFORMW 「ははッ、ジョーダンだよジョーダン！」
						PRINTFORMW 「真に受けてんなよ、ウケルゼ」
						PRINTFORML 
						PRINTFORMW 特に印象に変化はないようだ
						
					ENDIF
				
				ENDIF
				
		ENDSELECT

	ENDIF
	PRINTL 
	PRINTL 

NEXT

RETURN 会話成功率


