@KOJO_MESSAGE_ASSIPAIR_COM_PUB303,ARG

SIF ASSIPLAY == 1
	RETURN 0
SELECTCASE SELECTCOM

;-------------------------------------------------
;122,助手を犯させる
;-------------------------------------------------
	CASE 122
		SELECTCASE ARG
			;気持ちよくて感じている
			CASE 1
				PRINTFORMW 『あん、あん…とても気持ちいいよ%UNICODE(0x2661)%』
				RETURN 1
			;こっちに来てと言うセリフ
			CASE 2
				PRINTFORMW 『さあ、こっちに来て！』
				PRINTFORMW 『あなたのこと、受け止めてあ・げ・る%UNICODE(0x2661)%』
				RETURN 1
			;大丈夫というセリフ
			CASE 3
				IF 0 == 1
					PRINTFORMW 『遠慮しなくていいのよ！』
				ELSE	
					PRINTFORMW 『大丈夫！』
					PRINTFORMW 『おねーさんがリードしてあげる！』
				ENDIF	
				RETURN 1
			;犯されて強がりを言うセリフ
			;CASE 4
			;	PRINTFORMW 『』
			;	RETURN 1
			;うまくなってきたねセリフ
			;CASE 5
			;	PRINTFORMW 『』
			;	RETURN 1
		ENDSELECT
		
;-------------------------------------------------
;124,ダブルフェラ
;-------------------------------------------------
	CASE 124
			SELECTCASE ARG
				;一緒に気持ちよくしようというセリフ
				CASE 1
					PRINTFORMW 『さあ、一緒に気持ちよくしよ！』
					RETURN 1
				;私のほうが気持ちよくしてるというセリフ
				CASE 2
					PRINTFORMW 『%CSTR:ASSI:一人称%のほうがうまいもん！』
					PRINTFORMW 『ね、%CSTR:ASSI:二人称%%UNICODE(0x2661)%』
					RETURN 1
				;煽りに対してのセリフ
				CASE 3
					PRINTFORMW 『そんなことないもん！%CSTR:ASSI:一人称%だって負けないんだから！』
					RETURN 1
				;いつもより感じてるねというセリフ
				CASE 4
					PRINTFORMW 『%CSTR:ASSI:二人称%のおちんちん、いつもよりビクビクしてるね』
					RETURN 1
				;レズっぽいセリフに対するセリフ
				CASE 5
					IF ABL:ASSI:レズっ気 >= 3
						PRINTFORMW 『えへへ、%CALLNAME%と一緒にするの嬉しいかも』
						RETURN 1
					ENDIF
					PRINTFORMW 『%CSTR:ASSI:一人称%とするのがそんなにうれしいの？』
					RETURN 1
				;二人のフェラは気持ちいいでしょ？セリフ
				CASE 6
					PRINTFORMW 『二人分のフェラは気持ちいいでしょ？%CSTR:ASSI:二人称%』
					RETURN 1
			ENDSELECT

;-------------------------------------------------
;125,ダブルパイズリ
;-------------------------------------------------
	CASE 125
		SELECTCASE ARG
			;一緒に気持ちよくしようというセリフ
			CASE 1
				PRINTFORMW 『さあ、二人で一緒にパイズリしよ！』
				RETURN 1
			;私のほうが気持ちよくしてるというセリフ
			CASE 2
				PRINTFORMW 『%CALLNAME%より%CSTR:ASSI:一人称%のおっぱいのほうが気持ちいいよね。%CSTR:ASSI:二人称%%UNICODE(0x2661)%』
				RETURN 1
			;煽りに対してのセリフ
			CASE 3
				PRINTFORMW 『言ったな〜！絶〜っ対に負けないんだから！』
				RETURN 1
			;いつもより感じてるねというセリフ
			;CASE 4
				PRINTFORMW 『%CSTR:ASSI:二人称%、いつもより感じているね』
				RETURN 1
			;お互いに乳首がこすれて気持ちいいセリフ
			CASE 5
				PRINTFORMW 『ん%UNICODE(0x2661)%乳首擦れて気持ちいいよぉ…』
				RETURN 1
			;二人のパイズリで気持ちよくなってセリフ
			CASE 6
				PRINTFORMW 『%CSTR:ASSI:一人称%たちのパイズリで気持ちよくなってね！』
				RETURN 1
			;パイズリで感じているの？セリフ
			CASE 7
				PRINTFORMW 『パイズリで感じてるの？』
				RETURN 1
		ENDSELECT
