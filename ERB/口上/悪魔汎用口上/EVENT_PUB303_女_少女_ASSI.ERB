				RETURN 1
			;私たち、とっても仲がいいでしょ？セリフ
			CASE 2
				PRINTFORMW 『%CSTR:ASSI:一人称%と%CSTR:ASSI:二人称%はとっても仲良しなの%UNICODE(0x2661)%』
				PRINTFORMW 『ね、%CSTR:ASSI:二人称%%UNICODE(0x2661)%』
				RETURN 1
			;うらやましいでしょ？セリフ
			CASE 3
				PRINTFORMW 『えへへ〜、とても羨ましいでしょ〜』
				RETURN 1
			;こっちが気になってるみたいだねセリフ
			CASE 4
				PRINTFORMW 『ねえねえ、%CALLNAME%こっちが気になってるみたいだね』
				RETURN 1
		ENDSELECT

