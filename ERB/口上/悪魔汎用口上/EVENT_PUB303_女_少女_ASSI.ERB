				RETURN 1
			;これはあたしのものよ！セリフ
			CASE 2
				PRINTFORMW 『これは%CSTR:ASSI:一人称%の物なんだからね！』
				PRINTFORMW 『%CALLNAME%なんかにあげないんだから！んじゅう…ちゅる』
				RETURN 1
			;うらやましいでしょ？セリフ
			CASE 3
				PRINTFORMW 『羨ましいでしょ？』
				RETURN 1
			;先にいただくわセリフ（仲がいい）
			CASE 4
				PRINTFORMW 『%CSTR:ASSI:一人称%が先に精液貰うよ』
				RETURN 1
		ENDSELECT
