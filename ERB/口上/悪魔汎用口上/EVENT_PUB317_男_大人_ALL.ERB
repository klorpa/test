;-------------------------------------------------
;苦痛刻印Lv3
;-------------------------------------------------
IF TFLAG:22 == 3
	CALL MARK_MESSAGE_A
	;陥落済み
	IF TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8
		;恋慕、親愛
		IF TALENT:3 || TALENT:6
			PRINTFORMW ………
			PRINTFORMW ……
			PRINTFORMW …
			PRINTFORMW 「……これで、満足か」
			PRINTFORMW マズい
			PRINTFORMW %CALLNAME:MASTER%の仕打ちに%CALLNAME:TARGET%は怒りと悲しみを顕にしていた
			PRINTFORMW まるで心を許した自分が馬鹿だったとでも言うように%CALLNAME:MASTER%から顔をそらす%CALLNAME:TARGET%
			PRINTFORMW ……ちょっと調子にのって、つい、うっかり、やり過ぎた
			PRINTFORMW しまったどうしよう
			PRINTFORMW 　
			PRINTFORMW %CALLNAME:TARGET%の機嫌を完全に損ねてしまった
			PRINTFORMW 従順Lvが下がった
			PRINTFORMW 態度が反抗的になった
			;[男嫌い][女嫌い]の取得
			IF TALENT:MASTER:オトコ == 1 || TALENT:MASTER:男の娘 == 1
				PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%に対して怒りを抱いている……
				TALENT:82 = 1
			ELSE
				PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%に対して怒りを抱いている……
				TALENT:88 = 1
			ENDIF
			;恭順の球が0、否定の球が2万5千、従順をLv0、反発刻印Lv3、[反抗的]の取得
			JUEL:4 = 0
			JUEL:100 = 25000
			ABL:0 = 0
			MARK:3 = 3
			TALENT:11 = 1
			PRINTFORMW 後で何とかしてを機嫌をとらないと許してくれないだろう……
		;淫乱、娼婦
		ELSEIF TALENT:4 || TALENT:7
;			PRINTFORMW 
		;服従、隷属
		ELSEIF TALENT:5 || TALENT:8
;			PRINTFORMW 
		ENDIF
	ELSE
;		PRINTFORMW 
	ENDIF
ENDIF

;-------------------------------------------------
