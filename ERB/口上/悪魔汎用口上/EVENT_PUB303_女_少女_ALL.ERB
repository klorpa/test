@BATTLE_MESSAGE_PUB303,ARG,ARG:1 = 0
IF CFLAG:ARG:入力行動 == 2022 && NO:ARG == 407
;超メギド使用時
	;CALLFORM SKILL_NAME_{CFLAG:ARG:入力行動},ARG
	PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
	PRINTFORML ┃%"%CSTR:一人称%がイかせてア・ゲ・ル" + UNICODE(0x2661),72,LEFT%┃
	PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛

ELSEIF ARG:1 > 0
	;忠誠度が上昇する行動をとった場合に表示する口上

	CALLFORM SKILL_NAME_{CFLAG:ARG:入力行動},ARG
	;使用したスキルの名前をLOCALSに代入
	LOCALS = %RESULTS%

	SELECTCASE ARG:1
		;物理ダメージを与える行動で、獰猛持ちの忠誠度が上昇
		CASE GETNUM(TALENT,"獰猛")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！ いっけぇ！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;魔法ダメージを与える行動で、狡猾持ちの忠誠度が上昇
		CASE GETNUM(TALENT,"狡猾")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！ やっちゃえ！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;高威力の行動で、残虐持ちの忠誠度が上昇
		CASE GETNUM(TALENT,"残虐")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！ 派手にトんじゃえ♪",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;相手の弱点を付いた攻撃or行動不能な状態異常の敵に対する攻撃で、賢い持ちの忠誠度が上昇
		CASE GETNUM(TALENT,"賢い")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！ さっすが" + CSTR:一人称 + "♪",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;破魔相性の行動で、天使持ちの忠誠度が上昇
		CASE GETNUM(TALENT,"天使")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！ " + "てんばつてきめんだよっ！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;呪殺相性の行動で、堕天使持ちの忠誠度が上昇
		CASE GETNUM(TALENT,"堕天使")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "ってタンビな感じでゾクゾクしちゃう" + UNICODE(0x2661),72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;回復・状態回復行動では友愛持ちの忠誠度が上昇
		CASE GETNUM(TALENT,"友愛")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！ 愛を感じるよっ！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;状態変化では高慢持ちの忠誠度が上昇
		CASE GETNUM(TALENT,"高慢")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%"こんなヤツ、" + LOCALS + "で十分だよねっ！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;特殊では冷静持ちの忠誠度が上昇
		CASE GETNUM(TALENT,"冷静")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "だね？ うん、任せて！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
;		;愚鈍持ちは主人とのLv差だけ忠誠度が上昇
		CASE GETNUM(TALENT,"愚鈍")
			PRINTFORML ┏＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "？ " + CSTR:二人称 + "の言うことなら間違いないよねっ",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
;		;虚心持ちは最低２だけ忠誠度が上昇
		CASE GETNUM(TALENT,"虚心")
			PRINTFORML ┏＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "Yes、understood？",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
	ENDSELECT
;ELSE
;	;システムによらず忠誠度を上げたいとか他があったらここに記載
ENDIF
