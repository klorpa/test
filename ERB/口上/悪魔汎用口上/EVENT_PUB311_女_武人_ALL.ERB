DRAWLINE
;初調教時
IF CFLAG:201 == 0 && TALENT:恋慕 == 0
	PRINTFORMW 「%CSTR:一人称%を調教だと」
	PRINTFORMW 「…面白い、やってみせるがいい｣
	CFLAG:201 = 1
	RETURN 0
;屈服刻印Lv1
ELSEIF CFLAG:201 < 2 && MARK:2 == 1
	PRINTFORMW 「この程度で%CSTR:一人称%を屈服させた気でいるのか？」
	PRINTFORMW 「まだ%CSTR:一人称%はなんともないぞ｣
	CFLAG:201 = 2
	RETURN 0
;屈服刻印Lv2
ELSEIF CFLAG:201 < 3 && MARK:2 == 2
	PRINTFORMW 「そろそろ諦めたらどうだ｣
	PRINTFORMW 「これ以上は時間の無駄だろう｣
	CFLAG:201 = 3
	RETURN 0
;屈服刻印Lv3
ELSEIF CFLAG:201 < 4 && MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0)
	PRINTFORMW 「また、来たのか？｣
	PRINTFORMW 「別に、待っていたわけじゃ｣
	CFLAG:201 = 4
	RETURN 0
;陥落
ELSEIF CFLAG:201 < 5 && (TALENT:3 || TALENT:4 || TALENT:5)
	;恋慕
	IF TALENT:3
		IF CFLAG:未調教陥落 == 1
