	PRINTFORMW 「そんなに私の身体はいいのか？」
	CFLAG:202 = 4
;陥落済み
ELSEIF CFLAG:202 < 5 || FLAG:7 == 2
	;恋慕、親愛
	IF TALENT:3 || TALENT:6
		;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 「ふふっ♪、来てくれたのか」
			PRINTFORMW 「しばらくの間は%CSTR:一人称%が%CSTR:二人称%を独り占めできるな」
		ELSEIF K:1 == 1
			PRINTFORMW 「ふふっ♪、来てくれたのか」
			PRINTFORMW 「しばらくの間は%CSTR:一人称%が%CSTR:二人称%を独り占めできるな」
		ELSE
			PRINTFORMW 「ふふっ♪、来てくれたのか」
			PRINTFORMW 「しばらくの間は%CSTR:一人称%が%CSTR:二人称%を独り占めできるな」
		ENDIF
	;淫乱、娼婦
	ELSEIF TALENT:4 || TALENT:7
		;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 「早くきてくれ」
			PRINTFORMW 「もうこっちの準備はできてるんだ」
		ELSEIF K:1 == 1
			PRINTFORMW 「早くきてくれ」
			PRINTFORMW 「もうこっちの準備はできてるんだ」
		ELSE
			PRINTFORMW 「早くきてくれ」
			PRINTFORMW 「もうこっちの準備はできてるんだ」
		ENDIF
	;服従、隷属
	ELSEIF TALENT:5 || TALENT:8
		;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	CFLAG:202 = 5
ENDIF
RETURN 0

