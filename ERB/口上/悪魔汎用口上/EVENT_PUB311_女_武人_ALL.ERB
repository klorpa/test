			;義理親子初めて
			PRINTFORMW 「その子は%CALLNAME:ASSI%」
			PRINTFORMW 「%CSTR:一人称%に性教育をさせたいのか」
			CFLAG:203 |= 2
		ENDIF
	ELSE
		CALL EVENTTRAIN_SECOND_PUB311
	ENDIF
ENDIF

;-------------------------------------------------
;調教開始時（2回目以降、X1をキャラ番号に変更する）
;-------------------------------------------------
