DRAWLINE
;反発刻印Lv3
IF MARK:3 == 3 && (CFLAG:217 < 1 || FLAG:7 == 2)
	PRINTFORMW 「この恨み、忘れないぞ」
	CFLAG:217 = 1
;屈服刻印Lv1以下
ELSEIF MARK:2 <= 1 && (CFLAG:219 < 1 || FLAG:7 == 2)
	PRINTFORMW 「もう終わりなのか？」
	CFLAG:219 = 1
;屈服刻印Lv2
ELSEIF MARK:2 == 2
	PRINTFORMW 「いつまで続ければ気が済むんだ」
	CFLAG:219 = 2
;屈服刻印Lv3＋陥落素質無し
ELSEIF MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0)
	PRINTFORMW 「また、呼んでくれると嬉しい」
	CFLAG:219 = 3
;陥落済み＋体力500未満
ELSEIF BASE:0 < 500 && (CFLAG:218 < 1 || FLAG:7 == 2)
	;恋慕、親愛
	IF TALENT:3 || TALENT:6
		PRINTFORMW 「全く、%CSTR:一人称%ももう疲れたぞ」
	;淫乱、娼婦
	ELSEIF TALENT:4 || TALENT:7
		PRINTFORMW 「もっと、もっと欲しい」
		PRINTFORMW 「これじゃ足りない」
	;服従、隷属
	ELSEIF TALENT:5 || TALENT:8
		PRINTFORMW 
	ENDIF
	CFLAG:218 = 1
;陥落済み
ELSEIF CFLAG:219 < 4 || FLAG:7 == 2
	;恋慕、親愛
	IF TALENT:3 || TALENT:6
		PRINTFORMW 「もう終わりなのか…」
		PRINTFORMW 「次はいつ来てくれるんだ」
	;淫乱、娼婦
	ELSEIF TALENT:4 || TALENT:7
		PRINTFORMW 「%CSTR:一人称%はまだ満足してないぞ」
		PRINTFORMW 「%CSTR:一人称%をこんなにしたのは%CSTR:二人称%だろう」
	;服従、隷属
	ELSEIF TALENT:5 || TALENT:8
		PRINTFORMW 
	ENDIF
	CFLAG:219 = 4
ENDIF
RETURN 0

