			PRINTFORML ┃%"Fufufu、" + LOCALS + "ですわ",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;愚鈍持ちは主人とのLv差だけ忠誠度が上昇
		CASE GETNUM(TALENT,"愚鈍")
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！　イきなさいッ！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;虚心持ちは最低２だけ忠誠度が上昇
		CASE GETNUM(TALENT,"虚心")
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
	ENDSELECT

