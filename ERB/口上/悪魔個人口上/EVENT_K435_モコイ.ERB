		;-------------------------------------------------
	CASE 85
		CALL EVENT_MESSAGE
		IF (CFLAG:608 < 1 || FLAG:7 == 2)
			PRINTFORMW
			CFLAG:608 = 1
		ENDIF
	CASEELSE
		SIF FLAG:6 > 0
			CALL EVENT_MESSAGE
ENDSELECT



