			CFLAG:603 = 1
		ENDIF
		;-------------------------------------------------
		;;ＮＴＲ夜這い・野外プレイ
		;;愛ＮＴＲか隷ＮＴＲ状態(TALENT:ＮＴＲが 1 か 3 か 4 か 6)で１日の終了時に発生。
		;愛ＮＴＲの場合、露出癖が4以上でないと発生しません
		;-------------------------------------------------
	CASE 61
		CALL EVENT_MESSAGE
		IF (CFLAG:604 < 1 || FLAG:7 == 2)
			IF TALENT:ＮＴＲ == 1 || TALENT:ＮＴＲ == 4
				;愛ＮＴＲ
				PRINTFORMW 
			ELSEIF TALENT:ＮＴＲ == 3 || TALENT:ＮＴＲ == 6
				;隷ＮＴＲ
				PRINTFORMW 
			ENDIF
			CFLAG:604 = 1
		ENDIF
		;-------------------------------------------------
		;;ＮＴＲ夜這い・野外調教
		;;隷ＮＴＲ状態(TALENT:ＮＴＲが 3 か 6)でCFLAG:現ＮＴＲ進行度が6以上の場合１日の終了時に発生。
		;-------------------------------------------------
	CASE 62
		CALL EVENT_MESSAGE
		IF (CFLAG:605 < 1 || FLAG:7 == 2)
			;隷ＮＴＲ
			PRINTFORMW 
			CFLAG:605 = 1
		ENDIF
	CASEELSE
		SIF FLAG:6 > 0
			CALL EVENT_MESSAGE
ENDSELECT
