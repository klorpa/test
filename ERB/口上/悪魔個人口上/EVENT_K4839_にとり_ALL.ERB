@EVENTSHOP
;にとりの場所を探す
A = -1
REPEAT CHARANUM
	SIF NO:COUNT == 4839
		A = COUNT
REND
IF A > -1
	IF A != TARGET
		;日数カウント これが１４以上（一週間分以上）になると寂しがられるって訳だ
		CFLAG:A:627 += 1
		;精液量リセット
		CFLAG:A:626 = 0
	ELSE
		;精液量をリセット
		CFLAG:A:626 = 0
	ENDIF
ENDIF
