;--------------------------------------------------------------------------------------------------
;★オセのタイプ変動に関係した関数
;--------------------------------------------------------------------------------------------------
;-------------------------------------------------
;†調教者の射精によるオセのタイプ変動　CFLAG:801
;-------------------------------------------------
@OSE_TYPE_CHANGE_1_K1704
;オセの口に射精
IF TFLAG:0 > 0
	CFLAG:801 += 1
;オセに中出し
ELSEIF TFLAG:2 > 0
	;アナルに中出し
	IF SELECTCOM == 62 || SELECTCOM == 63 || SELECTCOM == 67 || SELECTCOM == 70 || SELECTCOM == 71 || SELECTCOM == 73 || SELECTCOM == 512 || SELECTCOM == 514
		;あなたが中出し
		IF ASSIPLAY == 0
			;陥落済み
			IF TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8
				CFLAG:801 += 2
				;親愛なら上昇
				SIF TALENT:親愛
					CFLAG:801 += 1
				;オセが夫なら下降
				SIF TALENT:夫
					CFLAG:801 -= 1
			ELSE
				CFLAG:801 += 1
			ENDIF
		;あなた以外が中出し
		ELSE
			CFLAG:801 += 1
		ENDIF
	;尻穴狂いなら上昇
	SIF TALENT:尻穴狂い
		CFLAG:801 += 3
	ENDIF
;逆レイプであなたが射精
ELSEIF TFLAG:12 > 0
	CFLAG:801 -= 1
	;オセが親愛or夫なら下降
	SIF TALENT:親愛
		CFLAG:801 -= 2
	SIF TALENT:夫
		CFLAG:801 -= 3
ENDIF

;口内or中出しで精液中毒Lv4以上なら上昇
SIF ABL:精液中毒 >= 4 && (TFLAG:0 > 0 || TFLAG:2 > 0 )
	CFLAG:801 += 2

;-------------------------------------------------
;†触手によるオセのタイプ変動（触手コマンドは一括）　CFLAG:801
;-------------------------------------------------
@OSE_TYPE_CHANGE_2_K1704
;アナル触手、触手乳首責め、触手緊縛、触手浣腸、触手口辱、触手ペニス責めを実行した場合
IF GROUPMATCH(SELECTCOM , 162 , 164 , 166 , 167 , 168 , 169 )
	CFLAG:801 += 1
	;触手が射精したら上昇
	SIF TFLAG:15 >0
		CFLAG:801 += 1
	;触手が射精して触手中毒Lv4以上なら上昇
	SIF TFLAG:15 >0 && ABL:触手中毒 >= 4
		CFLAG:801 += 3
	;アナルコマンドで尻穴狂いなら上昇
	SIF TALENT:尻穴狂い && (SELECTCOM == 162 || SELECTCOM == 167)
		CFLAG:801 += 3
	;口内or中出しで精液中毒Lv4以上なら上昇
	SIF ABL:精液中毒 >= 4 && (TFLAG:0 > 0 || TFLAG:2 > 0 )
	CFLAG:801 += 2
ENDIF

;-------------------------------------------------
;†オセの絶頂or射精によるオセのタイプ変動　CFLAG:801
;-------------------------------------------------
@OSE_TYPE_CHANGE_3_K1704
;絶頂の判定
;Ａ絶頂した場合
IF NOWEX:2 > 0 
	;強絶頂
	IF TFLAG:29 > 1
		CFLAG:801 += 2
	;絶頂
	ELSEIF TFLAG:29 > 0
		CFLAG:801 += 1
	ENDIF
	;尻穴狂いなら上昇
	SIF TALENT:尻穴狂い
		CFLAG:801 += 2
ENDIF

;Ｂ絶頂した場合
IF NOWEX:3 > 0 
	;強絶頂
	IF TFLAG:29 > 1
		CFLAG:801 += 2
	;絶頂
	ELSEIF TFLAG:29 > 0
		CFLAG:801 += 1
	ENDIF
ENDIF

;オセが射精した場合
;Ａ系調教時射精で上昇
IF TFLAG:10 > 0
	IF GROUPMATCH(SELECTCOM , 4 , 5 , 45 , 46 , 62 , 63 , 67 , 70 , 71 , 73 , 500 , 512 , 514 )
		CFLAG:801 += 2
		;尻穴狂いなら上昇
		SIF TALENT:尻穴狂い
			CFLAG:801 += 2
	;調教者に中出しで下降
	ELSEIF GROUPMATCH(SELECTCOM , 64 , 92 , 93 , 94 , 95 , 96 , 97 )
		CFLAG:801 -= 2
		;大量射精
		SIF TFLAG:10 == 2
			CFLAG:801 -= 2
		;オセに主導権
		SIF TEQUIP:主導権
			CFLAG:801 -= 3
		;相手があなたで親愛なら下降
		SIF TALENT:親愛 && ASSIPLAY == 0
			CFLAG:801 -= 1
		;相手があなたでオセが夫なら下降
		SIF TALENT:夫 && ASSIPLAY == 0
			CFLAG:801 -= 2
		;尻穴狂いなら上昇
		SIF TALENT:尻穴狂い
			CFLAG:801 += 2
	;フェラする、パイズリするで射精
	ELSEIF GROUPMATCH(SELECTCOM , 2 , 90 , 91 )
		CFLAG:801 -= 1
		;大量射精
		SIF TFLAG:10 == 2
			CFLAG:801 -= 2
			;オセに主導権
		SIF TEQUIP:主導権
			CFLAG:801 -= 3
	ENDIF
;口内、中出しで調教者の精液中毒Lv4以上なら下降
SIF ABL:PLAYER:精液中毒 >= 4 && ( SELECTCOM == 2 || SELECTCOM == 64 || SELECTCOM == 92 || SELECTCOM == 93 || SELECTCOM == 94 || SELECTCOM == 95 || SELECTCOM == 96 || SELECTCOM == 97)
	CFLAG:801 -= 2
ENDIF

