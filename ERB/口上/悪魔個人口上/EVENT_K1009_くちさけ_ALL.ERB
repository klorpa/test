		;-------------------------------------------------
	CASE 62
		CALL EVENT_MESSAGE
		IF (CFLAG:605 < 1 || FLAG:7 == 2)
			;隷ＮＴＲ
			PRINTFORMW 
			CFLAG:605 = 1
		ENDIF
		;-------------------------------------------------
		;ダンジョン内性欲処理：仲魔とセックスする
		;やられちゃう側
		;PLAYERに実行者の番号が入っています
		;-------------------------------------------------
	CASE 79
		CALL EVENT_MESSAGE
		IF (CFLAG:602 < 1 || FLAG:7 == 2)
			PRINTFORMW
			CFLAG:602 = 1
		ENDIF
