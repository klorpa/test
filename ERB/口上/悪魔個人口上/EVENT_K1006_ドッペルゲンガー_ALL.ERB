;性転換関数
;HALF：性転換（＋乳関連）
;FULL：体格など全て更新
;-------------------------------------------------
@K_1006_TRANS_SEXUAL_HALF
	IF TALENT:MASTER:オトコ == 1
		TALENT:オトコ = 0
		TALENT:処女 = CFLAG:MASTER:元処女
		TALENT:爆乳 = 0
		TALENT:巨乳 = 0
		TALENT:貧乳 = 0
		TALENT:絶壁 = 0
		IF RAND:5 == 0
			TALENT:爆乳 = 1
		ELSEIF RAND:4 == 0
			TALENT:巨乳 = 1
		ELSEIF RAND:3 == 0
			;普乳
		ELSEIF RAND:2 == 0
			TALENT:貧乳 = 1
		ELSE
			TALENT:絶壁 = 1
		ENDIF
	ELSE
		TALENT:オトコ = 1
		TALENT:爆乳 = 0
		TALENT:巨乳 = 0
		TALENT:貧乳 = 0
		TALENT:絶壁 = 0
	ENDIF
	;性転換経験＋１
	EXP:80 += 1




@K_1006_TRANS_SEXUAL_FULL

	;人外要素の削除
	TALENT:獣 = 0
	TALENT:鳥 = 0
	TALENT:爬虫類 = 0
	TALENT:魚 = 0
	TALENT:不定形 = 0
	TALENT:異形 = 0
	TALENT:鉤足 = 0
	TALENT:鉤爪 = 0

	;初めての性転換時
	IF CFLAG:220 == 3
		;女になる場合
		IF TALENT:MASTER:オトコ
			;もともと女ということにする
			CFLAG:元性別 =  2
			TALENT:処女 = CFLAG:MASTER:元処女
			CFLAG:現胸サイズ = CFLAG:MASTER:元胸サイズ
			;女時の胸のサイズが設定されて無いならランダム
			IF CFLAG:MASTER:元胸サイズ == 0
				SELECTCASE RAND:100
					CASE IS < 40
						CFLAG:現胸サイズ = 3
					CASE IS < 60
						CFLAG:現胸サイズ = 2
					CASE IS < 80
						CFLAG:現胸サイズ = 4
					CASE IS < 90
						CFLAG:現胸サイズ = 5
					CASE IS < 100
						CFLAG:現胸サイズ = 1
				ENDSELECT
			ENDIF
			SELECTCASE CFLAG:現胸サイズ
				CASE 1
					TALENT:絶壁 = 1
				CASE 2
					TALENT:貧乳 = 1
				CASE 3
				CASE 4
					TALENT:巨乳 = 1
				CASE 5
					TALENT:爆乳 = 1
			ENDSELECT
			SIF CFLAG:MASTER:元Ｖ敏感
				TALENT:Ｖ敏感 = 1
			SIF CFLAG:MASTER:元Ｖ鈍感
				TALENT:Ｖ鈍感 = 1
			SIF TALENT:男の娘 == 1
				CFLAG:元男の娘 = 1
			TALENT:オトコ = 0
			TALENT:男の娘 = 0
		;男になる場合
		ELSE
