		ELSEIF RAND:2 == 0
			LOCALS = うぐぐぐ
		ELSE
			LOCALS = くっそー……
		ENDIF
	;回避した
	CASE 18
		IF RAND:8 == 0
			LOCALS = 危ねー！
		ELSEIF RAND:7 == 0
			LOCALS = おっと
		ELSEIF RAND:6 == 0
