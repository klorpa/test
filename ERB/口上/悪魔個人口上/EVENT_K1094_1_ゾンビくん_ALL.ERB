			ELSEIF TALENT:5 || TALENT:8
				IF PLAYER == MASTER
					PRINTFORML 
					PRINTFORMW 
				ENDIF
			ENDIF
			CALL EVENT_MESSAGE
			CFLAG:606 = 1
		ENDIF
