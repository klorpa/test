@BATTLE_EVENT_K505, ARG, ARG:1, ARG:2
;-------------------------------------------------
SIF ARG < 0 && NO:ARG != 505
	RETURN 0
LOCAL:1 = 1
LOCALS = EMPTY

;崩壊してるか反発刻印3の場合は口上表示しない
;SIF TALENT:ARG:9 || MARK:ARG:3 == 3
;	RETURN 0

SELECTCASE TFLAG:戦闘イベント
	;COOP発動 ARGは発動者
	CASE 1
		IF RAND:3 == 0
			LOCALS = あ！　チャンスですわ！
		ELSEIF RAND:2 == 0
			LOCALS = お手伝い……していただけます？
		ELSE
			LOCALS = お掃除してしまいます！
		ENDIF
	;COOP参加
	CASE 2
		IF FLAG:行動順1 != -1 && FLAG:行動順1 == MASTER
			IF RAND:4 == 0
				LOCALS = %CSTR:二人称%ー！　素敵ー！
			ELSEIF RAND:3 == 0
				LOCALS = きゃー！　かっこいい！
			ELSEIF RAND:2 == 0
				LOCALS = では、参ります！
			ELSE
				LOCALS = お手伝いいたします！
			ENDIF
		ELSE
			IF RAND:4 == 0
				LOCALS = あら！　お手伝いできますね！
			ELSEIF RAND:3 == 0
				LOCALS = 合わせてみます！
			ELSEIF RAND:2 == 0
				LOCALS = え、えーい！
			ELSE
				LOCALS = この方とは気が合いますの！
			ENDIF
		ENDIF
	;被召喚時
	CASE 3
		IF RAND:3 == 0
			LOCALS = お手伝いの時間ですのね！
		ELSEIF RAND:2 == 0
			LOCALS = お掃除、お掃除……あら？
		ELSE
			LOCAL:1 = 2
			LOCALS = %CSTR:一人称%、妖精 %CALLNAME%と申します。
			LOCALS:1 = お手伝いなら任せてくださいまし。
		ENDIF
	;攻撃　ARGが攻撃をする仲魔、ARG:1が敵、ARG:2がダメージ量
	CASE 11
		CALLFORM SKILL_TYPE_{CFLAG:ARG:入力行動}
		LOCAL = MAXBASE:(ARG:1):GET_TYPE(RESULT)
		;相手が死んだ
		IF BASE:(ARG:1):ＨＰ <= 0
				IF RAND:2
					LOCALS = あ、あら？
				ELSE
					LOCALS = ……まぐれですわ。
				ENDIF
		;弱点をついた。つまりCOOP
		ELSEIF LOCAL > 100 && LOCAL < 999
			IF CFLAG:(ARG:1):防御フラグ
				LOCALS = 防がれてしまいました……
			ELSE
				;こっちはCOOP発動に回すためにカット
				RETURN 0
			ENDIF
		;大ダメージ
		ELSEIF ダメージ割合(ARG:1, ARG:2) > 50
			LOCALS = %CSTR:一人称%、そんなに力は強くないのですけれど……。
		ELSEIF ダメージ割合(ARG:1, ARG:2) > 25
			LOCALS = あら、ごめんなさいね。
		ELSEIF ダメージ割合(ARG:1, ARG:2) <= 0
			LOCALS = ……ダメみたいですわ。
		ENDIF
	;被ダメージ
	;ARGが攻撃を受ける仲魔、ARG:1が敵、ARG:2がダメージ量
	CASE 12
		;死んだ
		IF BASE:ARG:ＨＰ <= 0
			LOCALS = きゅうー……
		;大ダメージ
		ELSEIF ダメージ割合(ARG, ARG:2) > 50 || 現HP割合(ARG) < 25
			IF 陥落(ARG) && RAND:2
				LOCALS = とっても痛いですわぁ……はうう……
			ELSE
				IF RAND:2
					LOCALS = も、もうちょっと！
				ELSE
					LOCALS = 回復していただけますかしら……
				ENDIF
			ENDIF
		;無効
		ELSEIF ARG:2 == 0
			IF RAND:2 == 0
				LOCALS = 効きませんわ！
			ELSE
				LOCALS = へいちゃらです！
			ENDIF
		;吸収
		ELSEIF ARG:2 < 0
			IF RAND:2 == 0
				LOCALS = 効きませんわ！
			ELSE
				LOCALS = へいちゃらです！
			ENDIF
		ENDIF
	;反射された
	CASE 15
		IF RAND:2 == 0
			LOCALS = きゃん！
		ELSE
			LOCALS = あ、あら……
		ENDIF
	;反射した
	CASE 16
		IF RAND:2 == 0
			LOCALS = 効きませんわ！
		ELSE
			LOCALS = へいちゃらです！
		ENDIF
	;回避された
	CASE 17
		IF RAND:8 == 0
			LOCALS = 外しました……
		ELSEIF RAND:7 == 0
			LOCALS = 回避されてしまいました。
		ELSEIF RAND:6 == 0
			LOCALS = 命中……しませんわね。
		ELSEIF RAND:5 == 0
			LOCALS = 当たりません……
		ELSEIF RAND:4 == 0
			LOCALS = 命中精度に難ありですの。
		ELSEIF RAND:3 == 0
			LOCALS = ああ……向いていないのかしら。
		ELSEIF RAND:2 == 0
			LOCALS = ふにゃー！
		ELSE
			LOCALS = 当たってくださらないのね。
		ENDIF
	;回避した
	CASE 18
		IF RAND:8 == 0
			LOCALS = 回避いたしましたわ！
		ELSEIF RAND:7 == 0
			LOCALS = 服を繕うのも一仕事ですもの。
		ELSEIF RAND:6 == 0
			LOCALS = ふふん。
		ELSEIF RAND:5 == 0
			LOCALS = ……あら、運がよかったのかしら。
		ELSEIF RAND:4 == 0
			LOCALS = 的にされるのは嫌ですもの。
		ELSEIF RAND:3 == 0
			LOCALS = か、間一髪でしたわ。
		ELSEIF RAND:2 == 0
			LOCALS = %CSTR:二人称%はご無事ですの？
		ELSE
			LOCALS = はああ……びっくりしましたわ。
		ENDIF
ENDSELECT
