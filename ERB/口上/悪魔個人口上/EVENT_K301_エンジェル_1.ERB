	CALL PALAM_MESSAGE_D
	;陥落済み
	IF TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8 || TALENT:9
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 「どうしてでしょう……%CSTR:二人称%のことが…気になって…」
	ENDIF
	CFLAG:248 = 1
ENDIF

;-------------------------------------------------
;初めて恐怖がLV3超えた
;-------------------------------------------------
P = PALAM:11 + UP:11
IF P > PALAMLV:3 && CFLAG:249 == 0
	CALL PALAM_MESSAGE_D
	;陥落済み
	IF TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8 || TALENT:9
		;妻
