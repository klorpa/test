;-------------------------------------------------
;パラメータ変化時のテキスト
;-------------------------------------------------
;-------------------------------------------------
;初めて潤滑がLV3超えた
;-------------------------------------------------
P = PALAM:4 + UP:4
IF P > PALAMLV:4 && CFLAG:245 == 0
	CALL PALAM_MESSAGE_D
	;ローションを使った場合
	IF SELECTCOM == 180
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
	CFLAG:245 = 1
ENDIF

;-------------------------------------------------
;初めて欲情がLV2超えた
;-------------------------------------------------
P = PALAM:6 + UP:6
IF P > PALAMLV:3 && CFLAG:246 == 0
	CALL PALAM_MESSAGE_D
	;媚薬を使った場合
	IF SELECTCOM == 181
		PRINTFORMW 「…この卑劣漢」
	;それ以外
	ELSE
		PRINTFORMW 「っ…………なにか？」
	ENDIF
	CFLAG:246 = 1
ENDIF

;-------------------------------------------------
;初めて屈服がLV3超えた
;-------------------------------------------------
P = PALAM:7 + UP:7
IF P > PALAMLV:3 && CFLAG:247 == 0
	CALL PALAM_MESSAGE_D
	;陥落済み
	IF TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8 || TALENT:9
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 「%CSTR:一人称%…どうしたらいいんでしょうか？」
	ENDIF
	CFLAG:247 = 1
ENDIF

;-------------------------------------------------
;初めて恭順がLV3超えた
;-------------------------------------------------
