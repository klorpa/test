			PRINTFORMW 「なんで、です？ よくわかんない、です」
			CFLAG:306 = 1
		;二回目以降
		ELSE
			;陥落済み
			IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:308 < 4 || FLAG:7 == 2)
				;恋慕、親愛
				IF TALENT:3 || TALENT:6
					PRINTFORMW 「%CSTR:12%、%CSTR:12%は、%CSTR:11%の、おっぱい、すき、ですか？」
					PRINTFORMW 「そうだったら、すごく、うれしい、です」
				;淫乱、娼婦
				ELSEIF TALENT:4 || TALENT:7
					PRINTFORMW 「おっぱい、もむもむ、どう、ですか？」
					PRINTFORMW 「%CSTR:11%、おっぱい、さわられるの、すき、です」
					PRINTFORMW 「%CSTR:12%も、そうだと、うれしい、ですよ？」
				;服従、隷属
				ELSEIF TALENT:5 || TALENT:8
					PRINTFORMW 「お、おっぱい、つねる、です、か？」
					PRINTFORMW 「つまみやすい、とこ、あるです、けど」
					PRINTFORMW %CALLNAME%は乳首を手で隠し、警戒している
				ENDIF
				CFLAG:306 = 4
			;未陥落
			ELSE
				;屈服刻印Lv3
				IF MARK:2 == 3 && (CFLAG:308 < 3 || FLAG:7 == 2)
					PRINTFORMW 「おっぱい、もむもむする、ですか？」
					PRINTFORMW 「なんだか、%CSTR:11%も、ふわふわする、ですよ？」
					CFLAG:306 = 3
				;それ以外
				ELSEIF CFLAG:306 < 2 || FLAG:7 == 2
					PRINTFORMW 「おっぱい、むにむに、する、です？」
					CFLAG:306 = 2
				ENDIF
			ENDIF
		ENDIF



