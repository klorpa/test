	PRINTFORMW 「やめなさいよ、こんな事！」
	CFLAG:202 = 1
;屈服刻印Lv1
ELSEIF MARK:2 == 1 && (CFLAG:202 < 2 || FLAG:7 == 2)
	PRINTFORMW 「くっ、嫌だって言ってるでしょう！」
	CFLAG:202 = 2
;屈服刻印Lv2
ELSEIF MARK:2 == 2 && (CFLAG:202 < 3 || FLAG:7 == 2)
	PRINTFORMW 「ま、またするの……？」
	CFLAG:202 = 3
;屈服刻印Lv3＋陥落素質無し
ELSEIF MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0) && (CFLAG:202 < 4 || FLAG:7 == 2)
	PRINTFORMW 「……やるならさっさと済ませて、すぐに出て行ってよ」
	CFLAG:202 = 4
;陥落済み
ELSEIF CFLAG:202 < 5 || FLAG:7 == 2
	;恋慕、親愛
	IF TALENT:3 || TALENT:6
		;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORML 「今日のトレーニングメニューは何かしら？」
			PRINTFORMW 「%CSTR:12%がしてくれる事なら、何でも大歓迎なんだけどね%UNICODE(0x2661)%」
		ELSEIF K:1 == 1
			PRINTFORML 「遅いわよ、体が冷えちゃうじゃない」
			PRINTFORMW 「さ、温めて%UNICODE(0x2661)%」
		ELSE
			PRINTFORML 「%CSTR:12%の仕事柄、他の子の所に行くのも仕方ないけど……」
			PRINTFORMW 「%CSTR:11%の事を放りっぱなしにしちゃダメよ？」
		ENDIF
	;淫乱、娼婦
	ELSEIF TALENT:4 || TALENT:7
		;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORML 「今日のトレーニングメニューは何かしら？」
			PRINTFORMW 「どんな事されちゃうか、想像しただけで熱くなってくるわ%UNICODE(0x2661)%」
		ELSEIF K:1 == 1
			PRINTFORML 「遅いわよ、体が冷えちゃうじゃない」
			PRINTFORMW 「さ、とろとろになるまで温めて%UNICODE(0x2661)%」
		ELSE
			PRINTFORML 「来たわね、楽しませてもらうわよ」
			PRINTFORMW 「空っぽになるくらい、搾り取っちゃうんだから%UNICODE(0x2661)%」
		ENDIF
	;服従、隷属
	ELSEIF TALENT:5 || TALENT:8
		;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	CFLAG:202 = 5
ENDIF
RETURN 0

