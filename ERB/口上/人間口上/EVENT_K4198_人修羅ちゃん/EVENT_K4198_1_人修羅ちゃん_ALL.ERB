			ENDIF
		ENDIF
		PRINTFORML Name is　%NAME:TARGET%
		PRINTFORML Nickname is　%CALLNAME:TARGET%
		PRINTL Is this ok？
		CALL INPUT_YN
		SIF RESULT == 1
			GOTO INPUT_LOOP
	ELSEIF RESULT == 1
		PRINTFORMW ―Name was unchanged―
	CFLAG:220 = 1
ENDIF
