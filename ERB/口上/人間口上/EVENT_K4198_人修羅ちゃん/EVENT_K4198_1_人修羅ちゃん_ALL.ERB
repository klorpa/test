	CALL INPUT_YN
	IF RESULT == 0
		$INPUT_LOOP
		PRINTL Please enter the『demi-fiend』's name
		INPUTS
		NAME:TARGET = %RESULTS%
		PRINTL Please enter the『demi-fiend』's nickname
		INPUTS
		CALLNAME:TARGET = %RESULTS%
		VARSET LOCALS
		IF NAME:TARGET == LOCALS && CALLNAME:TARGET == LOCALS
