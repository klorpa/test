				;隷ＮＴＲ
				PRINTFORMW 
			ENDIF
			CFLAG:604 = 1
		ENDIF
		;-------------------------------------------------
		;;ＮＴＲ夜這い・野外調教
		;;隷ＮＴＲ状態(TALENT:ＮＴＲが 3 か 6)でCFLAG:現ＮＴＲ進行度が6以上の場合１日の終了時に発生。
		;-------------------------------------------------
	CASE 62
		CALL EVENT_MESSAGE
		IF (CFLAG:605 < 1 || FLAG:7 == 2)
			IF TALENT:ＮＴＲ == 1 || TALENT:ＮＴＲ == 4
				;愛ＮＴＲ
				PRINTFORMW 
			ELSEIF TALENT:ＮＴＲ == 3 || TALENT:ＮＴＲ == 6
				;隷ＮＴＲ
				PRINTFORMW 
			ENDIF
			CFLAG:605 = 1
		ENDIF
	CASEELSE
		CALL EVENT_MESSAGE
ENDSELECT
