;-------------------------------------------------
;@KOJO_MESSAGE_PALAMCNG_A（4518をキャラ番号に置換）
;調教者の射精をトリガーにした口上
;-------------------------------------------------
@KOJO_MESSAGE_PALAMCNG_A_K4518_OBEY
;-------------------------------------------------
;調教者の射精時のテキスト
;-------------------------------------------------
;-------------------------------------------------
;射精箇所変更時のテキスト　有効時はここで関数終了
;-------------------------------------------------
IF CFLAG:106
	CALL PALAM_MESSAGE_A
	;髪
	IF CFLAG:106 == 1
	;顔
	ELSEIF CFLAG:106 == 2
	;胸
	ELSEIF CFLAG:106 == 3
	;腹
	ELSEIF CFLAG:106 == 4
	;腋
	ELSEIF CFLAG:106 == 5
	;太腿
	ELSEIF CFLAG:106 == 6
	;割れ目
	ELSEIF CFLAG:106 == 7
	;尻
	ELSEIF CFLAG:106 == 8
	;ゴム
	ELSEIF CFLAG:106 == 9
		;セックスで射精かつ、アナルセックス以外を実行時
		IF GROUPMATCH(SELECTCOM , 60 , 61 , 66 , 68 , 69 , 72)
		ELSE
		ENDIF
	ENDIF
	RETURN 0
ENDIF

;-------------------------------------------------
;手、口以外で射精
;対応コマンドはキス、素股、足コキ、アナル強制奉仕、ダブル素股
;-------------------------------------------------
IF TFLAG:9 == 1
	CALL PALAM_MESSAGE_A
ENDIF

;-------------------------------------------------
;助手を犯すで射精
;-------------------------------------------------
IF TFLAG:7 > 0
	CALL PALAM_MESSAGE_A
ENDIF

;-------------------------------------------------
;口で射精
;-------------------------------------------------
IF TFLAG:0 > 0
	CALL PALAM_MESSAGE_A
ENDIF

;-------------------------------------------------
;手で射精
;-------------------------------------------------
IF TFLAG:1 > 0
	CALL PALAM_MESSAGE_A
ENDIF

;-------------------------------------------------
;胸で射精
;-------------------------------------------------
IF TFLAG:34 > 0
	CALL PALAM_MESSAGE_A
ENDIF

;-------------------------------------------------
;セックスで射精
;-------------------------------------------------
IF TFLAG:2 > 0
	CALL PALAM_MESSAGE_A
	;アナルセックス系
	IF SELECTCOM == 62 || SELECTCOM == 63 || SELECTCOM == 67 || SELECTCOM == 70 || SELECTCOM == 71 || (SELECTCOM == 505 && TFLAG:40 == 2)
	;セックス系
	ELSE
	ENDIF
ENDIF

;-------------------------------------------------
;逆レイプで射精させた
;-------------------------------------------------
IF SELECTCOM == 64 && TFLAG:10 > 0
	CALL PALAM_MESSAGE_A
	;陥落済み
ENDIF

