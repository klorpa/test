;--------------------------------------------------
;簡易助手口上の発動時に初調教時口上をスキップ（Y1を助手のキャラ番号にする）
;複数人いる際には、SIF NO:ASSI == Y1 || NO:ASSI == Z1と追加していく
;--------------------------------------------------
;IF CFLAG:201 == 0 && ASSI > 0
;	SIF NO:ASSI == Y1
;		CFLAG:201 =1
;ENDIF
;-------------------------------------------------
;調教開始時（一回のみ） CFLAG201
;-------------------------------------------------
DRAWLINE
;初調教時
IF CFLAG:201 == 0 && TALENT:恋慕 == 0
	CFLAG:201 = 1
	RETURN 0
;屈服刻印Lv1
ELSEIF CFLAG:201 < 2 && MARK:2 == 1
	PRINTFORMW 
	CFLAG:201 = 2
	RETURN 0
;屈服刻印Lv2
ELSEIF CFLAG:201 < 3 && MARK:2 == 2
	PRINTFORMW 
	CFLAG:201 = 3
	RETURN 0
;屈服刻印Lv3
ELSEIF CFLAG:201 < 4 && MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0)
	PRINTFORMW 
	CFLAG:201 = 4
	RETURN 0
;陥落
ELSEIF CFLAG:201 < 5 && (TALENT:3 || TALENT:4 || TALENT:5)
	;恋慕
	IF TALENT:3
		IF CFLAG:未調教陥落 == 1
			;調教せずに恋慕をつけた場合
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;淫乱
	ELSEIF TALENT:4
		PRINTFORMW 
	;服従
	ELSEIF TALENT:5
		PRINTFORMW 
	ENDIF
	CFLAG:201 = 5
	RETURN 0
ENDIF

;-------------------------------------------------
;簡易助手口上（Y1を助手のキャラ番号に置換、X1をキャラ番号に変更する) CFLAG203
;掛け合いをするキャラを増やす場合は、構文をコピー＆ペーストをして、CFLAGの数を204、205と増やす
;-------------------------------------------------
;助手の有無をチェック（いない場合は二回目以降へ飛ぶ）
IF ASSI < 0 
