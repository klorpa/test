;口上のある助手が居ない場合は、通常の二回目以降の口上へ飛ぶ
ELSE
	CALL EVENTTRAIN_SECOND_K4503
ENDIF

;-------------------------------------------------
;調教開始時（2回目以降、X1をキャラ番号に変更する）
;-------------------------------------------------
@EVENTTRAIN_SECOND_K4503
;反発刻印Lv3
IF MARK:3 == 3 && (CFLAG:202 < 4 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 4
;屈服刻印Lv0
ELSEIF MARK:2 == 0 && (CFLAG:202 < 1 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 1
;屈服刻印Lv1
ELSEIF MARK:2 == 1 && (CFLAG:202 < 2 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 2
;屈服刻印Lv2
ELSEIF MARK:2 == 2 && (CFLAG:202 < 3 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 3
;屈服刻印Lv3＋陥落素質無し
ELSEIF MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0) && (CFLAG:202 < 4 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 4
;陥落済み
ELSEIF CFLAG:202 < 5 || FLAG:7 == 2
	;恋慕、親愛
	IF TALENT:3 || TALENT:6
		;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 「あ、はぁ〜い、今開けるね」
			PRINTFORMW 「んっ、ちゅっ……えへへへっ%ハート()%」
		ELSEIF K:1 == 1
			PRINTFORMW 「おかえりなさぁい%ハート()%　……んっ」
			PRINTFORMW 「…んっ……ん〜〜〜」
			PRINTFORMW 目を閉じてキスをせがむ%CALLNAME%の顎に指を掛けて、そっと唇を重ねた
			PRINTFORML 
			PRINTFORMW 「……んふふ〜♪んっ%ハート()%　んっ%ハート()%」
			PRINTFORMW 頬を染めながら、さらにキスをせがむ%CALLNAME%を抱き上げ、
			PRINTFORMW 唇を塞ぎながらベッドへ移動した
		ELSE
			PRINTFORMW 「おかえりなさい、ア・ナ・タ%ハート()%」
			PRINTFORMW 「わたしにする？わたしにする？それとも、わ・た・し%ハート()%」
			PRINTFORMW 選択肢が無い事に苦笑しながら%CALLNAME%を抱き寄せ、
			PRINTFORMW 重ねた唇で回答を伝えた
		ENDIF
	;淫乱、娼婦
	ELSEIF TALENT:4 || TALENT:7
		;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;服従、隷属
	ELSEIF TALENT:5 || TALENT:8
		;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	CFLAG:202 = 5
ENDIF
RETURN 0

;-------------------------------------------------
;EVENTEND関係（X1をキャラ番号に置換）
;調教終了時のセリフ
;-------------------------------------------------
@EVENTEND_K4503
;--------------------------------------------------
;特定キャラが助手、主人時に特殊キャラ口上を呼び出す(Y1を特定キャラ、X1を奴隷のキャラ番号に置換）
;--------------------------------------------------
;IF NO:MASTER == Y1
;	CALL EVENTEND_PY1_K4503
;	RETURN 0
;ENDIF
;IF ASSI > 0 && ASSIPLAY
;	SIF NO:ASSI == Y1
;	CALL EVENTEND_PY1_K4503
;	RETURN 0
;ENDIF
;--------------------------------------------------
;調教終了時のセリフ
;--------------------------------------------------
DRAWLINE
;反発刻印Lv3
IF MARK:3 == 3 && (CFLAG:217 < 1 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:217 = 1
;屈服刻印Lv1以下
ELSEIF MARK:2 <= 1 && (CFLAG:219 < 1 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:219 = 1
;屈服刻印Lv2
ELSEIF MARK:2 == 2
	PRINTFORMW 
	CFLAG:219 = 2
;屈服刻印Lv3＋陥落素質無し
ELSEIF MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0)
	PRINTFORMW 
	CFLAG:219 = 3
;陥落済み＋体力500未満
ELSEIF BASE:0 < 500 && (CFLAG:218 < 1 || FLAG:7 == 2)
	;恋慕、親愛
	IF TALENT:3 || TALENT:6
		PRINTFORMW 「…はぁ……ねぇ、%CSTR:二人称%」
		PRINTFORMW 息を乱して寝そべる%CALLNAME%の方を見ると、%CALLNAME%は嬉しそうに微笑んだ
		PRINTFORMW 
		PRINTFORMW 「%CSTR:一人称%…とっても幸せだよ%ハート()%」
		PRINTFORMW 「また一緒にしようね、約束っ%ハート()%」
	;淫乱、娼婦
	ELSEIF TALENT:4 || TALENT:7
		PRINTFORMW 
	;服従、隷属
	ELSEIF TALENT:5 || TALENT:8
		PRINTFORMW 
	ENDIF
	CFLAG:218 = 1
;陥落済み
ELSEIF CFLAG:219 < 4 || FLAG:7 == 2
	;恋慕、親愛
	IF TALENT:3 || TALENT:6
		PRINTFORMW 「ねっ、ねっ、…んっ…」
		PRINTFORMW 目を閉じてキスを待つ%CALLNAME%に優しく口付けをした
	;淫乱、娼婦
	ELSEIF TALENT:4 || TALENT:7
		PRINTFORMW 
	;服従、隷属
	ELSEIF TALENT:5 || TALENT:8
		PRINTFORMW 
	ENDIF
	CFLAG:219 = 4
ENDIF
RETURN 0

