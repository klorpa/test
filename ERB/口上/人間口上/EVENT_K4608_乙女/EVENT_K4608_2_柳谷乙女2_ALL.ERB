;-------------------------------------------------
;調教開始時（2回目以降、4608_2をキャラ番号に変更する）
;-------------------------------------------------
@EVENTTRAIN_SECOND_K4608_2
;反発刻印Lv3
IF MARK:3 == 3 && (CFLAG:202 < 4 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 4
;屈服刻印Lv0
ELSEIF MARK:2 == 0 && (CFLAG:202 < 1 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 1
;屈服刻印Lv1
ELSEIF MARK:2 == 1 && (CFLAG:202 < 2 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 2
;屈服刻印Lv2
ELSEIF MARK:2 == 2 && (CFLAG:202 < 3 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 3
;屈服刻印Lv3＋陥落素質無し
ELSEIF MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0) && (CFLAG:202 < 4 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 4
;陥落済み
ELSEIF CFLAG:202 < 5 || FLAG:7 == 2
	;恋慕、親愛
	IF TALENT:3 || TALENT:6
		;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;淫乱、娼婦
	ELSEIF TALENT:4 || TALENT:7
		;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;服従、隷属
	ELSEIF TALENT:5 || TALENT:8
		;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	CFLAG:202 = 5
ENDIF
RETURN 0

