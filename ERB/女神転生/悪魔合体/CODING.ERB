@CODING
#LOCALSIZE 2
DRAWLINE
PRINTL The plug-in software developed from analyzing some demons has been completed
DRAWLINE
FOR LOCAL, 0, CHARANUM
	SIF FLAG:(NO:LOCAL+20000) < 1000
		CONTINUE
	LOCAL:1 = ABL:LOCAL:コーディング ? ABL:LOCAL:コーディング # CSVABL(NO:LOCAL, GETNUM(ABL, "コーディング"),0)
	SIF !LOCAL:1
		CONTINUE
	SIF ITEM:(LOCAL:1)
		CONTINUE
	CALL MESSAGE_B(1, @"Developed the plug-in【%PLUGINNAME_E(LOCAL:1)%】from %CALLNAME:LOCAL%")
	ITEM:(LOCAL:1) = 1
NEXT
