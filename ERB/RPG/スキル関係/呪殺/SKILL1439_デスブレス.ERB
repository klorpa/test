;通常攻撃
;スキルのテンプレートになっています

;スキル名
@SKILL_NAME_1439,ARG
RESULTS = Death Breath

;　命中
@SKILL_HITRATE_1439, ARG
RETURN 100

;　威力
@SKILL_POWER_1439, ARG
SIF FLAG:拡散フラグ
	RETURN 125
RETURN 190

;　最低攻撃回数
@SKILL_MINATTACKNUMBER_1439,ARG
RETURN 1

;　最大攻撃回数
@SKILL_MAXATTACKNUMBER_1439,ARG
RETURN 1

;　ＨＰ2を支払うかＭＰ3を支払うか
@SKILL_COSTTYPE_1439, ARG
SIF FLAG:拡散フラグ
	RETURN 1
RETURN 3
;消費なし＝1

;　コストの量
@SKILL_COST_1439,ARG
RETURN 14

;　相性
@SKILL_TYPE_1439,ARG
RETURN GET_TYPE_NUM("呪殺")

;　1EXTRAか2MAGICか
@SKILL_DECIDE_TYPE_1439
RETURN 1
;どちらでもない

;　1物理ダメージで計算するか2魔法ダメージで計算するか
@SKILL_DAMAGETYPE_1439
RETURN 1

;　対象が1敵か2味方か3場全体か4特殊か
@SKILL_TARGET_1439
RETURN 1

;　射程
@SKILL_RANGE_1439,ARG
RETURN 3

;　範囲　1単体　21列　3全体　4　敵味方全体
@SKILL_SPHERE_1439,ARG
RETURN 1

;　 効果のタイプ　1攻撃、2回復、3状態変化、4特殊、5状態回復
@SKILL_EFECT_1439
RETURN 1

;　 特殊処理の有無、及び処理内容
@SKILL_SPECIALEFECT_1439,ARG
;ARG = 1だった場合は処理の有無を返します
SIF ARG == 1
	RETURN 0
;以下に特殊な処理を記述します

;継承タイプ
@SKILL_SUCCESSION_TYPE_1439
RETURN 14
;Dark

;速度補正
@SKILL_SPEED_1439, ARG = -1
RETURN 0

;　口上の表示とそのタイプ
@SKILL_MESSAGE_1439,ARG


;戦闘時以外に使用できるか
@SKILL_ACTIONABLE_FIELD_1439
RETURN 0

;スキル変化？用ランク
@SKILL_RANK_1439
RETURN 5

@SKILL_RANKUP_1439
RETURN 0

;テンプレート用、追加効果シリーズ
;追加効果ステータス
@SKILL_ADDTIONAL_STATE_1439,ARG
RETURN GET_STATE_NUM("DYING")

;追加効果相性
@SKILL_ADDTIONAL_TYPE_1439,ARG
RETURN GET_TYPE_NUM("呪殺")

;追加効果基本付与確率
@SKILL_ADDTIONAL_BASE_RATE_1439,ARG
RETURN 25

;追加効果付与確率上限
@SKILL_ADDTIONAL_MAX_RATE_1439,ARG
RETURN 75

;拡散攻撃用追加処理
@SKILL_SPREAD_1439,ARG
RETURN 1
;拡散距離:1

;====================================================
;スキル及び行動の実行
;====================================================
@ACTION_1439,ARG,ARG:1
CALL ATTACK_SPREAD,ARG,ARG:1,1439



;====================================================
;実行内容ここまで
;====================================================

;　 説明文の表示
@SKILL_EXPLAIN_1439
PRINTL 暗黒のブレスを吹きつけ、対象とその周囲にダメージ、DYINGを付与。

;継承に必要な部位
@継承部位_1439,ARG
SIF TALENT:ARG:口
	RETURN 1
RETURN 0


;使用条件
@SKILL_SPECIAL_ACTIONABLE_1439,ARG
RETURN 1
