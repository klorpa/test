;---------------------------------
;ウェイトスキルは専用コマンドから実行させ
;そこでほとんど処理するので、個別の設定はほぼなしで
;---------------------------------
;スキル名
@SKILL_NAME_1733,ARG
RESULTS = Void Wind
;　命中
@SKILL_HITRATE_1733, ARG
RETURN 0

;　威力
@SKILL_POWER_1733, ARG
RETURN 0

;　最低攻撃回数
@SKILL_MINATTACKNUMBER_1733,ARG
RETURN 0

;　最大攻撃回数
@SKILL_MAXATTACKNUMBER_1733,ARG
RETURN 0

;　ＨＰ2を支払うかＭＰ3を支払うか
@SKILL_COSTTYPE_1733, ARG
RETURN 1
;消費なし＝1

;　コストの量
;MAXMPの1%
@SKILL_COST_1733,ARG
RETURN 2

;　相性
@SKILL_TYPE_1733,ARG
RETURN 0

;　1EXTRAか2MAGICか
@SKILL_DECIDE_TYPE_1733
RETURN 2
;どちらでもない

;　1物理ダメージで計算するか2魔法ダメージで計算するか
@SKILL_DAMAGETYPE_1733
RETURN 1


;　対象が1敵か2味方か3場全体か4特殊か
@SKILL_TARGET_1733
RETURN 4

;　射程
@SKILL_RANGE_1733,ARG
RETURN 1

;　範囲　1単体　21列　3全体　4　敵味方全体
@SKILL_SPHERE_1733,ARG
RETURN 4

;　 効果のタイプ　1攻撃、2回復、3状態変化、4特殊、5状態回復
@SKILL_EFECT_1733
RETURN 4

;継承タイプ
@SKILL_SUCCESSION_TYPE_1733
RETURN GET_SUCCESSION_NUM("疾風")
;継承不可

;速度補正
@SKILL_SPEED_1733, ARG = -1
RETURN 0

;　口上の表示とそのタイプ
@SKILL_MESSAGE_1733,ARG

;戦闘時以外に使用できるか
@SKILL_ACTIONABLE_FIELD_1733
RETURN 0

;スキル変化？用ランク
@SKILL_RANK_1733
RETURN 0
;変化不可

@ウェイトスキル_1733
RETURN 1
;戦闘中に使用できるか
@SKILL_ACTIONABLE_BATTLE_1733
RETURN 0
;====================================================
;スキル及び行動の実行
;====================================================
@ACTION_1733,ARG,ARG:1

;====================================================
;実行内容ここまで
;====================================================
;====================================================
;戦闘中以外での実行
;====================================================
@ACTION_FIELD_1733,ARG,ARG:1
@SKILL_EXPLAIN_1733
PRINTL １ターンの間、疾風相性の攻撃を無効化する

;使用条件
@SKILL_SPECIAL_ACTIONABLE_1733,ARG
RETURN 0

