@SKILL_ADDTIONAL_STATE_280,ARG
RETURN GET_STATE_NUM("CHARM")

;追加効果相性
@SKILL_ADDTIONAL_TYPE_280,ARG
RETURN GET_TYPE_NUM("精神")

