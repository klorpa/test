;元バルカンキック

;スキル名
@SKILL_NAME_280,ARG
RESULTS = Wonderful Stick
;素敵なステッキ

;　命中
@SKILL_HITRATE_280, ARG
RETURN 90

;　威力
@SKILL_POWER_280, ARG
RETURN 170

;　最低攻撃回数
@SKILL_MINATTACKNUMBER_280,ARG
RETURN 3

;　最大攻撃回数
@SKILL_MAXATTACKNUMBER_280,ARG
RETURN 8

;一体辺りの最大ＨＩＴ数
@SKILL_MAXATK_PER_280,ARG
RETURN 8

;一体辺りの最低ＨＩＴ数
@SKILL_MINATK_PER_280,ARG
RETURN 1

;　ＨＰ2を支払うかＭＰ3を支払うか
@SKILL_COSTTYPE_280, ARG
RETURN 3
;消費なし＝1

;　コストの量
@SKILL_COST_280,ARG
RETURN 30

;　相性
@SKILL_TYPE_280,ARG
RETURN GET_TYPE_NUM("打撃")

;　1EXTRAか2MAGICか
@SKILL_DECIDE_TYPE_280
RETURN 1

;　1物理ダメージで計算するか2魔法ダメージで計算するか
@SKILL_DAMAGETYPE_280
RETURN 1


;　対象が1敵か2味方か3場全体か4特殊か
@SKILL_TARGET_280
RETURN 1

;　射程
@SKILL_RANGE_280,ARG
RETURN 1

;　範囲　1単体　21列　3全体　4　敵味方全体
@SKILL_SPHERE_280,ARG
RETURN 2

;　 効果のタイプ　1攻撃、2回復、3状態変化、4特殊、5状態回復
@SKILL_EFECT_280
RETURN 1

;　 特殊処理の有無、及び処理内容
@SKILL_SPECIALEFECT_280,ARG
;ARG = 1だった場合は処理の有無を返します
SIF ARG == 1
	RETURN 0
;以下に特殊な処理を記述します

;継承タイプ
@SKILL_SUCCESSION_TYPE_280
RETURN 2
;打撃

;速度補正
@SKILL_SPEED_280, ARG = -1
RETURN 5

;　口上の表示とそのタイプ
@SKILL_MESSAGE_280,ARG


;戦闘時以外に使用できるか
@SKILL_ACTIONABLE_FIELD_280
RETURN 0

;スキル変化？用ランク
@SKILL_RANK_280
RETURN 13

