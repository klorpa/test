@参加人数_4560
RETURN 1

@参加条件表示_4560,ARG
SELECTCASE ARG
	CASE 1
		PRINTFORM Skill from the Mudo line.
	CASE 2
		PRINTFORM Status element inflicting magic
ENDSELECT

@発動者条件_4560, ARG
FOR LOCAL, 1 , FLAG:スキル数+1
	SIF ABL:ARG:("スキル" + TOSTR(LOCAL)) == 0
		CONTINUE
	CALLFORM SKILL_DECIDE_TYPE_{ABL:ARG:("スキル" + TOSTR(LOCAL))}
	SIF RESULT != 2
		CONTINUE
	CALLFORM SKILL_TYPE_{ABL:ARG:("スキル" + TOSTR(LOCAL))} , ARG
	SIF RESULT != GET_TYPE_NUM("呪殺")
		CONTINUE
	CALLFORM SKILL_EFECT_{ABL:ARG:("スキル" + TOSTR(LOCAL))}
	SIF RESULT != 3
		CONTINUE
	CALL CHECK_ACTIONABLE , ARG , ABL:ARG:("スキル" + TOSTR(LOCAL))
	SIF RESULT == 0
		CONTINUE
	RETURN 1
NEXT
RETURN 0

@参加者条件_4560, ARG , ARG:1
#LOCALSIZE 2
SIF CPOS(ARG) < 1 || CPOS(ARG) > 6
	RETURN 0
CALL CHECK_COST_LINCAGE, ARG, 4560, ARG:1
SIF RESULT == 0
	RETURN 0
;状態変化魔法スキルを使えること(DYINGは除く)
FOR LOCAL, 1, 42
	LOCAL = LOCAL == FLAG:スキル数 + 1 ? 21 # LOCAL
	LOCAL:1 = ABL:ARG:@"\@ LOCAL > 20 ? 装備 # \@スキル{LOCAL > 20 ? LOCAL - 20 # LOCAL}"
	SIF !LOCAL:1 && LOCAL > 20
		BREAK
	SIF !LOCAL:1
		CONTINUE
	;状態異常でないなら戻る
	CALLFORM SKILL_EFECT_{LOCAL:1}
	SIF RESULT != 3
		CONTINUE
	;魔法でないなら戻る
	CALLFORM SKILL_DECIDE_TYPE_{LOCAL:1}
	SIF RESULT != 2
		CONTINUE
	;即死は除外
	CALLFORM SKILL_ADDTIONAL_STATE_{LOCAL:1}
	SIF RESULT == GET_STATE_NUM("DYING")
		CONTINUE
	CALL CHECK_ACTIONABLE , ARG , LOCAL:1
	SIF RESULT == 0
		CONTINUE
	RETURN 1
NEXT
RETURN 0
