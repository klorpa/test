;追加効果ステータス
@SKILL_ADDTIONAL_STATE_1009,ARG
RETURN GET_STATE_NUM("DYING")


;追加効果相性
@SKILL_ADDTIONAL_TYPE_1009,ARG
RETURN 10

;追加効果基本付与確率
@SKILL_ADDTIONAL_BASE_RATE_1009,ARG
RETURN 20

;追加効果付与確率上限
@SKILL_ADDTIONAL_MAX_RATE_1009,ARG
RETURN 95
