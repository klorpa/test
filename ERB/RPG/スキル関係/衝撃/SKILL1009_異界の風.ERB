;通常攻撃
;スキルのテンプレートになっています

;スキル名
@SKILL_NAME_1009,ARG
RESULTS = 異界の風

;　命中
@SKILL_HITRATE_1009, ARG
RETURN 85

;　威力
@SKILL_POWER_1009, ARG
RETURN 315

;　最低攻撃回数
@SKILL_MINATTACKNUMBER_1009,ARG
RETURN 1

;　最大攻撃回数
@SKILL_MAXATTACKNUMBER_1009,ARG
RETURN 1

;　ＨＰ2を支払うかＭＰ3を支払うか
@SKILL_COSTTYPE_1009, ARG
RETURN 3
;消費なし＝1

;　コストの量
@SKILL_COST_1009,ARG
RETURN 63

;　相性
@SKILL_TYPE_1009,ARG
RETURN 7
;Force

;　1EXTRAか2MAGICか
@SKILL_DECIDE_TYPE_1009
RETURN 1
;魔法

;　1物理ダメージで計算するか2魔法ダメージで計算するか
@SKILL_DAMAGETYPE_1009
RETURN 2


;　対象が1敵か2味方か3場全体か4特殊か
@SKILL_TARGET_1009
RETURN 1

;　射程
@SKILL_RANGE_1009,ARG
RETURN 3

;　範囲　1単体　21列　3全体　4　敵味方全体
@SKILL_SPHERE_1009,ARG
RETURN 3

;　 効果のタイプ　1攻撃、2回復、3状態変化、4特殊、5状態回復
@SKILL_EFECT_1009
RETURN 1

;　 特殊処理の有無、及び処理内容
@SKILL_SPECIALEFECT_1009,ARG
;ARG = 1だった場合は処理の有無を返します
SIF ARG == 1
	RETURN 0
;以下に特殊な処理を記述します

;継承タイプ
@SKILL_SUCCESSION_TYPE_1009
RETURN 10

@継承不能_1009
RETURN 1
;不可

;速度補正
@SKILL_SPEED_1009, ARG = -1
RETURN 0

;　口上の表示とそのタイプ
@SKILL_MESSAGE_1009,ARG

;戦闘時以外に使用できるか
@SKILL_ACTIONABLE_FIELD_1009
RETURN 0

;スキル変化？用ランク
@SKILL_RANK_1009
RETURN 0
;不可

