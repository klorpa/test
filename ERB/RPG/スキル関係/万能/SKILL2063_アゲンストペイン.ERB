;セト専用
;非常に高いクリティカル率を誇る万能物理属性攻撃
;命中弱体の影響を倍受けるので、弱体しとこうという感じに

;スキル名
@SKILL_NAME_2063,ARG
RESULTS = Agents of Pain

;　命中
@SKILL_HITRATE_2063, ARG
SIF ARG == -1
	RETURN 100
;通常のンダに加えて判定を受ける
RETURN 100 * MAX(32, 32 + CFLAG:ARG:命中強化) / 32
;　威力
@SKILL_POWER_2063, ARG
RETURN 500

;　最低攻撃回数
@SKILL_MINATTACKNUMBER_2063,ARG
RETURN 1

;　最大攻撃回数
@SKILL_MAXATTACKNUMBER_2063,ARG
RETURN 1

@SKILL_CRITICAL_RATE_2063
RETURN 60
;　ＨＰ2を支払うかＭＰ3を支払うか
@SKILL_COSTTYPE_2063, ARG
RETURN 3
;消費なし＝1

;　コストの量
@SKILL_COST_2063,ARG
RETURN 0

;　相性
@SKILL_TYPE_2063,ARG
RETURN 17
;Almig

;　1EXTRAか2MAGICか
@SKILL_DECIDE_TYPE_2063
RETURN 1
;EX

;　1物理ダメージで計算するか2魔法ダメージで計算するか
@SKILL_DAMAGETYPE_2063
RETURN 1


;　対象が1敵か2味方か3場全体か4特殊か
@SKILL_TARGET_2063
RETURN 1

;　射程
@SKILL_RANGE_2063,ARG
RETURN 1

;　範囲　1単体　21列　3全体　4　敵味方全体
@SKILL_SPHERE_2063,ARG
RETURN 1

;　 効果のタイプ　1攻撃、2回復、3状態変化、4特殊、5状態回復
@SKILL_EFECT_2063
RETURN 1

;　 特殊処理の有無、及び処理内容
@SKILL_SPECIALEFECT_2063,ARG
;ARG = 1だった場合は処理の有無を返します
SIF ARG == 1
	RETURN 0
;以下に特殊な処理を記述します

;継承タイプ 継承不能
@SKILL_SUCCESSION_TYPE_2063
RETURN 20

;速度補正
@SKILL_SPEED_2063, ARG = -1
RETURN 0

;　口上の表示とそのタイプ
@SKILL_MESSAGE_2063,ARG

;戦闘時以外に使用できるか
@SKILL_ACTIONABLE_FIELD_2063
RETURN 0

;スキル変化？用ランク
@SKILL_RANK_2063
RETURN 0

;テンプレート用、追加効果シリーズ
;追加効果ステータス
@SKILL_ADDTIONAL_STATE_2063,ARG
RETURN 0


;追加効果相性
@SKILL_ADDTIONAL_TYPE_2063,ARG
RETURN 0

;追加効果基本付与確率
@SKILL_ADDTIONAL_BASE_RATE_2063,ARG
RETURN 0

;追加効果付与確率上限
@SKILL_ADDTIONAL_MAX_RATE_2063,ARG
RETURN 0

@敵専用_2063
RETURN 1
