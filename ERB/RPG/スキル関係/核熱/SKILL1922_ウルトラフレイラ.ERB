;追加効果ステータス
@SKILL_ADDTIONAL_STATE_1922,ARG
RETURN 0

;追加効果相性
@SKILL_ADDTIONAL_TYPE_1922,ARG
RETURN 0

;追加効果基本付与確率
@SKILL_ADDTIONAL_BASE_RATE_1922,ARG
RETURN 0

;追加効果付与確率上限
@SKILL_ADDTIONAL_MAX_RATE_1922,ARG
RETURN 0
