;通常攻撃
;スキルのテンプレートになっています

;スキル名
@SKILL_NAME_1922,ARG
RESULTS = Ultra Freire

;　命中
@SKILL_HITRATE_1922, ARG
RETURN 100

;　威力
@SKILL_POWER_1922, ARG
SIF ARG < 0
	RETURN 110
SIF スキル属性表示中
	RETURN BASE:ARG:ＭＰ * 2 + 110
RETURN FLAG:拡散フラグ

;　最低攻撃回数
@SKILL_MINATTACKNUMBER_1922,ARG
RETURN 1

;　最大攻撃回数
@SKILL_MAXATTACKNUMBER_1922,ARG
RETURN 1

;　ＨＰ2を支払うかＭＰ3を支払うか
@SKILL_COSTTYPE_1922, ARG
RETURN 3
;消費なし＝1

;　コストの量
@SKILL_COST_1922,ARG
RETURN 1

;　相性
@SKILL_TYPE_1922,ARG
RETURN 16
;Nuclr

;　1EXTRAか2MAGICか
@SKILL_DECIDE_TYPE_1922
RETURN 2
;魔法

;　1物理ダメージで計算するか2魔法ダメージで計算するか
@SKILL_DAMAGETYPE_1922
RETURN 2


;　対象が1敵か2味方か3場全体か4特殊か
@SKILL_TARGET_1922
RETURN 1

;　射程
@SKILL_RANGE_1922,ARG
RETURN 3

;　範囲　1単体　21列　3全体　4　敵味方全体
@SKILL_SPHERE_1922,ARG
RETURN 3

;　 効果のタイプ　1攻撃、2回復、3状態変化、4特殊、5状態回復
@SKILL_EFECT_1922
RETURN 1

;　 特殊処理の有無、及び処理内容
@SKILL_SPECIALEFECT_1922,ARG
;ARG = 1だった場合は処理の有無を返します
SIF ARG == 1
	RETURN 0
;以下に特殊な処理を記述します

;継承タイプ
@SKILL_SUCCESSION_TYPE_1922
RETURN 19

@継承不能_1922
RETURN 1
;不可

;速度補正
@SKILL_SPEED_1922, ARG = -1
RETURN 0

;　口上の表示とそのタイプ
@SKILL_MESSAGE_1922,ARG

;戦闘時以外に使用できるか
@SKILL_ACTIONABLE_FIELD_1922
RETURN 0

;スキル変化？用ランク
@SKILL_RANK_1922
RETURN 0



