;====================================================
;スキル及び行動の実行
;====================================================
@ACTION_1139,ARG,ARG:1
;CALL PAY_COST,ARG,1139


IF CFLAG:(ARG):PTフラグ == 0
	LOCAL:7 = 1
	LOCAL:8 = 7
ELSE
	LOCAL:7 = 7
	LOCAL:8 = 17
ENDIF

FOR LOCAL,LOCAL:7,LOCAL:8
	LOCALS = ポジション{LOCAL}
	SIF FLAG:LOCALS < 0
		CONTINUE
	IF CFLAG:(FLAG:LOCALS):ステート == GET_STATE_NUM("BIND") || CFLAG:(FLAG:LOCALS):ステート == GET_STATE_NUM("POISON") || CFLAG:(FLAG:LOCALS):ステート == GET_STATE_NUM("CLOSE")
		PRINTFORM 　　TARGET:[{LOCAL,2}] %CALLNAME:(FLAG:LOCALS),20,LEFT%　>>>>>>　
		CALL ATTACK_BADSTATE,ARG,FLAG:LOCALS,GET_STATE_NUM("DYING"),8,200,95
	ELSE
		CONTINUE
	ENDIF
NEXT


