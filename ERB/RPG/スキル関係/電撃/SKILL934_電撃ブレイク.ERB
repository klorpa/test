;---------------------------------
;ウェイトスキルは専用コマンドから実行させ
;そこでほとんど処理するので、個別の設定はほぼなしで
;---------------------------------
;スキル名
@SKILL_NAME_934,ARG
RESULTS = Void Elec
;　命中
@SKILL_HITRATE_934, ARG
RETURN 0

;　威力
@SKILL_POWER_934, ARG
RETURN 0

;　最低攻撃回数
@SKILL_MINATTACKNUMBER_934,ARG
RETURN 0

;　最大攻撃回数
@SKILL_MAXATTACKNUMBER_934,ARG
RETURN 0

;　ＨＰ2を支払うかＭＰ3を支払うか
@SKILL_COSTTYPE_934, ARG
RETURN 1
;消費なし＝1

;　コストの量
;MAXMPの1%
@SKILL_COST_934,ARG
RETURN 2

;　相性
@SKILL_TYPE_934,ARG
RETURN 0

;　1EXTRAか2MAGICか
@SKILL_DECIDE_TYPE_934
RETURN 2
;どちらでもない

;　1物理ダメージで計算するか2魔法ダメージで計算するか
@SKILL_DAMAGETYPE_934
RETURN 1


;　対象が1敵か2味方か3場全体か4特殊か
@SKILL_TARGET_934
RETURN 4

;　射程
@SKILL_RANGE_934,ARG
RETURN 1

;　範囲　1単体　21列　3全体　4　敵味方全体
@SKILL_SPHERE_934,ARG
RETURN 4

;　 効果のタイプ　1攻撃、2回復、3状態変化、4特殊、5状態回復
@SKILL_EFECT_934
RETURN 4

;継承タイプ
@SKILL_SUCCESSION_TYPE_934
RETURN GET_SUCCESSION_NUM("電撃")
;継承不可

;速度補正
@SKILL_SPEED_934, ARG = -1
RETURN 0

;　口上の表示とそのタイプ
@SKILL_MESSAGE_934,ARG

;戦闘時以外に使用できるか
@SKILL_ACTIONABLE_FIELD_934
RETURN 0

;スキル変化？用ランク
@SKILL_RANK_934
RETURN 0
;変化不可

@ウェイトスキル_934
RETURN 1
;戦闘中に使用できるか
@SKILL_ACTIONABLE_BATTLE_934
RETURN 0
;====================================================
;スキル及び行動の実行
;====================================================
@ACTION_934,ARG,ARG:1

;====================================================
;実行内容ここまで
;====================================================
;====================================================
;戦闘中以外での実行
;====================================================
@ACTION_FIELD_934,ARG,ARG:1
@SKILL_EXPLAIN_934
PRINTL For 1 turn、nullify enemy Elect attacks

;使用条件
@SKILL_SPECIAL_ACTIONABLE_934,ARG
RETURN 0

