@戦闘能力修正_3605,ARG,ARG:1 = -1

SELECTCASE ARG
	CASE 2
		SIF ARG:1 == -1
			RETURN 13
		IF EQUIP:(ARG:1):頭 == 3205 && EQUIP:(ARG:1):足 == 4603
			LOCAL = BASE:(ARG:1):LV * 3 / 2 + 13
			SIF TALENT:(ARG:1):献身的
				LOCAL = LOCAL * 5 / 4
			RETURN LOCAL
		ELSE
			RETURN 13
		ENDIF
	CASE 3
		SIF ARG:1 == -1
			RETURN 4
		IF EQUIP:(ARG:1):頭 == 3205 && EQUIP:(ARG:1):足 == 4603
			LOCAL = BASE:(ARG:1):LV / 2
			SIF TALENT:(ARG:1):献身的
				LOCAL = LOCAL * 5 / 4
			RETURN LOCAL
		ELSE
			RETURN 4
		ENDIF
ENDSELECT
RETURN 0

