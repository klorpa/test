;==========================================================
;インストールソフト装備
;==========================================================
@SETUP_SOFT,ARG = 500,ARG:1 = 0,ARG:2 = 0
; P LOCAL:5
; A LOCAL:6
; Q  I
LOCAL:5 = 0
;表示させるキャラを抽出（I:*に表示するキャラの登録番号、LOCAL:2に人数）
VARSET I, -1
LOCAL:6 = 0
FOR LOCAL,ARG,600
	SIF ITEM:(LOCAL) < 1
		CONTINUE
	IF ARG:1 == 1
		SIF SOFT_IS_FUSIONSOFT(LOCAL, ARG:2) == 0
			CONTINUE
	ENDIF
	I:(LOCAL:6) = LOCAL
	LOCAL:6 += 1
NEXT
LOCAL:2 = LOCAL:6


;PRINTFORML %" " * 31% 必要容量


LOCAL:9 = LINECOUNT
$PRINT_LIST
CLEARLINE LINECOUNT - LOCAL:9
CUSTOMDRAWLINE =
FOR LOCAL,LOCAL:5*20,MIN(LOCAL:5*20+20,LOCAL:2)
	IF I:LOCAL && ITEMNAME:(I:LOCAL) != ""
		PRINTFORM [{I:LOCAL,2}] %ITEMNAME_E(I:LOCAL),20,LEFT% \@ EQUIP:MASTER:(ITEMNAME:(I:LOCAL)) ? [ ON] # [OFF] \@
		CALLFORM SOFT_EXPLAIN_{I:LOCAL}
	ENDIF
NEXT
PRINTL
DRAWLINE
PRINTFORML ＣＯＭＰ-Capacity：{NUM_NAKAMA() + ソフト容量()}/{FLAG:ＣＯＭＰ容量} Used  Pick a software to toggle it's use　＜page.{LOCAL:5}＞
DRAWLINE
PRINTFORMLC \@(LOCAL:5 <= 0) ? %" " * 16% # [1007]Previous page\@
PRINTLC [1000]Return
PRINTFORMLC \@(LOCAL:5 >= (LOCAL:2 - 1) / 20) ? %" " * 16% # [1009]Next page\@
PRINTL 

$INPUT_LOOP_1
INPUT
IF RESULT == 1000
	RETURN 0
ELSEIF RESULT == 1007
	IF LOCAL:5 > 0
		LOCAL:5 -= 1
		GOTO PRINT_LIST
	ELSE
		GOTO INPUT_LOOP_1
	ENDIF
ELSEIF RESULT == 1009
	LOCAL = (LOCAL:2 - 1) / 20
	IF LOCAL:5 < LOCAL
		LOCAL:5 += 1
		GOTO PRINT_LIST
	ELSE
		GOTO INPUT_LOOP_1
	ENDIF
ELSEIF RESULT < 500 || RESULT >= 600 || MATCH(I,RESULT) == 0
	GOTO INPUT_LOOP_1
ENDIF
LOCAL = RESULT
IF SOFT_SIZE(LOCAL) > -1
	IF EQUIP:MASTER:(ITEMNAME:(LOCAL))
		EQUIP:MASTER:(ITEMNAME:(LOCAL)) = 0
	ELSEIF FLAG:ＣＯＭＰ容量-NUM_NAKAMA()-ソフト容量() >= SOFT_SIZE(LOCAL) && ITEM:(LOCAL)
		EQUIP:MASTER:(ITEMNAME:(LOCAL)) = 1
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_1
	ENDIF
ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_1
ENDIF
GOTO PRINT_LIST

