;=======================================================================
;エンカウント処理
;=======================================================================
@CHECK_ENCOUNT_26
IF RAND:100 < FLAG:エンカウント率
	CALL ENEMY_TABLE
	RETURN 1
ELSE
	FLAG:未遭遇歩数 += 1
	RETURN 0
ENDIF
;=======================================================================
;敵の種族の種類数
;=======================================================================
@CLASS_NUM_26
RETURN CLASS_NUM_COMMON("上級")
;=======================================================================
;敵の総数
;ARGは出現する敵の種類です
;=======================================================================
@ENEMY_NUM_26, ARG
RETURN ENEMY_NUM_COMMON("上級", ARG)
;=======================================================================
;敵の種類
;=======================================================================
@ENEMY_26, ARG
SELECTCASE FLAG:現M
	CASE IS < 9
		SELECTCASE RAND:100
			CASE IS < 30
				;30%でサラシナヒメ
				RETURN 605,30+RAND:(1+MAX((FLAG:現M-1) % 4,0)*3)
			CASE IS < 55
				;25%でフジムスメ
				RETURN 507,31+RAND:(1+MAX((FLAG:現M-1) % 4,0)*3)
				ABL:(CHARANUM-1):スキル4 = 106
			CASE IS < 70
				;15%でセンリ
				RETURN 806,33+RAND:(1+MAX((FLAG:現M-1) % 4,0)*3)
			CASE IS < 85
				;15%でハクジョウシ
				RETURN 705,34+RAND:(1+MAX((FLAG:現M-1) % 4,0)*3)
			CASE IS < 100
				;10%でおしち
				RETURN 1008,35+RAND:(1+MAX((FLAG:現M-1) % 4,0)*3)
		ENDSELECT
ENDSELECT
