;=======================================================================
;[5]調べる　実行時のイベント
;=======================================================================
@EVENT_CHECK_DUNGEON_26

;==========
;出口の処理
;==========
IF DA:(FLAG:現X):(FLAG:現Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 3
	CALL TREASURE_26
	RETURN RESULT
	
;=======
;階段
;=======
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 6
	CALL UPSTAIRS_26
	RETURN RESULT

ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 7
	CALL DOWNSTAIRS_26
	RETURN RESULT

ELSE
	CALL MESSAGE_WINDOW_D, "", "＞Nothing seems to be here."
	RETURN 0
ENDIF

;=======================================================================
;タイル進入時のイベント
;=======================================================================
@EVENT_ENTER_DUNGEON_26,ARG
;ARG = 進入時の向き　0の場合ワープなど
;==========
;出口の処理
;==========
IF DA:(FLAG:現X):(FLAG:現Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 3
	CALL TREASURE_26
	RETURN RESULT

;========
;罠の処理
;========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 5
	CALL TRAP_26
	RETURN RESULT
;=======
;階段
;=======
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 6
	CALL UPSTAIRS_26
	RETURN RESULT

ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 7
	CALL DOWNSTAIRS_26
	RETURN RESULT


;==========
;扉の処理
;==========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 2
	CALL DUNGEON_DOOR,ARG
	RETURN 1


;========================
;イベント
;========================
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 4
	SELECTCASE FLAG:現M
		CASE 4
			CALL SHOW_PICTURE, "D"
			IF ダンジョンフラグ:(FLAG:現ダンジョン):0 > 2 || (ダンジョンフラグ:(FLAG:現ダンジョン):0 == 2 && ダンジョンフラグ:(FLAG:現ダンジョン):1 == 2)
				CALL MESSAGE_WINDOW_D, "", "＞Nodody is here..."
				;誰もいない……
				SIF ダンジョンフラグ:(FLAG:現ダンジョン):0 == 2 || ダンジョンフラグ:(FLAG:現ダンジョン):0 == 4
					CALL MESSAGE_WINDOW_D, "", "＞Susano-o seems to have gone to the rooftop."
					;スサノオは屋上に向かったようだ
				RETURN 1
			ELSEIF ダンジョンフラグ:(FLAG:現ダンジョン):0 == 0
				CALL MESSAGE_WINDOW_D, "", "＞A rough but good-looking man is standing here."
				;体格の良い、乱暴そうな男が立っている
				IF ダンジョンフラグ:(FLAG:現ダンジョン):1 == 0
					CALL MESSAGE_WINDOW_D, "Fury Susano-o", "Ah, you where sent by my brother!/...hm, no you weren't./I am Susano-o, the strongest of the Tianjin!"
					;むう、兄者の手先か！/……何だ、違うのか/俺はスサノオ、天津神最強の男よ！
					CALL MESSAGE_WINDOW_D, "Fury Susano-o", "姉者とどうしても一発やりたくてよォ、/兄者と協力してこのびるぢんぐに/封印したまではよかったんだが/あの野郎、最初に姉者とヤるのは自分だとか抜かしやがった！"
					CALL MESSAGE_WINDOW_D, "Fury Susano-o", "イイオンナってなぁつえー男のモンになるべきだ、そうだろ！？/つまり天津神最強の こ の 俺 様 に/姉者と最初に一発ヤる権利があるってのに、/あの野郎はそれが分かってねえ"
					CALL MESSAGE_WINDOW_D, "Fury Susano-o", "とはいえいつまでもこうやって睨み合ってちゃ/ヤれるもんもヤれねーんだよな…"
					CALL MESSAGE_WINDOW_D, "Fury Susano-o", "...oh, that's right./I will give you a quest./Will you go and defeat my brother to the east of here?"
					;……おっし、そうだ/それなりの駄賃くらいはくれてやるからよ、/ここの東にいる兄者をとっちめて来てくんねーか？
					CALL INPUT_SELECT_D, "[0] Yes/[1] 知るか/[2] I don't care/[3] Don't be silly /[4] I'll just take you down instead/[5] Why not let your older sister decide?", , , 2
					;はい/[1] 知るか/[2] どうでもいい/[3] ふざけるな /[4] とりあえず倒す/[5] 姉に選ばせたら？
					IF RESULT == 0
						CALL MESSAGE_WINDOW_D, "Fury Susano-o", "Yes, thats a good answer./Well, I'll make you my brother-in-law if you defeat him./I'm counting on you!"
						;おーし、いい返事だ/そうだな、上手くいったら俺の舎弟にしてやるよ/それじゃ任せたぜ！
						ダンジョンフラグ:(FLAG:現ダンジョン):0 = 1					
						CALL MESSAGE_WINDOW_D, "", "＞You left the room."
						;＞部屋を出た
						FLAG:現Y += 2
						RETURN 1
