;=======================================================================
;指定したマスに進入可能か
;=======================================================================
@CAN_ENTER_26,ARG,ARG:1
IF FLAG:現M == 0 && ARG == 9 && ARG:1 == 2
	IF ダンジョンフラグ:(FLAG:現ダンジョン):2 >= 1
		RETURN 1
	ENDIF
	IF (ダンジョンフラグ:(FLAG:現ダンジョン):0 >= 2 && ダンジョンフラグ:(FLAG:現ダンジョン):1 >= 2) || ダンジョンフラグ:(FLAG:現ダンジョン):0 == 4 || ダンジョンフラグ:(FLAG:現ダンジョン):1 == 4
		IF ダンジョンフラグ:(FLAG:現ダンジョン):2 == 0
			CALL MESSAGE_WINDOW_D, "", "＞The barrier preventing access to the rooftop elevator has been released."
			;屋上へと続くエレベーターの結界が解かれた
			ダンジョンフラグ:(FLAG:現ダンジョン):2 = 1
		ENDIF
		RETURN 1
	ELSE
		CALL MESSAGE_WINDOW_D, "", "＞There is a barrier preventing you from moving on."
		;結界が張られていてこの先に進めない
		RETURN 0
	ENDIF
	
ELSE
	CALL CAN_ENTER,ARG,ARG:1
	RETURN RESULT
ENDIF
