

;=======================================================================
;森本病院
;=======================================================================
;=======================================================================
;攻略条件
;=======================================================================
@PREREQUISITE_DUNGEON_55
SIF STRFLAG_NUM_EV("罪ルート進行", , , , [[イベント:ペルソナ２]]) == 12
	RETURN 1
RETURN 0



;=======================================================================
;ダンジョン名
;=======================================================================
@GET_DUNGEON_NAME_55
RESULTS = Xibalba

;=======================================================================
;指定したマスに進入可能か
;=======================================================================
@CAN_ENTER_55, ARG, ARG:1
#LOCALSIZE 2
SELECTCASE DA:ARG:(ARG:1)
	CASE 4002
		IF !STRFLAG_D("扉開放")
			IF FLAG:現Y == ARG:1 - 1
				CALL MESSAGE_WINDOW_D, "", "＞Doors won't open！"
				RETURN 0
			ELSE
				CALL MESSAGE_WINDOW_D, "", "＞Unlocked the door with the key"
				CALLF STRFLAG_D("扉開放", 1)
				RETURN 1
			ENDIF
		ELSE
			RETURN 1
		ENDIF
	CASE 4012
		IF !STRFLAG_D("Longinus5撃破")
			CALL MESSAGE_WINDOW_D, "", "＞You heard the rumbling sound of motor behind the door./＞Open the door and proceed?"
			CALL INPUT_YN_D
			IF RESULT == 1
				RETURN 0
			ELSE
				FOR LOCAL:1, 3, 13
					FOR LOCAL, 0, 7
						CALL SET_FLOOR_ANALYZE(2, FLAG:現ダンジョン, 4, LOCAL, LOCAL:1)
					NEXT
				NEXT
				CALL MESSAGE_WINDOW_D, "", "＞扉をあけて部屋に足を踏み入れた"
				RETURN 1
			ENDIF
		ELSE
			RETURN 1
		ENDIF
	CASE 7002
		IF !STRFLAG_D("Longinus1撃破")
			CALL MESSAGE_WINDOW_D, "", "＞＞You heard the rumbling sound of motor behind the door/＞Open the door and proceed?"
			CALL INPUT_YN_D
			IF RESULT == 0
				FOR LOCAL:1, 9, 16
					FOR LOCAL, 3, 8
						CALL SET_FLOOR_ANALYZE(2, FLAG:現ダンジョン, 7, LOCAL, LOCAL:1)
					NEXT
				NEXT
				CALL MESSAGE_WINDOW_D, "", "＞扉をあけて部屋に足を踏み入れた"
				RETURN 1
			ELSE
				RETURN 0
			ENDIF
		ELSE
			RETURN 1
		ENDIF
	CASE 8002
		CALL MESSAGE_WINDOW_D, "", "＞扉の向こうから異様な気配がする…/＞扉を開けて進みますか？"
		CALL INPUT_YN_D
		IF RESULT == 0
			CALL DUNGEON_WORP_ID, 9, 9014
			RETURN 0
		ELSE
			RETURN 0
		ENDIF
	CASE 8012
		IF !STRFLAG_D("ボロンティック撃破")
			CALL MESSAGE_WINDOW_D, "", "＞扉には謎の文字が刻まれている…/＞なにやら妙な気配がするが…/＞扉を開けて進みますか？"
			CALL INPUT_YN_D
			IF RESULT == 0
				FOR LOCAL:1, 8, 21
					FOR LOCAL, 18, 27
						CALL SET_FLOOR_ANALYZE(2, FLAG:現ダンジョン, 8, LOCAL, LOCAL:1)
					NEXT
				NEXT
				CALL MESSAGE_WINDOW_D, "", "＞扉をあけて部屋に足を踏み入れた"
				RETURN 1
			ELSE
				RETURN 0
			ENDIF
		ELSE
			RETURN 1
		ENDIF
	CASEELSE
		CALL CAN_ENTER, ARG, ARG:1
		RETURN RESULT
ENDSELECT

