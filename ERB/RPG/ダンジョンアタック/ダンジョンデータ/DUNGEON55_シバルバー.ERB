;----------------------------------------------
;フューラー
;----------------------------------------------
@SET_ACTION_フューラー, ARG
#LOCALSIZE 3
;入力行動を-1とターゲットを-1にしておく
CFLAG:ARG:入力行動 = -1
CFLAG:ARG:ターゲット = -1
SIF 現HP割合(ARG) <= 25
	CFLAG:ARG:行動回数 = 3
IF !TFLAG:敵オメガクラスタ
	CFLAG:ARG:入力行動 = [[スキル:オメガクラスタ]]
ELSEIF CFLAG:ARG:１moreフラグ == 2
	CFLAG:ARG:入力行動 = [[スキル:聖櫃]]
ELSEIF CFLAG:ARG:行動した回数 == 0
	;聖杯のクール制御
	TFLAG:435++
	;波紋チェック
	LOCAL:1 = 0
	;ンダは-8まで容認
	SIF MINARRAY(CFLAG:ARG:0, GETNUM(CFLAG, "攻撃強化"), GETNUM(CFLAG, "BS付着率強化") + 1) < -8
		LOCAL:1 = 1
	;味方のうち一人から判定する
	LOCAL = POS(RAND(1, 7))
	IF LOCAL >= 0
		;カジャは8まで容認
		SIF MAXARRAY(CFLAG:LOCAL:0, GETNUM(CFLAG, "攻撃強化"), GETNUM(CFLAG, "BS付着率強化") + 1) > 8
			LOCAL:1 = 1
		;反射も波紋対象
		SIF CFLAG:LOCAL:物理反射フラグ || CFLAG:LOCAL:魔法反射フラグ
			LOCAL:1 = 2
	ENDIF
	;波紋フラグが立っている時
	IF (LOCAL:1 == 1 && RAND:2) || (LOCAL:1 == 2 && !RAND:3)
		CFLAG:ARG:入力行動 = [[スキル:泡沫の波紋]]
		TFLAG:435 += 3
	;1/3で反射は聖櫃送り
	ELSEIF LOCAL:1 == 2 && RAND:2
		CFLAG:ARG:入力行動 = [[スキル:聖櫃]]
	ELSE
		SELECTCASE RAND:100
			CASE IS < 50
				CFLAG:ARG:入力行動 = [[スキル:聖槍]]
			CASE IS < 75
				CFLAG:ARG:入力行動 = [[スキル:ＭＧ３４]]
			CASEELSE
				CFLAG:ARG:入力行動 = [[スキル:聖櫃]]
		ENDSELECT
	ENDIF
ELSEIF CFLAG:ARG:行動した回数 == 1
	SELECTCASE RAND:100
		CASE IS < 50
			;聖杯はクール期間がある
			IF TFLAG:435 < 3
				CFLAG:ARG:入力行動 = [[スキル:聖槍]]
			ELSE
				CFLAG:ARG:入力行動 = [[スキル:聖杯]]
				TFLAG:435 = 0
			ENDIF
		CASE IS < 75
			CFLAG:ARG:入力行動 = [[スキル:ＭＧ３４]]
		CASEELSE
			CFLAG:ARG:入力行動 = [[スキル:ヴリルブリッツ]]
	ENDSELECT
ELSE
	SELECTCASE RAND:100
		CASE IS < 25
			CFLAG:ARG:入力行動 = [[スキル:聖槍]]
		CASE IS < 50
			CFLAG:ARG:入力行動 = [[スキル:ＭＧ３４]]
		CASE IS < 75
			CFLAG:ARG:入力行動 = [[スキル:聖櫃]]
		CASEELSE
			CFLAG:ARG:入力行動 = [[スキル:ヴリルブリッツ]]
	ENDSELECT
ENDIF

;アクション実行不能
CALL CHECK_ACTIONABLE, ARG, CFLAG:ARG:入力行動
SIF RESULT == 0
	CFLAG:ARG:入力行動 = 0
;ここまでターゲットが決まってなかったら、ランダムターゲットをよんで決定
SIF CFLAG:ARG:ターゲット == -1
	CALL RANDOM_TARGET, ARG, CFLAG:ARG:入力行動
RETURN 1


