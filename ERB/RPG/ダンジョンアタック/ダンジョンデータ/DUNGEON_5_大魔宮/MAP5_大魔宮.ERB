;共通
;-----------------------------------------------------------------------
;F イベント床
SIF ARGS == "F"
	RETURN 11
;G イベント扉
SIF ARGS == "G"
	RETURN 12
;H イベントダークゾーン
SIF ARGS == "d"
	RETURN -11
;一方通行
SIF ARGS == "A"
	RETURN 902
SIF ARGS == ">"
	RETURN 922
SIF ARGS == "V"
	RETURN 942
SIF ARGS == "<"
	RETURN 962
;落とし穴
SIF ARGS == "P"
	RETURN 15
;HP罠
SIF ARGS == "H"
	RETURN 25
;MP罠
SIF ARGS == "M"
	RETURN 35
;店
SIF ARGS == "S"
	RETURN 989
;出口
SIF ARGS == "E"
	RETURN 999
RETURN 0
;=======================================================================
;タイル調整
;=======================================================================
@MAKE_FLOOR_LINE_COMMON_5, ARG, ARG:1, ARG:2
#LOCALSIZE 30
;LOCAL:3 宝箱 ダンジョンフラグ:75~99
;LOCAL:4 イベント
;LOCAL:8 メッセンジャー
;(0/0)でLOCALを消去
SIF ARG:1 == 0 && ARG:2 == 0
	VARSET LOCAL
	
IF ARG == 1
	SIF RANGE(FLAG:現M, 60, 64)
		DA:(ARG:1):(ARG:2) = -1
	;→
	IF ARG:1 == (FLAG:最大X -1)
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:16 * 100 + 16
		LOCAL:16++
	;←
	ELSEIF ARG:1 == 0
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:17 * 100 + 17
		LOCAL:17++
	;↓
	ELSEIF ARG:2 == (FLAG:最大Y - 1)
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:26 * 100 + 26
		LOCAL:26++
	;↑
	ELSEIF ARG:2 == 0
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:27 * 100 + 27
		LOCAL:27++
	ENDIF
ENDIF
;イベントダークゾーン
IF ARG == -11
	DA:(ARG:1):(ARG:2) = -(FLAG:現M * 1000 + LOCAL:29 * 10 + 1)
	LOCAL:29++
	SELECTCASE -DA:(ARG:1):(ARG:2) / 10
		CASE 700
			;SIF STRFLAG_D("盾ゲット")
			SIF ITEM:ガルの盾
				DA:(ARG:1):(ARG:2) = -1
		;ガイアは銀のレリーフ獲得で姿を消す
		CASE 2501
			SIF ITEM:銀のレリーフ
				DA:(ARG:1):(ARG:2) = -1
		CASE 3801
			SIF STRFLAG_D("セト撃破")
				DA:(ARG:1):(ARG:2) = -1
		CASE 6001
			SIF STRFLAG_D("Y子")
				DA:(ARG:1):(ARG:2) = -1
	ENDSELECT
ENDIF
;イベント床
IF ARG == 11
	DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:1 * 10 + 1
	LOCAL:1++
ENDIF
;イベント扉
IF ARG == 12
	DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:2 * 10 + 2
	LOCAL:2++
ENDIF
IF ARG == 3
	IF GETBIT(ダンジョンフラグ:(FLAG:現ダンジョン):(FLAG:現M+25), LOCAL:3)
		DA:(ARG:1):(ARG:2) = 1
	ELSE
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:3 * 10 + 3
	ENDIF
	LOCAL:3++
	;未実装領域の宝箱を撤廃
	SIF FLAG:現M > 40
		DA:(ARG:1):(ARG:2) = 1
ENDIF
IF ARG == 4
	DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:4 * 10 + 4
	LOCAL:4++
	SELECTCASE DA:(ARG:1):(ARG:2) / 10
		CASE 100
			;朱実
			SIF STRFLAG_D("朱実会話")
				DA:(ARG:1):(ARG:2) = 1
		CASE 600
			;ネコマタ
			SIF STRFLAG_D("ネコマタ会話") || !STRFLAG_D("ガル依頼") || ITEM:ガルの盾
			 	DA:(ARG:1):(ARG:2) = 1
		CASE 700
			;ミノタウロス
			SIF STRFLAG_D("ミノタウロス撃破")
				DA:(ARG:1):(ARG:2) = 1
		CASE 900
			;ロキ
			SIF STRFLAG_D("ロキ撃破")
				DA:(ARG:1):(ARG:2) = 1
		;冥界
		;CASE 1000
		;	SIF STRFLAG_D("Y子")
		;		DA:(ARG:1):(ARG:2) = 1
		CASE 1001
			;リック
			SIF ITEM:リックの腕輪
				DA:(ARG:1):(ARG:2) = 1
		CASE 1400
			;SIF STRFLAG_D("タバサの像")
			SIF ITEM:タバサの像
				DA:(ARG:1):(ARG:2) = 1
		CASE 1600
			SIF STRFLAG_D("メデューサ撃破")
				DA:(ARG:1):(ARG:2) = 1
		CASE 1601
			SIF STRFLAG_D("Hinokagutsuchi")
				DA:(ARG:1):(ARG:2) = 1
		CASE 2600
			SIF STRFLAG_D("シャドウ撃破")
				DA:(ARG:1):(ARG:2) = 1
		CASE 2800
			SIF ITEM:ラトスの像
				DA:(ARG:1):(ARG:2) = 1
		CASE 3000, 3001
			SIF STRFLAG_D("ヘカーテ撃破")
				DA:(ARG:1):(ARG:2) = 1
		CASE 3800
			SIF ITEM:ルースの小瓶
				DA:(ARG:1):(ARG:2) = 1
	ENDSELECT
ENDIF
IF ARG == 6
	IF ARG:1 == (FLAG:最大X -1)
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:16 * 100 + 16
		LOCAL:16++
	ELSEIF ARG:2 == (FLAG:最大Y - 1)
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:26 * 100 + 26
		LOCAL:26++
	ELSE
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:6 * 100 + 6
		LOCAL:6++
	ENDIF
ENDIF
IF ARG == 7
	IF ARG:1 == 0
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:17 * 100 + 17
		LOCAL:17++
	ELSEIF ARG:2 == 0
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:27 * 100 + 27
