	CASEELSE
		GOTO INPUT_LOOP
ENDSELECT
SIF !STRFLAG_D(@"M{LOCAL}", 0, [[ダンジョン:大魔宮]])
	GOTO INPUT_LOOP
CALL DUNGEON_WORP_ID, LOCAL, 999, 1

;=======================================================================
;指定したマスに進入可能か
;=======================================================================
@CAN_ENTER_5,ARG,ARG:1
SELECTCASE DA:ARG:(ARG:1)
	;∧
	CASE 902, 912
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		SIF FLAG:現Y < ARG:1
			RETURN 0
		FLAG:現Y -= 2
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		FLAG:現Y += 2
	;＞
	CASE 922, 932
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		SIF FLAG:現X > ARG
			RETURN 0
		FLAG:現X += 2
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		FLAG:現X -= 2
	;∨
	CASE 942, 952
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		SIF FLAG:現Y > ARG:1
			RETURN 0
		FLAG:現Y += 2
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		FLAG:現Y -= 2
	;＜
	CASE 962, 972
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		SIF FLAG:現X < ARG
			RETURN 0
		FLAG:現X -= 2
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		FLAG:現X += 2
	CASE 992
		PRINTW ＞扉は開かない
		RETURN 0
	CASEELSE
		;2以外の扉はドアイベントへ
		IF DA:ARG:(ARG:1) % 10 == 2 && DA:ARG:(ARG:1) > 2
			CALL DOOR_5, DA:ARG:(ARG:1) / 10
			RETURN RESULT
		ENDIF
ENDSELECT
CALL CAN_ENTER,ARG,ARG:1
RETURN RESULT

