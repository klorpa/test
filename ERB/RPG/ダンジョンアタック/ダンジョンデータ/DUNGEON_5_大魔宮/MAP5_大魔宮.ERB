;=======================================================================
;攻略条件
;=======================================================================
@PREREQUISITE_DUNGEON_5
;大魔宮への誘いクリア
IF イベントフラグ:3:0 == 1
	RETURN 1
ELSE
	RETURN 0
ENDIF

;=======================================================================
;ダンジョン名
;=======================================================================
@GET_DUNGEON_NAME_5
RESULTS = 大魔宮

;=======================================================================
;フロア名表示
;=======================================================================
@FLOORNAME_5
SELECTCASE FLAG:現M
	CASE IS < 8
		PRINTFORML ダイダロスの塔  %TOFULL(TOSTR(8-FLAG:現M))%Ｆ
	CASE 9
		PRINTL ヴァルハラの回廊  １Ｆ
	CASE 10
		PRINTL ヴァルハラの回廊  Ｂ１Ｆ
	CASE 12
		PRINTL 天空の街ビエン  Ｂ１Ｆ
	CASE IS < 17
		PRINTFORML 天空の街ビエン  %TOFULL(TOSTR(FLAG:現M-12))%Ｆ
	CASE IS < 26
		PRINTFORML マズルカの回廊  %TOFULL(TOSTR(FLAG:現M-17))%Ｆ
	CASE IS < 31
		PRINTFORML マズルカの回廊  %TOFULL(TOSTR(33-FLAG:現M))%Ｆ
	CASE 32, 39
		PRINTL 炎の腐海  ２F
	CASE 33, 38
		PRINTL 炎の腐海  １Ｆ
	CASE 34, 37
		PRINTL 炎の腐海  Ｂ１Ｆ
	CASE 35, 36 
		PRINTL 炎の腐海  Ｂ２Ｆ
	CASE 41
		PRINTL 青の大広間  Ｂ１Ｆ
	CASE 42
		PRINTL 青の大広間  １Ｆ 
	CASE 43
		PRINTL 青の大広間  ２Ｆ
	CASE 44
		PRINTL 青の大広間  ３Ｆ  
	CASE IS < 47
		PRINTFORML 赤の塔  %TOFULL(TOSTR(FLAG:現M-38))%Ｆ
	CASE 60
		PRINTL 冥界
ENDSELECT





;=======================================================================
;スタート地点入力
;=======================================================================
