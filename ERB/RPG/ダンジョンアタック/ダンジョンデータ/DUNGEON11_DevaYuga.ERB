			CALL DUNGEON_DOWNSTAIRS,1,2,17
	CASE 3
		SIF FLAG:現X == 17 && FLAG:現Y == 5
			CALL DUNGEON_DOWNSTAIRS,2,17,5
	CASE 4
		SIF FLAG:現X == 6 && FLAG:現Y == 17
			CALL DUNGEON_DOWNSTAIRS,3,6,17
