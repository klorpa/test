;=======================================================================
;タイル進入時のイベント
;=======================================================================
@EVENT_ENTER_DUNGEON_42,ARG
;ARG = 進入時の向き　0の場合ワープなど
;==========
;出口の処理
;==========
IF DA:(FLAG:現X):(FLAG:現Y) == 8
	IF ダンジョンフラグ:42:新宿フラグ("ベル・デル進行度") == 1 || ダンジョンフラグ:42:新宿フラグ("ベル・デル進行度") == 3
		IF FLAG:直哉ルート
			PRINTW ＞You should go meet Naoya at the Gaean church.
			;ガイア教会でナオヤに会おう
		ELSE
			PRINTW ＞
		ENDIF
		RETURN 0
	ELSEIF ダンジョンフラグ:42:新宿フラグ("ベル・デル進行度") == 2
		IF FLAG:直哉ルート
			PRINTW ＞Find the boy with the knitted hat in the north of the city.
			;街の北にいるニット帽の少年を探そう
		ELSE
			PRINTW ＞
		ENDIF
		RETURN 0
	ENDIF
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 3
	SELECTCASE FLAG:現M
		CASE 4
			IF FLAG:現X == 5 && FLAG:現Y == 4
				CALL GET_TREASURE,1031,2,FLAG:現ダンジョン,32
			ELSEIF FLAG:現X == 9 && FLAG:現Y == 7
				CALL GET_TREASURE,1019,2,FLAG:現ダンジョン,33
			ENDIF

	ENDSELECT
	RETURN 1
;========
;イベント
;========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 4
	CALL EVENT_42
	RETURN 1
;=======
;階段
;=======
ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 6 || DA:(FLAG:現X):(FLAG:現Y) % 10 == 7
	CALL STAIR_42
	RETURN 1
ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 9
	SELECTCASE FLAG:現M
		CASE 0
			IF FLAG:現X == 1 && FLAG:現Y == 19
				CALL DUNGEON_WORP,1,1,1
			ELSEIF FLAG:現X == 7 && FLAG:現Y == 19
				CALL DUNGEON_WORP,1,7,1
			ELSEIF FLAG:現X == 11 && FLAG:現Y == 19
				CALL DUNGEON_WORP,1,11,1
			ELSEIF FLAG:現X == 18 && FLAG:現Y == 19
				CALL DUNGEON_WORP,1,18,1
			ELSEIF FLAG:現X == 15 && FLAG:現Y == 11
				;アマラ宇宙への入り口
				CALL DUNGEON_WORP,9,3,3
			ELSEIF FLAG:現X == 13 && FLAG:現Y == 18
				CALL DUNGEON_TERMINAL
				RETURN 1
			ENDIF
		CASE 1
			IF FLAG:現X == 1 && FLAG:現Y == 0
				CALL DUNGEON_WORP,0,1,18
			ELSEIF FLAG:現X == 7 && FLAG:現Y == 0
				CALL DUNGEON_WORP,0,7,18
			ELSEIF FLAG:現X == 11 && FLAG:現Y == 0
				CALL DUNGEON_WORP,0,11,18
			ELSEIF FLAG:現X == 18 && FLAG:現Y == 0
				CALL DUNGEON_WORP,0,18,18
			ENDIF
		CASE 9
			SIF FLAG:現X == 1 && FLAG:現Y == 1
				CALL DUNGEON_WORP,1,5,7
		CASEELSE
			CALL DUNGEON_TERMINAL
			RETURN 1
	ENDSELECT
	RETURN 1
	



;==========
;扉の処理
;==========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 2
	CALL DUNGEON_DOOR,ARG
	RETURN 1
RETURN 0
ENDIF
RETURN 0

