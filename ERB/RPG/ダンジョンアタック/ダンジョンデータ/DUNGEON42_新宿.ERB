	IF TIME == 3
		ダンジョンフラグ:42:新宿フラグ("朝夜")= 2
		;PRINTFORMW 夜です永眠してください
	ELSE
		ダンジョンフラグ:42:新宿フラグ("朝夜")= 1
		;PRINTFORMW 朝です死んでください
	ENDIF
ENDIF
	
IF FLAG:現M == 0 && ARG == 17 && ARG:1 == 4
 	IF ダンジョンフラグ:42:新宿フラグ("ベル・デル進行度") == 2 && FLAG:百合の花ルート
		PRINTW ＞You shouldn't return to the Lily Community Chest office now!
		;今は百合の花共同募金事務所に戻るべきではない！
		RETURN 0
	ELSEIF ダンジョンフラグ:42:新宿フラグ("百合の花依頼") == 0
		PRINTW ＞The door seems to be locked.
		;扉には鍵がかけられているようだ
		RETURN 0
	ELSEIF ダンジョンフラグ:42:新宿フラグ("百合の花依頼") > 1 
		RETURN 1
	ENDIF
ENDIF
IF FLAG:現M == 1 && ARG == 2 && ARG:1 == 9
	IF ダンジョンフラグ:42:新宿フラグ("薬イベント進行度") < 1
		PRINTW ＞The door seems to be locked.
		RETURN 0
	ELSEIF ダンジョンフラグ:42:新宿フラグ("薬イベント進行度") >= 1
		RETURN 1
	ENDIF
ENDIF
IF FLAG:現M == 1 && ARG == 10 && ARG:1 == 15
	IF ダンジョンフラグ:42:新宿フラグ("イベントクリアフラグ") < 1 || FLAG:百合の花ルート == 0
		PRINTL ＞The door doesn't open...
