			;---- EDIT 004 ADD END ---------------------------
			RETURN 1
		ENDIF
	ELSE
		;夜
		IF ダンジョンフラグ:42:新宿フラグ("朝夜") == 2
			PRINTFORMW 「Our priest has a day off. You should came back later.」
			;「司祭様はお休み中だ　昼に出直して来るといい」
		ELSEIF ダンジョンフラグ:42:新宿フラグ("朝夜") == 1
			PRINTFORMW 「Suspicous person, this is the mesiac church.」
			;「怪しい奴め、ここはメシア教会だ」
			PRINTFORMW 「I cannot let people without an indulgence inside.」
			;「免罪符の無い者を通すわけにはいかない」
			;ベス
;			IF FINDCHARA_B(4501,1) == 2 && (TALENT:(RESULT:1):恋慕 || TALENT:(RESULT:1):親愛)
;				PRINTFORML 「昔はこんなに警備が厳重じゃなかったのに」
