				RETURN [[キャラ:オロバス]]
			ELSEIF RAND:2 == 0
				RETURN [[キャラ:アラハバキ]]
			ELSE
				RETURN [[キャラ:ジャンバヴァン]]
			ENDIF
		ELSEIF ARG == 1
			
			IF (FLAG:現M == 29 && FLAG:現Y < 22) || (FLAG:現M == 30 && FLAG:現Y < 11)
				RETURN 35 + RAND:3
			ELSEIF FLAG:現M == 28
				RETURN 42 + RAND:3
			ELSEIF FLAG:現M < 31
				RETURN 39 + RAND:3
			ENDIF
		ENDIF
ENDSELECT

