;=======================================================================
;指定したマスに進入可能か
;=======================================================================
@airport_ENTER,ARG,ARG:1
;DA:ARG:(ARG:1)
;ARG、ARG:1はそれぞれ対象のマスのX、Y座標を表しています。
;このダンジョンではエレベーターの扉が一度右側から開けておかないと左側からは開けられない、という処理に使用しています。
SELECTCASE FLAG:現M
	CASE 23
		IF ARG == 10 && ARG:1 == 16 && ダンジョンフラグ:13:4 < 8
			CALL MESSAGE_WINDOW_D, "", "＞扉は閉ざされている"
			RETURN 0
		ENDIF
		IF ARG == 1 && ARG:1 == 7 && ダンジョンフラグ:13:4 > 6
			RETURN 0
		ENDIF
	CASE 9
		IF ARG == 3 && ARG:1 == 4 && ダンジョンフラグ:13:4 < 14
			CALL MESSAGE_WINDOW_D, "", "＞エレベーターだ"
			CALL MESSAGE_WINDOW_D, "", "＞扉は閉ざされている"
			RETURN 0
		ENDIF
ENDSELECT
RETURN 1

;=======================================================================
;空港タイル
;=======================================================================
;イベントタイル特殊表示4
@airport_TILE, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE 1500,1501,1502,2500,2501
		SETCOLOR 0x00ff00
		RESULTS = ｍ
	CASE 2600
		SETCOLOR 0x00ff00
		RESULTS = 真
	CASE 2601
		SETCOLOR 0x00ff00
		RESULTS = シ
	CASE 2602
		SETCOLOR 0x00ff00
		RESULTS = 祐
	CASE 2603
		SETCOLOR 0x00ff00
		RESULTS = ア
	CASE 2900
		SETCOLOR 0x00ff00
		RESULTS = 祐
	CASE 3000
		RESULTS = □
	CASE 3001
		SETCOLOR 0x00ff00
		RESULTS = シ
	CASE 3002
		IF ダンジョンフラグ:13:13 % 5 > 0
			SETCOLOR 0x00ff00
			RESULTS = Ｈ
		ELSE
			RESULTS = □
		ENDIF
	CASE 2701
		IF ダンジョンフラグ:13:13 % 7 > 0
			SETCOLOR 0x00ff00
			RESULTS = Ｈ
		ELSE
			RESULTS = □
		ENDIF
	CASE 2700
		SETCOLOR 0x00ff00
		RESULTS = 真
	CASE 2301
		IF ダンジョンフラグ:13:12 % 11 > 0
			SETCOLOR 0x00ff00
			RESULTS = Ｈ
		ELSE
			RESULTS = □
		ENDIF
	CASEELSE
		RESULTS = ！
ENDSELECT

;イベントタイル特殊表示9
@airport_TILE2, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE 2600
		RESULTS = ↓
	CASE 2900
		RESULTS = ↑
	CASE 1700,1701
		RESULTS = □
	CASE 1900
		IF ダンジョンフラグ:13:11 < 3
			RESULTS = ！
		ELSE
			RESULTS = □
		ENDIF
	CASE 2300
		RESULTS = 扉
	CASEELSE
		RESULTS = □
ENDSELECT

