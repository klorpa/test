						PRINTW 
						PRINTL 「I see…Chie was the same too…」
						;一緒じゃ無い
						PRINTL 「You don't seem like a bad person…I'll be in your care for a bit」
						PRINTW 
						PRINTL ＞With the cheerful Yukiko, you left the dungeon。
						PRINTW 
						CALL ADD_NEW_COMPANION,4561,100
						;戦闘参加可能で加入するが、調教で戦闘参加不可能になる
						SIF RESULT
							CFLAG:(RESULT:1):戦闘参加不可能 = -1
						ダンジョンフラグ:72:8 = 3
					ENDIF
				ENDIF
			ENDIF
			PRINTL
			PRINTL ＞You got ￥50000
			PRINTW
			MONEY += 50000
			DA:(FLAG:現X):(FLAG:現Y) = 1
			FLAG:脱出 = 1
			RETURN 1
		CASE 3
			IF FLAG:現X == 8 && FLAG:現Y == 4
				CALL DUNGEON_WORP,3,13,7
			ELSEIF FLAG:現X == 13 && FLAG:現Y == 8
				CALL DUNGEON_WORP,3,10,13
			ELSEIF FLAG:現X == 9 && FLAG:現Y == 13
				CALL DUNGEON_WORP,3,4,10
			ELSEIF FLAG:現X == 4 && FLAG:現Y == 9
				CALL DUNGEON_WORP,3,7,4
			ENDIF
	ENDSELECT
ELSEIF DA:(FLAG:現X):(FLAG:現Y) %10 == 9
	SELECTCASE DA:(FLAG:現X):(FLAG:現Y) /10
		CASE 0
			CALL DUNGEON_TERMINAL
			RETURN 1
	ENDSELECT
ENDIF
RETURN 0


