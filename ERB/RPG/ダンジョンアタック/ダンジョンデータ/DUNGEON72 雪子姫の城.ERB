;=======================================================================
;[5]調べる　実行時のイベント
;=======================================================================
@EVENT_CHECK_DUNGEON_72

;==========
;出口の処理
;==========
IF DA:(FLAG:現X):(FLAG:現Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
;% 10は10で割ったあまり、つまり下一桁がでてきます
;そして、下一桁はタイルの種類を表す
ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 3
	CALL TREASURE_72
	RETURN RESULT
;=======
;階段
;=======
ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 6
	CALL UPSTAIRS_72
	RETURN RESULT

ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 7
	CALL DOWNSTAIRS_72
	RETURN RESULT
ELSEIF DA:(FLAG:現X):(FLAG:現Y) %10 == 9
	SELECTCASE DA:(FLAG:現X):(FLAG:現Y) /10
	CASE 0
		CALL DUNGEON_TERMINAL
		RETURN 1
	CASE 1
		PRINTL Fairy Silky：
		PRINTL 「Fufu, you look like you're wondering how I can be here at my low level。」
		PRINTL 「With support magic, I can keep up with higher level opponents。」
		PRINTW 
		PRINTL 「Using -kaja spells like rakukaja and sukukaja。」
		PRINTW 「With support magic, even powerful enemies are no trouble。」
		PRINTW 「I hope you find a wonderful partner to cast support magic for you。」
	CASE 2
		PRINTL Yoma Lilim：
		PRINTL 「Rakunda! Tarunda！」
		PRINTL 「Mfufufufufu. Spoke too soon♪」
		PRINTW 
		PRINTL 「I'll just cast a bit of weakening magic and have no worries」
		PRINTL 「And I can carefully overwhelm strong opponents」
		PRINTW 
		PRINTL 「By the way, have you seen a summoner？」
		PRINTW 「Because I was told to explore, I went searching around, but now, I don't now where I've been」
	CASE 3
		PRINTL Natural Summoner：
		PRINTL 「Silky's rakukaja as a card…next…」
		PRINTL 「Ah, hello〜」
		PRINTW 
		PRINTL 「Skill cards are certainly handy〜I can have my favorite skills over and over」
		PRINTL 「When a not-so-pretty demon has a good skill」
		PRINTW 「just bring loyalty to 1000〜」
		PRINTL 「Alright! Then, Lilim! I'll make a card at Jakyou Manor…huh？」
		PRINTW 
		PRINTW 「I was in town a bit ago…well………」
		PRINTL 「E-excuse me！」
 		PRINTL 「Where…where am I！！！」
 		PRINTW 
	CASEELSE
		CALL DUNGEON_TERMINAL
		RETURN 1
	ENDSELECT
ELSE
	PRINTW Nothing seems to be here.
	RETURN 0
ENDIF






