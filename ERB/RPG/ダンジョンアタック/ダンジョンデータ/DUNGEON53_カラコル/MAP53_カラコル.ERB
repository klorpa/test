;=======================================================================
;タイル定義
;=======================================================================
@DEFINE_TILES_53, ARGS, ARG, ARG:1
;敵出現率0ゾーン
SIF ARGS == "N"
	RETURN 11
;イベントドア
SIF ARGS == "G"
	RETURN 12
SIF ARGS == "C"
	RETURN 989
SIF ARGS == "S"
	RETURN 999
;=======================================================================
;タイル調整
;=======================================================================
@MAKE_FLOOR_LINE_COMMON_53, ARG, ARG:1, ARG:2
;LOCAL:3 宝箱 ダンジョンフラグ:75~99
;LOCAL:4 イベント
;LOCAL:8 メッセンジャー
;(0/0)でLOCALを消去
SIF ARG:1 == 0 && ARG:2 == 0
	VARSET LOCAL
IF ARG == 1
	;→
	IF ARG:1 == (FLAG:最大X -1)
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:16 * 100 + 16
		LOCAL:16++
	;←
	ELSEIF ARG:1 == 0
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:17 * 100 + 17
		LOCAL:17++
	;↓
	ELSEIF ARG:2 == (FLAG:最大Y - 1)
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:26 * 100 + 26
		LOCAL:26++
	;↑
	ELSEIF ARG:2 == 0
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:27 * 100 + 27
		LOCAL:27++
	ENDIF
ENDIF
IF ARG == 3
	IF GETBIT(ダンジョンフラグ:(FLAG:現ダンジョン):(FLAG:現M+75), LOCAL:3)
		DA:(ARG:1):(ARG:2) = 1
	ELSE
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:3 * 10 + 3
	ENDIF
	LOCAL:3++
ENDIF
IF ARG == 4
	DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:4 * 10 + 4
	LOCAL:4++
	SELECTCASE DA:(ARG:1):(ARG:2) / 10
		CASE 0
			;入り口の帝国兵
			SIF STRFLAG_D("入り口帝国兵撃破")
				DA:(ARG:1):(ARG:2) = 1
		CASE 900
			SIF STRFLAG_D("ジョーカー撃破")
				DA:(ARG:1):(ARG:2) = 1
		CASE 1200
			SIF STRFLAG_D("聖槍騎士団撃破")
				DA:(ARG:1):(ARG:2) = 1
	ENDSELECT
ENDIF
IF ARG == 6
	DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:6 * 100 + 6
	LOCAL:6++
ENDIF
IF ARG == 7
	DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:7 * 100 + 7
	LOCAL:7++
ENDIF
IF ARG == 8
	DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:8 * 10 + 8
	LOCAL:8++
ENDIF
;イベント扉(G)
IF ARG == 12
	DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:12 * 10 + 12
	LOCAL:12++
ENDIF
