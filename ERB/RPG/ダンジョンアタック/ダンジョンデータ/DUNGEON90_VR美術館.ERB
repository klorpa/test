;スナッピーの思考パターン
@SET_ACTION_スナッピー,ARG
IF FLAG:経過ターン数 == 0
	CALL CHECK_ACTIONABLE,ARG,[[スキル:ドルフィンキック]]
	IF RESULT == 1
		CFLAG:ARG:入力行動 = [[スキル:ドルフィンキック]]
		CALL RANDOM_TARGET,ARG,[[スキル:ドルフィンキック]]
		RETURN 0
	ELSE
		GOTO DECIDE_ACTION
	ENDIF
ENDIF
SIF FLAG:経過ターン数 > 0
	CFLAG:ARG:行動回数 = 2

IF CFLAG:ARG:１moreフラグ == 2
	IF CFLAG:ARG:行動した回数 == 1
		CALL CHECK_ACTIONABLE,ARG,[[スキル:ドルフィンキック]]
		IF RESULT == 1
			CFLAG:ARG:入力行動 = [[スキル:ドルフィンキック]]
			CALL RANDOM_TARGET,ARG,[[スキル:ドルフィンキック]]
		ELSE
			GOTO DECIDE_ACTION
		ENDIF
	ELSE
			GOTO DECIDE_ACTION
	ENDIF
ELSEIF FLAG:経過ターン数 % 4 == 0 && CFLAG:ARG:行動した回数 == 0
	IF CFLAG:ARG:行動した回数 == 1
		CALL CHECK_ACTIONABLE,ARG,[[スキル:ドルフィンキック]]
		IF RESULT == 1
			CFLAG:ARG:入力行動 = [[スキル:ドルフィンキック]]
			CALL RANDOM_TARGET,ARG,[[スキル:ドルフィンキック]]
		ELSE
			GOTO DECIDE_ACTION
		ENDIF
	ELSE
			GOTO DECIDE_ACTION
	ENDIF
ELSE
	$DECIDE_ACTION
	CALL SEARCH_ACT,ARG
	IF RESULT == 0
		CFLAG:ARG:入力行動 = -1
		CFLAG:ARG:防御フラグ = 1
		RETURN 0
	ENDIF
	LOCAL = RAND:RESULT
	;スキル番号決定
	LOCAL:1 = S:(10+LOCAL)
	$TARGET_LOOP
	
	CALL RANDOM_TARGET,ARG,LOCAL:1
	CALLFORM SKILL_TARGET_{LOCAL:1}
	;ここでスキル番号を行動内容に代入
	SIF CFLAG:ARG:ターゲット == -1
		GOTO DECIDE_ACTION
	CFLAG:ARG:入力行動 = LOCAL:1
ENDIF


