;=======================================================================
;[5]調べる　実行時のイベント
;=======================================================================
@EVENT_CHECK_DUNGEON_90

;==========
;出口の処理
;==========
IF DA:(FLAG:現X):(FLAG:現Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
;% 10は10で割ったあまり、つまり下一桁がでてきます
;そして、下一桁はタイルの種類を表す
ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 3
	CALL TREASURE_90
	RETURN RESULT
;=======
;階段
;=======
ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 6
	CALL UPSTAIRS_90
	RETURN RESULT

ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 7
	CALL DOWNSTAIRS_90
	RETURN RESULT

ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 9
	CALL DUNGEON_TERMINAL
	RETURN 1


ELSEIF ダンジョンフラグ:90:2 == 1 && ダンジョンフラグ:90:3 == 1 && ダンジョンフラグ:90:4 == 1
	PRINTFORMW %NAME:MASTER%はポケットの中の駒を確認した
	PRINTFORMW 必要な駒は３つ全て揃っている……
	RETURN 0
ELSEIF ダンジョンフラグ:90:2 == 1 && ダンジョンフラグ:90:3 == 1
	PRINTFORMW %NAME:MASTER%はポケットの中の駒を確認した
	PRINTFORMW 必要な駒はナイトとルークを持っている……
	RETURN 0
ELSEIF ダンジョンフラグ:90:2 == 1 && ダンジョンフラグ:90:4 == 1
	PRINTFORMW %NAME:MASTER%はポケットの中の駒を確認した
	PRINTFORMW 必要な駒はナイトとビショップを持っている……
	RETURN 0
ELSEIF ダンジョンフラグ:90:3 == 1 && ダンジョンフラグ:90:4 == 1
	PRINTFORMW %NAME:MASTER%はポケットの中の駒を確認した
	PRINTFORMW 必要な駒はルークとビショップを持っている……
	RETURN 0
ELSEIF ダンジョンフラグ:90:2 == 1
	PRINTFORMW %NAME:MASTER%はポケットの中の駒を確認した
	PRINTFORMW 必要な駒はナイトだけを持っている……
	RETURN 0
ELSEIF ダンジョンフラグ:90:3 == 1
	PRINTFORMW %NAME:MASTER%はポケットの中の駒を確認した
	PRINTFORMW 必要な駒はルークだけを持っている……
	RETURN 0
ELSEIF ダンジョンフラグ:90:4 == 1
	PRINTFORMW %NAME:MASTER%はポケットの中の駒を確認した
	PRINTFORMW 必要な駒はビショップだけを持っている……
	RETURN 0

ELSE
	PRINTW Nothing seems to be here.
	RETURN 0
ENDIF

;=======================================================================
;タイル進入時のイベント
;=======================================================================
@EVENT_ENTER_DUNGEON_90,ARG
;ARG = 進入時の向き　0の場合ワープなど
;==========
;出口の処理
;==========
IF DA:(FLAG:現X):(FLAG:現Y) % 10 == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 3
	CALL TREASURE_90
	RETURN RESULT
;=======
;階段
;=======
ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 6
	CALL UPSTAIRS_90
	RETURN RESULT

ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 7
	CALL DOWNSTAIRS_90
	RETURN RESULT
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 9
	CALL DUNGEON_TERMINAL
	RETURN 1

;========
;ワープの処理
;========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 4
	SELECTCASE FLAG:現M
		CASE 0
			IF FLAG:現X == 15 && FLAG:現Y == 7
				FLAG:現M = 2
				FLAG:現X = 8
				FLAG:現Y = 16
			ENDIF
		CASE 1
			IF FLAG:現X == 15 && FLAG:現Y == 10
				FLAG:現M = 4
				FLAG:現X = 16
				FLAG:現Y = 16
			ELSEIF FLAG:現X == 1 && FLAG:現Y == 10
				FLAG:現M = 3
				FLAG:現X = 7
				FLAG:現Y = 6
			ENDIF
		CASE 2
			IF FLAG:現X == 8 && FLAG:現Y == 16
				FLAG:現M = 0
				FLAG:現X = 15
				FLAG:現Y = 7
			ENDIF
		CASE 3
			IF FLAG:現X == 7 && FLAG:現Y == 6
				FLAG:現M = 1
				FLAG:現X = 1
				FLAG:現Y = 10
			ELSEIF FLAG:現X == 7 && FLAG:現Y == 5
				FLAG:現X = 9
				FLAG:現Y = 6
			ELSEIF FLAG:現X == 9 && FLAG:現Y == 6
				FLAG:現X = 7
				FLAG:現Y = 5
			ELSEIF FLAG:現X == 6 && FLAG:現Y == 7
				FLAG:現X = 4
				FLAG:現Y = 13
			ELSEIF FLAG:現X == 4 && FLAG:現Y == 13
				FLAG:現X = 6
				FLAG:現Y = 7
			ELSEIF FLAG:現X == 5 && FLAG:現Y == 14
				FLAG:現X = 16
				FLAG:現Y = 13
			ELSEIF FLAG:現X == 16 && FLAG:現Y == 13
				FLAG:現X = 5
				FLAG:現Y = 14
			ELSEIF FLAG:現X == 3 && FLAG:現Y == 14
				FLAG:現X = 14
				FLAG:現Y = 1
			ELSEIF FLAG:現X == 14 && FLAG:現Y == 1
				FLAG:現X = 3
				FLAG:現Y = 14
			ELSEIF FLAG:現X == 14 && FLAG:現Y == 14
				FLAG:現X = 16
				FLAG:現Y = 4
			ELSEIF FLAG:現X == 16 && FLAG:現Y == 4
				FLAG:現X = 14
				FLAG:現Y = 14
			ELSEIF FLAG:現X == 8 && FLAG:現Y == 2
				FLAG:現X = 9
				FLAG:現Y = 12
			ELSEIF FLAG:現X == 9 && FLAG:現Y == 12
				FLAG:現X = 8
				FLAG:現Y = 2


			ENDIF
		CASE 4
			IF FLAG:現X == 16 && FLAG:現Y == 16
				FLAG:現M = 1
				FLAG:現X = 15
				FLAG:現Y = 10
			ENDIF
	ENDSELECT

	CALL MAKE_FLOOR, FLAG:現ダンジョン
	CALL AUTOMAP
	RETURN 1

;==========
;扉の処理
;==========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 2
	CALL DUNGEON_DOOR,ARG
	RETURN 1


;========================
;イベント
;========================
ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 4

	IF FLAG:現M == 2 && FLAG:現X == 9 && FLAG:現Y == 1
		IF ダンジョンフラグ:(FLAG:現ダンジョン):2 == 0
			PRINTFORML チェス盤の上に、ナイトの駒が一つ置かれている
			PRINTFORMW %NAME:MASTER%はナイトの駒をポケットに仕舞った……
			ダンジョンフラグ:(FLAG:現ダンジョン):2 = 1
		ELSE
			PRINTFORML チェス盤の上には何も無い
			PRINTFORMW ここに置いてあったナイトの駒は、既に手に入れている…………
		ENDIF
	ELSEIF FLAG:現M == 3 && FLAG:現X == 16 && FLAG:現Y == 1
		IF ダンジョンフラグ:(FLAG:現ダンジョン):3 == 0
			PRINTFORML チェス盤の上に、ルークの駒が一つ置かれている
			PRINTFORMW %NAME:MASTER%はルークの駒をポケットに仕舞った……
			ダンジョンフラグ:(FLAG:現ダンジョン):3 = 1
		ELSE
			PRINTFORML チェス盤の上には何も無い
			PRINTFORMW ここに置いてあったルークの駒は、既に手に入れている…………
		ENDIF
	ELSEIF FLAG:現M == 4 && FLAG:現X == 1 && FLAG:現Y == 1
		IF ダンジョンフラグ:(FLAG:現ダンジョン):4 == 0
			PRINTFORML チェス盤の上に、ビショップの駒が一つ置かれている
			PRINTFORMW %NAME:MASTER%はビショップの駒をポケットに仕舞った……
			ダンジョンフラグ:(FLAG:現ダンジョン):4 = 1
		ELSE
			PRINTFORML チェス盤の上には何も無い
			PRINTFORMW ここに置いてあったビショップの駒は、既に手に入れている…………
		ENDIF


