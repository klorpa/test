;=====================================================================
;エロ装備しているかどうか判断
;=====================================================================
@IS_EROEQUIP_F,ARG = -1
#FUNCTION
SIF ARG == -1
	ARG = PLAYER
SIF EQUIP:PLAYER:胴 == 3501 || EQUIP:PLAYER:胴 == 3532 || EQUIP:PLAYER:胴 == 3535 || EQUIP:PLAYER:胴 == 3545 || EQUIP:PLAYER:胴 == 3546
	RETURNF 1
RETURNF 0
