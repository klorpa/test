;===============================
;サマナーLvの最大値
;===============================
@GET_SUMMONER_LV(ARG = 0)
#FUNCTION
LOCAL:1 = 0
FOR LOCAL,1,7
	LOCALS = ポジション{LOCAL}
	IF FLAG:LOCALS > -1
		SELECTCASE GET_STATE(CFLAG:(FLAG:LOCALS):ステート)
			CASE "DYING","STONE","FLY","PALYZE","BIND","CHARM","SLEEP","PANIC"
				CONTINUE
			CASEELSE
				LOCAL:1 += MAX(TALENT:(FLAG:LOCALS):サマナー,LOCAL:1)
		ENDSELECT
	ENDIF
NEXT
RETURNF LOCAL:1