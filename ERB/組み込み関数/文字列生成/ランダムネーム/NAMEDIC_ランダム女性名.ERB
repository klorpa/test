;========================================================================
;適当な女性の名前を指定した数だけランダム生成してRESULTS配列に代入
;========================================================================
@NAMEDIC_RandomFemale,ARG = 1
#LOCALSIZE 1
#LOCALSSIZE 2
SIF ARG <= 0
	RETURN 
LOCALS = 西欧_東欧_イスラム_インド_モンゴル_意味不明
FOR LOCAL,1,ARG+1
	SELECTCASE AUTO_SPLIT(LOCALS,"_",RAND(0,6))
		CASE "西欧"
			CALL NAMEDIC_ランダムカタカナ,1,RAND(1,3)
			LOCALS:1 = ディア_リス_リーナ_レット_レーテ
		CASE "東欧"
			CALL NAMEDIC_ランダムカタカナ,1,RAND(1,2)
			LOCALS:1 = アンヌ_シア_ドーラ_リーテェ_リクス
		CASE "イスラム"
			CALL NAMEDIC_ランダムカタカナ,1,RAND(2,3)
			LOCALS:1 = イダ_ザード_ティマ_リーン
		CASE "インド"
			CALL NAMEDIC_ランダムカタカナ,1,RAND(1,3)
			LOCALS:1 = シャナ_ティ_ハール_ラータ
		CASE "モンゴル"
			CALL NAMEDIC_ランダムカタカナ,1,RAND(1,2)
			LOCALS:1 = アン_トリ_ラン_リク_リチュ_リン_ルク_ルクナ_ルチェ_ルン
		CASE "意味不明"
			CALL NAMEDIC_ランダムカタカナ,1,RAND(2,8)
			LOCALS:1 = 
	ENDSELECT
	RESULTS:LOCAL = %RESULTS%%RANDOM_SPLIT(LOCALS:1,"_",1)%
NEXT
ARRAYREMOVE RESULTS,0,1
